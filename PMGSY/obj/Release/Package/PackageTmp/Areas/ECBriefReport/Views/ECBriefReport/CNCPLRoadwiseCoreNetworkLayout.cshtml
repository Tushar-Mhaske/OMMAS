@model PMGSY.Areas.ECBriefReport.Models.FAPhaseProfileReportViewModel

@if (Layout == null)
{
    Layout = Request.IsAjaxRequest() ? null : "~/Areas/AccountReports/Views/Account/_Layout.cshtml";
}

<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
@*<script src="~/Areas/ECBriefReport/Scripts/FAPhaseProfileLayout.js"></script>*@


<script src="~/Areas/ECBriefReport/Scripts/CNCPLRoadwise.js"></script>

<div id="dvhdFAPhaseProfileDetails" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;CNCPL - Roadwise Core Network New Connectivity Priority List</span>

    <span id="spCollapseIconCN" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>
<div id="dvFAPhaseProfileParameter" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmFAPhaseProfileLayout">
        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.StateName)
     
        @Html.HiddenFor(model => model.DistName)
        @Html.HiddenFor(model => model.BlockName)
        
   
        <table class="rowstyle">
            <tr>
                <td style="width: 2%;"></td>
                <td style="width: 5%;">
                    @Html.LabelFor(model => model.StateCode)
                </td>
                <td style="width: 15%;">
                    @Html.DropDownListFor(model => model.StateCode, new SelectList(Model.StateList, "Value", "Text"), new { @id = "ddlFAState", style = "width:170px;", title = "State" })
                    <br />
                    @Html.ValidationMessageFor(m => m.StateCode)
                </td>
                <td style="width: 7%;">
                    @Html.LabelFor(model => model.DistrictCode)
                </td>
                <td style="width: 14%;">
                    @Html.DropDownListFor(model => model.DistrictCode, new SelectList(Model.DistrictList, "Value", "Text"), new { @id = "ddlFADistrict", style = "width:170px;" })
                    <br />
                    @Html.ValidationMessageFor(m => m.DistrictCode)
                </td>
                <td style="width: 7%;">
                    @Html.LabelFor(model => model.BlockCode)
                </td>
                <td style="width: 14%;">
                    @Html.DropDownListFor(model => model.BlockCode, new SelectList(Model.BlockList, "Value", "Text"), new { @id = "ddlFABlock", style = "width:170px;" })
                    <br />
                    @Html.ValidationMessageFor(m => m.BlockCode)
                </td>
            </tr>
            <tr><td style="text-align: left;">
                    <input type="button" id="btnViewFA" value="View" class="button" />
                </td>
            </tr>
           
        </table>
    </form>
</div>

<div id="divFAPhaseProfileReport" style="width:100%;"></div>
