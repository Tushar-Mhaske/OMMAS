@using MvcReportViewer

@model PMGSY.Areas.OtherReports.Models.CorrigendumReportModel

  @Html.MvcReportViewer("/PMGSYCitizen/GEPNIC_CORRIGENDUM_DETAILSReport",
     new
     {
        STATE = Model.StateCode,
        DISTRICT = Model.DistrictCode,
        YEAR = Model.Year,
        PMGSY = Model.SchemeCode,
        StateName = Model.StateName,
        DistrictName = Model.DistName,
        YearLabel = Model.Year,
     },
       new { Height = 600, Width = "100%", style = "border:none;" })

