@model PMGSY.Areas.QMSSRSReports.Models.MaintenanceInspModel



<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script src="~/Areas/QMSSRSReports/Scripts/MaintenanceInspections.js"></script>
<div id="dvSearchParameter" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
@*<div id="loadFilters">*@

    @{
        var TypeOfMonitor = new List<SelectListItem>{
                                    new SelectListItem { Text = "All", Value = "A",Selected = true },
                                    new SelectListItem { Text = "NQM", Value = "I" }, 
                                    new SelectListItem { Value = "S", Text = "SQM" }
                                 };

        var GradeList = new List<SelectListItem>{
                                    new SelectListItem { Text = "All", Value = "0",Selected = true },
                                    new SelectListItem { Text = "Satisfactory", Value = "1" },
                                    new SelectListItem { Text = "Satisfactory Required Improvement", Value = "2" },  
                                    new SelectListItem { Text = "UnSatisfactory", Value = "3" }, 
                                    new SelectListItem { Text = "NA", Value = "4" }, 
                                 };                       
                                 
                                 
    }

    <form id="frmQMMaintenanceInspDetails">
        @Html.AntiForgeryToken()

        @Html.ValidationSummary(true)
        @Html.Hidden("hdnRoleCode",PMGSY.Extensions.PMGSYSession.Current.RoleCode.ToString())
       <table style="width: 100%;" class="rowstyle">
            <tr>
           
                <td style="text-align: left; width: 5%;">
                 @Html.LabelFor(model => model.StateCode)
                </td>
                <td style="width: 15%;">
                    
                        @Html.DropDownListFor(model => model.StateCode, new SelectList(Model.StateList, "Value", "Text"), new { @title ="State", @tabindex = 1, @id = "ddlStateCompleted", @style = "width:150px" })
                        @Html.ValidationMessageFor(m => m.StateCode)                    
                </td>
                 <td style="text-align: left; width: 5%;">
                 @Html.LabelFor(model => model.DistrictCode)
                </td>
                <td style="width: 15%;">
                    
                        @Html.DropDownListFor(model => model.DistrictCode, new SelectList(Model.DistrictList, "Value", "Text"), new { @title ="District", @tabindex = 2, @id = "ddlDistMaintenanceInsp", @style = "width:155px" })
                        @Html.ValidationMessageFor(m => m.DistrictCode)                    
                </td>
                 <td style="text-align: left; width: 5%;">
                 @Html.LabelFor(model => model.MonitorType)
                </td>
                <td style="width: 15%;">
                    
                        @Html.DropDownListFor(model => model.MonitorType,  new SelectList(TypeOfMonitor, "Value", "Text") , new { @title = "MonitorType", @tabindex = 3, @id = "ddlMonitorType", @style = "width:155px" })
                        @Html.ValidationMessageFor(m => m.MonitorType)                    
                </td>
                 <td style="text-align: left; width: 5%;">
                 @Html.LabelFor(model => model.Grade)
                </td>
                <td style="width: 15%;">
                    
                        @Html.DropDownListFor(model => model.Grade, new SelectList(GradeList, "Value", "Text"), new { @title ="Grade", @tabindex = 4, @id = "ddlGrade", @style = "width:150px" })
                        @Html.ValidationMessageFor(m => m.Grade)                    
                </td>
                 @*@if (PMGSY.Extensions.PMGSYSession.Current.RoleCode != 8)
                 { 
                    <td>
                        <input type="button" id="btnCompletedView" value="View"  Class="jqueryButton"  Title="View Details"tabindex="4" />
                    </td>
                 }
                 else
                 {
                     <td></td>
                 }*@



                
            </tr>
              <tr>
                
                 <td style="text-align: left; width: 5%;">
                 @Html.LabelFor(model => model.FromMonth)
                </td>
                <td style="width: 15%;">
                    
                        @Html.DropDownListFor(model => model.FromMonth, new SelectList(Model.MonthList, "Value", "Text"), new { @title ="From Month", @tabindex = 5, @id = "ddlFromMonthInsp", @style = "width:150px" })
                        @Html.ValidationMessageFor(m => m.FromMonth)                    
                </td>
                   <td style="text-align: left; width: 5%;">
                 @Html.LabelFor(model => model.FromYear)
                </td>
                <td style="width: 15%;">
                    
                        @Html.DropDownListFor(model => model.FromYear, new SelectList(Model.YearList, "Value", "Text"), new { @title ="From Year", @tabindex = 6, @id = "ddlFromYearInsp", @style = "width:155px" })
                        @Html.ValidationMessageFor(m => m.FromYear)                    
                </td>
                     <td style="text-align: left; width: 5%;">
                 @Html.LabelFor(model => model.ToMonth)
                </td>
                <td style="width: 15%;">
                    
                        @Html.DropDownListFor(model => model.ToMonth, new SelectList(Model.MonthList, "Value", "Text"), new { @title ="To Month", @tabindex = 5, @id = "ddlToMonthInsp", @style = "width:155px" })
                        @Html.ValidationMessageFor(m => m.ToMonth)                    
                </td>
                   <td style="text-align: left; width: 5%;">
                 @Html.LabelFor(model => model.ToYear)
                </td>
                <td style="width: 15%;">
                    
                        @Html.DropDownListFor(model => model.ToYear, new SelectList(Model.YearList, "Value", "Text"), new { @title ="To Year", @tabindex = 6, @id = "ddlToYearInsp", @style = "width:150px" })
                        @Html.ValidationMessageFor(m => m.ToYear)                    
                </td>
                  </tr>
           <tr>

               <td></td>
               <td></td>
               <td></td>
               <td style="width: 15%; float: right;">
                   <input type="button" value="View" title="View Details" id="btn_View_Report" class="jqueryButton ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only" />
               </td>
               <td></td>
               <td></td>
               <td></td>
               <td></td>
           </tr>
        </table>
    </form>
</div>

<div id="loadReport2" class="divLoadReport"></div>