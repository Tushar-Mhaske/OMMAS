@{
    ViewBag.Title = "CUCPLDetails";
}


@model PMGSY.Models.CoreNetworkReports.CoreNetworkReportViewModel

<script type="text/javascript" src="@Url.Content("~/Scripts/jquery.ui.dialog.js")"> </script>
<script src="~/Scripts/CoreNetworkReports/CUCPLDetails.js" type="text/javascript"></script>
<script src="~/Scripts/custom/Generic.js"></script>
<form id="frmCN1StateLevelReport">
    @Html.Hidden("hdnLevelId", PMGSY.Extensions.PMGSYSession.Current.LevelId.ToString())

    @if (PMGSY.Extensions.PMGSYSession.Current.StateCode != 0)
    {           
        @Html.Hidden("STATE_NAME", PMGSY.Extensions.PMGSYSession.Current.StateName.ToString())
    }

    @if (PMGSY.Extensions.PMGSYSession.Current.DistrictCode != 0)
    {
        @Html.Hidden("DISTRICT_NAME", PMGSY.Extensions.PMGSYSession.Current.DistrictName.ToString())  
       
             
    }

    @Html.HiddenFor(model => model.BLOCK_NAME)
    <div style="width: 100%">
        <table class="ui-state-default ui-corner-all" style="width: 100%;">
            <tr>
                <td style="width: 2%;"></td>
                <td style="text-align: left; width: 1%;">
                    @Html.Label("Route") &nbsp;
                </td>
                <td style="width: 2%;">
                    @Html.DropDownList("Route_CUCPLDetails", ViewData["Route"] as IEnumerable<SelectListItem>,new { @title = "Route" ,@tabindex = 1 })
                </td>
                <td style="width: 20%;">
                    <input type="button" value="View" class="jqueryButton" id="btCUCPLDetails" title="View Details" tabindex="2" />
                </td>
            </tr>
        </table>
    </div>
    <br />
    @if (PMGSY.Extensions.PMGSYSession.Current.LevelId == 6)     //Mord
    {
       
        
        <div id="CUCPLStateReport">
            <table id="CUCPLStateReportTable"></table>
        </div>
        <div id="CUCPLStateReportPager"></div>
        <br />

        <div id="CUCPLDistrictReport">
            <table id="CUCPLDistrictReportTable"></table>
        </div>
        <div id="CUCPLDistrictReportPager"></div>

        <br />
        <div id="CUCPLBlockReport">
            <table id="CUCPLBlockReportTable"></table>
        </div>
        <div id="CUCPLBlockReportPager"></div>

        <br />
        <div id="CUCPLFinalReport">
            <table id="CUCPLFinalReportTable"></table>
        </div>
        <div id="CUCPLFinalReportPager"></div>
               
    }

    else if (PMGSY.Extensions.PMGSYSession.Current.LevelId == 4)     //State
    {
        @Html.HiddenFor(model => model.MAST_STATE_CODE)
         <div id="CUCPLDistrictReport">
            <table id="CUCPLDistrictReportTable"></table>
        </div>
        <div id="CUCPLDistrictReportPager"></div>

        <br />
        <div id="CUCPLBlockReport">
            <table id="CUCPLBlockReportTable"></table>
        </div>
        <div id="CUCPLBlockReportPager"></div>

        <br />
        <div id="CUCPLFinalReport">
            <table id="CUCPLFinalReportTable"></table>
        </div>
        <div id="CUCPLFinalReportPager"></div>

    }
    else if (PMGSY.Extensions.PMGSYSession.Current.LevelId == 5)     //District
    {
        @Html.HiddenFor(model => model.MAST_STATE_CODE)
        @Html.HiddenFor(model => model.MAST_DISTRICT_CODE)
         <div id="CUCPLBlockReport">
            <table id="CUCPLBlockReportTable"></table>
        </div>
        <div id="CUCPLBlockReportPager"></div>

        <br />
        <div id="CUCPLFinalReport">
            <table id="CUCPLFinalReportTable"></table>
        </div>
        <div id="CUCPLFinalReportPager"></div>
    }
</form>







