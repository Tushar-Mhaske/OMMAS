@using MvcReportViewer
@model PMGSY.Areas.AccountReports.Models.SheduleReportModel

<div id="dvFundReceivedReport" style="width: 100%; height: 600px; overflow-x: hidden; overflow-y: hidden">
    @Html.MvcReportViewer("/PMGSYCitizen/FundReceived",
         new
         {

             PINTRptID = Model.ReportID,
             PINTAdminNDCode = Model.Agency,
             PINTAccMonth = Model.Month,
             PINTAccYear = Model.Year,
             PINTisAllPIU = Model.Piu,
             PCHARFUNDTYPE = Model.FundType,
             SrrdaDpiu = Model.SRRDA_DPIU,
             SrrdaNdCode = Model.SrrdaNdCode
         },
         new { Height = 600, Width = 1220, style = "border:none;" })
</div>
