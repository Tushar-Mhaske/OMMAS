@model PMGSY.Areas.ProgressReport.Models.ProgressViewModel

@{
    ViewBag.Title = "AspirationalDistAchievement";
}

<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script src="~/Areas/ProgressReport/Scripts/AspirationalDistAchievement.js"></script>

<div id="dvhdAspirationalDistrictAchievement" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;Aspirational District Achievement</span>
</div>
<div id="loadFilters" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmAspirationalDistrictAchievement">
        @Html.ValidationSummary(true)

        <table style="width: 100%;" class="rowstyle">
             <tr>
            <td style="width: 5%; vertical-align: top"></td>
                <td style="text-align: left; width: 5%; vertical-align: top"></td>
           
                 <td style="width: 5%; vertical-align: top;text-align:center;">
                    @Html.LabelFor(model => model.Year)
                </td>
                <td style="width: 5%; vertical-align: top">
                  @Html.DropDownListFor(model => model.Year, new SelectList(Model.YearList, "Value", "Text"), new { @title = "Year", @id = "ddlYear", @style = "width:150px" })
                    <br />
                    @Html.ValidationMessageFor(m => m.Year)
                </td>
              <td style="width: 5%; vertical-align: top; text-align:left">
                    <input type="button" class="jqueryButton"  value="View" id="btnViewReport" title="View" />
                </td>
                <td style="width: 5%; vertical-align: top"></td>
            </tr>
        </table>
    </form>
</div>
<div id="loadReport" class="divLoadReport"></div>



