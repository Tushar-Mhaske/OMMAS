@{
    ViewBag.Title = "SearchMLAMember";
}



<script src="~/Scripts/custom/Generic.js" type="text/javascript"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery.ui.autocomplete.js")"> </script>
<script src="~/Scripts/Master/SearchMLAMember.js" type="text/javascript"></script>



<form id="frmSearchMember">
    <div id="desigList" style="width: 100%">

        <div id="dvhdSearch" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
            @*margin-left:10px;margin-right:10px;margin-top:10px;*@
            <span class="ui-jqgrid-title" style="float: left">&nbsp;Search</span>

            <span id="spCollapseIconS" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
        </div>
        <div id="dvSearchParameter" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">@*margin-left: 10px; margin-right: 10px;*@

            <table style="width: 100%" class="rowstyle">
                <tr>
                    <td style="text-align: left; width: 10%;"></td>
                    <td style="text-align: left; width: 10%;">@Html.Label("State")</td>
                    <td style="text-align: left; width: 20%">@Html.DropDownList("State", null, new { @title = "State" })</td>
                    <td style="text-align: left; width: 10%;">@Html.Label("Vidhan Sabha Term")</td>
                    <td style="text-align: left; width: 20%">@Html.DropDownList("Term", null, new { @title = "Term" })</td>
                </tr>
                <tr>
                    <td style="text-align: left; width: 10%;"></td>
                    <td style="text-align: left; width: 15%;">@Html.Label("MLA Constituency")</td>
                    <td style="text-align: left; width: 20%">@Html.DropDownList("Constituency", null, new { @title = "MLA Constituency" })</td>
                    <td style="text-align: left; width: 12%;">@Html.Label("Member Name")</td>
                    <td style="text-align: left; width: 20%">@Html.TextBox("txtMemberName", "", new { @title = "Member Name", @id = "txtMemberName", @maxlength = 50, @placeholder = "Enter Member Name...", @autocomplete = "on" })</td>

                </tr>
                <tr>
                    <td colspan="5">
                    <center>
                    <input type="button" id="btnMemberSearch" name="btnSearch" value="Search" class="jqueryButton" title="Search MLA Member" tabindex="4" />
                   </center>
                    </td>

                </tr>
            </table>

        </div>
    </div>
</form>
