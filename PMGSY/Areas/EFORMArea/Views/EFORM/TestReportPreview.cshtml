@{
    Layout = null;
}
@model PMGSY.Areas.EFORMArea.TestReportPreviewModel.EFORM_TR_PREVIEW_MODEL
<!DOCTYPE html>

<html>
<head>
    <title>TestReportPreview</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <script type="text/javascript" src="~/Scripts/jquery-1.9.1.min.js"></script>
    <link rel="stylesheet" href="~/Areas/EFORMArea/Content/bootstrap.min.css" />
    <script src="~/Areas/EFORMArea/Scripts/bootstrap.min.js"></script>

    <style>

        /*=========== Common Stylesheet ============*/

        input[type="text"] {
            font-size: 22px;
        }

        /* inputbox width for values like codes,length,dates, */
        .inputRemark {
            width: 100%;
            height: 100%;
        }

        .inputTHWidth {
            width: 50px;
            min-height: 32px;
        }

        .inputCodeWidth {
            width: 100px;
            min-height: 32px;
        }

        .inputShortWidth {
            width: 150px;
            min-height: 32px;
        }

        .inputExtrShortWidth {
            width: 60px;
            min-height: 32px;
        }

        .inputMediumWidth {
            width: 250px;
            min-height: 32px;
        }

        .inputMidlLargeWidth {
            width: 400px;
            min-height: 32px;
        }

        .inputLargeWidth {
            width: 700px;
            min-height: 32px;
        }

        .SectionUnderline {
            border: 0.5px solid gray;
        }

        .PageBreakLine {
            border: 3px solid gray;
            margin-top: 35px;
            margin-bottom: 35px;
        }

        .table-borderless td {
            border: none;
        }

        .table-bordered thead th {
            vertical-align: top;
            text-align: center;
            background-color: #8c8c8c24;
            border: 3px solid black;
        }

        .table-hover tbody tr:hover td {
            background-color: #d5bcbc24;
        }

        /*=========== QM Page CSS ===========*/

        .fontTestReportPDF {
            font-size: 15px;
            /*font-family: Calibri;*/
        }
        /* 2. QUALITY ARRANGEMENTS-OBSERVATIONS Tables */
        .tab1_2Pt td {
            width: 80%;
        }

        /* 3. ATTENTION TO QUALITY */
        .tab1_3Pt td {
            width: 80%;
        }

        .Applicablechecks {
            font-size: large;
            font-style: normal;
            text-align: center;
            padding: 5px;
        }

        input.largerCheckbox {
            width: 30px;
            height: 30px;
        }

        input[type="checkbox"] {
            width: 25px;
            height: 25px;
        }

        .customBorder thead th {
            border: 3px solid black;
        }

        .customBorder tbody td {
            border: 3px solid black;
        }

        .table tbody td {
            border: 3px solid black;
            height: 10px;
        }

        .table thead th {
            border: 3px solid black;
            height: 10px;
        }

        .table thead td {
            border: 3px solid black;
            height: 10px;
        }

        tr {
            line-height: 20px;
        }


        /* ------------ test reports new css ------------*/
        .topDetailsAlign {
            min-width: 150px;
            max-width: 150px;
        }

        .srnowidth {
            min-width: 26px;
            max-width: 60px;
        }

        .srnowidthTypeA {
            min-width: 26px;
            max-width: 60px;
        }

        .srnowidthTypeB {
            min-width: 22px;
            max-width: 22px;
        }

        .srnowidthTypeC {
            min-width: 26px;
            max-width: 26px;
        }



        .tbldescWidth {
            min-width: 350px;
            max-width: 350px;
        }

        .subtblDescWidth {
            min-width: 250px;
            max-width: 250px;
        }

        .subtblDescWidthTypeA {
            min-width: 250px;
            max-width: 1000px;
        }
        /* to apply purple-white color back for all tables */
        .table-light {
            --bs-table-bg: #f5f2fb8c;
        }

        .typeADetailsColumnWidth {
            width: 160px;
        }

        .BaseCoursePan {
            text-align: center
        }
    </style>
</head>
<body style="font-size:18.9px;">
    <div style="text-align:right;margin-right:150px;">
        <button> <a href="javascript:window.print()"> Download as PDF or Print </a></button>
    </div>

    <div style="text-align: right; margin-right: 150px; font-size: 17px; color: red; font-style: italic "><label>(Note: Select paper size: A4 & scale: 45%)</label>   </div>

    @{ bool isPage2Avail = false;}
    <div class="container-fluid" style="background-color:lightgray">
        <div class="container-fluid" style="width: 92%; background-color: #fff; margin-top: 30px; margin-bottom: 30px; margin-left:auto; margin-right:auto">

            <div class="border border-dark"><label style="text-align:left;float:left;font-weight:bold"> FORM ID <input type="text" value="@Model.PREFILLED_DETAILS.EFORM_ID" class="inputShortWidth" disabled></label> <h3 class="text-center"> Test Datasheets for QM E-form  <span style="float:right;color:black">Template Version <input type="text" value="@Model.PREFILLED_DETAILS.TEMPLATE_VERSION" class="inputExtrShortWidth" data-toggle="tooltip" data-placement="bottom" title="Template Version" disabled></span> </h3></div>
            @*Page :1*@
            @if (Model.ITEM_APPLICABLE_DETAILS_VIEW.ITEM_1.Equals("Y") || Model.ITEM_APPLICABLE_DETAILS_VIEW.ITEM_2.Equals("Y"))
            {
                <div class="border border-dark text-center mt-2 mb-2 bg-dark text-white"> <h4> EARTHWORK/SUBGRADE </h4> </div>
            }

            @if (Model.ITEM_APPLICABLE_DETAILS_VIEW.ITEM_1.Equals("Y"))
            {
                <div id="EarthworkSubgrade1.1" class="m-x-1 my-5 p-2 border border-dark border-rounded">

                    <table class="table table-light table-bordered table-hover">
                        <tbody>
                            <tr>

                                <th><span class="Applicablechecks"> <b style="font-size:x-large"> &#x2713;</b> </span>APPLICABLE</th>
                                <th> 1 </th>
                                <th> Field Density of </th>
                                @if (Model.SUBITEM_OPTIONS_SELECTED_DETAIL_VIEW_List.Where(s => s.OPTION_ID == 1).Any())
                                {
                                    <th class="border border-end-0" colspan="1"> Earthwork <b style="font-size:x-large"> &#x2713;</b>  </th>
                                }
                                else
                                {
                                    <th class="border border-end-0" colspan="1"> Earthwork <input type="checkbox" disabled /> </th>
                                }
                                @if (Model.SUBITEM_OPTIONS_SELECTED_DETAIL_VIEW_List.Where(s => s.OPTION_ID == 2).Any())
                                {
                                    <th class="border border-start-0 border-end-0" colspan="2"> Subgrade <b style="font-size:x-large"> &#x2713;</b>  </th>
                                }
                                else
                                {
                                    <th class="border border-start-0 border-end-0" colspan="2"> Subgrade <input type="checkbox" disabled /> </th>
                                }

                                @if (Model.TYPEA_SUMMARY_PAGE1_SRM_1_VIEW.IS_HEAVY_COMPACTION.Equals("Y"))
                                {
                                    <th class="border border-start-0" colspan="2"> Heavy Compaction <b style="font-size:x-large"> &#x2713;</b> </th>
                                }
                                else
                                {
                                    <th class="border border-start-0" colspan="2"> Heavy Compaction <input type="checkbox" disabled /> </th>
                                }

                            </tr>
                            <tr>
                                <td colspan="2"></td>
                                <td colspan="6" class="text-center"> (Sand Replacement Method) </td>
                            </tr>
                            <tr>
                                <td colspan="2"> Road Name / Package No </td>
                                <td colspan="6">
                                    <div class="grow-wrap p-2 border border-grey text-wrap fontPIUPdf" style="border: 1px solid lightgray; color: #666;  min-height: 30px; max-width: 100%;" data-toggle="tooltip" data-placement="bottom" title="Road Name/Package Number">
                                        <p class="fontPIUPdf px-2">
                                            <b> @Model.PREFILLED_DETAILS.ROAD_NAME</b>
                                        </p>
                                    </div>

                                </td>
                            </tr>
                            <tr>
                                <td colspan="3"> Maximum Dy Density (MDD) as per lab record (gm/cc) </td>
                                <td>1.CH &nbsp;<u>  @Model.TYPEA_SUMMARY_PAGE1_SRM_1_VIEW.MDD_CH1 </u></td>
                                <td>2.CH &nbsp;<u>  @Model.TYPEA_SUMMARY_PAGE1_SRM_1_VIEW.MDD_CH2</u> </td>
                                <td>3.CH &nbsp;<u>  @Model.TYPEA_SUMMARY_PAGE1_SRM_1_VIEW.MDD_CH3 </u></td>
                                <td> Date of Testing </td>
                                <td colspan="1">  @Model.TYPEA_SUMMARY_PAGE1_SRM_1_VIEW.TESTING_DATE</td>
                            </tr>
                        </tbody>
                    </table>

                    <br />
                    @* 1. a *@
                    <div class="table-responsive">
                        <table class="table table-light table-bordered table-hover customBorder">
                            <thead>
                                <tr>
                                    <th class="srnowidthTypeB"> a) </th>
                                    <th class="tbldescWidth"> Calibration </th>
                                    <th class="typeADetailsColumnWidth">1.CH &nbsp;<u>@Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 1).Select(x => x.CH1_VALUE).FirstOrDefault()</u></th>
                                    <th class="typeADetailsColumnWidth">2.CH &nbsp;<u>@Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 1).Select(x => x.CH2_VALUE).FirstOrDefault()</u></th>
                                    <th class="typeADetailsColumnWidth">3.CH &nbsp;<u>@Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 1).Select(x => x.CH3_VALUE).FirstOrDefault()</u></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="srnowidthTypeB"> (i) </td>
                                    <td class="tbldescWidth"> Mean weight of sand in cone (of pouring cylinder) (W2) in gm. </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 2).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 2).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 2).Select(x => x.CH3_VALUE).FirstOrDefault() </td>

                                </tr>
                                <tr>
                                    <td class="srnowidthTypeB"> (ii) </td>
                                    <td class="tbldescWidth"> Volume of calibrating cylinder (V) in cm3. </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 3).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 3).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 3).Select(x => x.CH3_VALUE).FirstOrDefault() </td>

                                </tr>
                                <tr>
                                    <td class="srnowidthTypeB"> (iii) </td>
                                    <td class="tbldescWidth"> Weight of sand (+ cylinder) before pouring into calibrating container (W1) in gm </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 4).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 4).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 4).Select(x => x.CH3_VALUE).FirstOrDefault() </td>

                                </tr>
                                <tr>
                                    <td class="srnowidthTypeB"> (iv) </td>
                                    <td class="tbldescWidth"> Mean weight of sand (+cylinder) after pouring into calibrating container (W3) in gm. </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 5).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 5).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 5).Select(x => x.CH3_VALUE).FirstOrDefault() </td>

                                </tr>
                                <tr>
                                    <td class="srnowidthTypeB"> (v) </td>
                                    <td class="tbldescWidth"> Weight of sand to fill calibrating cylinder. (Wa = W1 - W2 - W3) in gm </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 6).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 6).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 6).Select(x => x.CH3_VALUE).FirstOrDefault() </td>

                                </tr>
                                <tr>
                                    <td class="srnowidthTypeB"> (vi) </td>
                                    <td class="tbldescWidth"> Bulk density of sand Ys = (Wa /V) gm/cm3 </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 7).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 7).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 7).Select(x => x.CH3_VALUE).FirstOrDefault() </td>

                                </tr>
                            </tbody>
                        </table>
                    </div>

                    @* 1. b *@
                    <div class="table-responsive">
                        <table class="table table-light table-bordered table-hover customBorder">
                            <thead>
                                <tr>
                                    <th class="srnowidthTypeA"> b) </th>
                                    <th colspan="3" class="subtblDescWidth"> Determination of soil density </th>
                                    <th class="typeADetailsColumnWidth"> </th>
                                    <th class="typeADetailsColumnWidth">  </th>
                                    <th class="typeADetailsColumnWidth">   </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="srnowidthTypeA"> (i) </td>
                                    <td colspan="3" class="subtblDescWidth"> Determination number </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 9).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 9).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 9).Select(x => x.CH3_VALUE).FirstOrDefault() </td>


                                </tr>
                                <tr>
                                    <td class="srnowidthTypeA"> (ii) </td>
                                    <td colspan="3" class="subtblDescWidth"> Weight of wet soil from hole (Ww) in gm </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 10).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 10).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 10).Select(x => x.CH3_VALUE).FirstOrDefault() </td>


                                </tr>
                                <tr>
                                    <td class="srnowidthTypeA"> (iii) </td>
                                    <td colspan="3" class="subtblDescWidth"> Weight of sand (+ cylinder) before pouring into hole (W1) in gm. </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 639).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 639).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 639).Select(x => x.CH3_VALUE).FirstOrDefault() </td>


                                </tr>
                                <tr>
                                    <td class="srnowidthTypeA"> (iv) </td>
                                    <td colspan="3" class="subtblDescWidth"> Weight of sand (+ cylinder) after pouring into hole and cone (W4) in gm </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 11).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 11).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 11).Select(x => x.CH3_VALUE).FirstOrDefault() </td>


                                </tr>
                                <tr>
                                    <td class="srnowidthTypeA"> (v) </td>
                                    <td colspan="3" class="subtblDescWidth"> Weight of sand in hole, in gm Wb = (W1 - W4- W2) </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 12).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 12).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 12).Select(x => x.CH3_VALUE).FirstOrDefault() </td>


                                </tr>
                                <tr>
                                    <td class="srnowidthTypeA"> (vi) </td>
                                    <td colspan="3" class="subtblDescWidth"> Bulk density Yb = (Ww /Wb) x Ys gm/cm3 </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 13).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 13).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 13).Select(x => x.CH3_VALUE).FirstOrDefault() </td>


                                </tr>
                                <tr>
                                    <td rowspan="1" class="srnowidthTypeA"> (vii) </td>
                                    <td rowspan="1" colspan="1" class="subtblDescWidth"> Moisture Content </td>
                                    @if (@Model.TYPEA_SUMMARY_PAGE1_SRM_1_VIEW.MOISTURE_CONTENT_METHOD.Contains("RMM"))
                                    {
                                        <td colspan="1" rowspan="1"> By RMM <b style="font-size:x-large"> &#x2713;</b></td>
                                        <td colspan="1" rowspan="1"> By Sand Bath Method <input type="checkbox" disabled /></td>
                                    }
                                    else if (@Model.TYPEA_SUMMARY_PAGE1_SRM_1_VIEW.MOISTURE_CONTENT_METHOD.Contains("SBM"))
                                    {
                                        <td colspan="1" rowspan="1"> By RMM <input type="checkbox" disabled /></td>
                                        <td colspan="1" rowspan="1"> By Sand Bath Method <b style="font-size:x-large"> &#x2713;</b></td>
                                    }
                                    else
                                    {
                                        <td colspan="1" rowspan="1"> By RMM <input type="checkbox" disabled /></td>
                                        <td colspan="1" rowspan="1"> By Sand Bath Method <input type="checkbox" disabled /></td>
                                    }

                                    <td></td>
                                    <td></td>
                                    <td></td>
                                </tr>
                                @* ------- sub row ------- *@
                                <tr>
                                    <td rowspan="3"></td>
                                    <td rowspan="3"></td>
                                    <td rowspan="3" class="srnowidthTypeA"> m = % water <br /> content of <br /> the wet mass </td>
                                    <td> Weight of wet soil (g) W1 </td>
                                    @if (@Model.TYPEA_SUMMARY_PAGE1_SRM_1_VIEW.MOISTURE_CONTENT_METHOD.Contains("RMM"))
                                    {


                                        <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 14).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                        <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 14).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                        <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 14).Select(x => x.CH3_VALUE).FirstOrDefault() </td>


                                    }
                                    else if (@Model.TYPEA_SUMMARY_PAGE1_SRM_1_VIEW.MOISTURE_CONTENT_METHOD.Contains("SBM"))
                                    {
                                        <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 15).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                        <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 15).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                        <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 15).Select(x => x.CH3_VALUE).FirstOrDefault() </td>

                                    }
                                    else
                                    {
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                    }
                                </tr>
                                <tr>

                                    <td> Weight of dry soil (g) W2 </td>

                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 16).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 16).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 16).Select(x => x.CH3_VALUE).FirstOrDefault() </td>


                                </tr>
                                <tr>
                                    <td> Loss of water (g) = W1-W2 </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 17).Select(x => x.CH1_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 17).Select(x => x.CH2_VALUE).FirstOrDefault() </td>
                                    <td> @Model.TYPEA_DETAIL_PAGE1_SRM_1_VIEW_List.Where(s => s.DETAIL_ITEM_ID == 17).Select(x => x.CH3_VALUE).FirstOrDefault() </td>

                                </tr>
                                @* ------- sub row ------- *@

                                <tr>
                                    <td class="srnowidthTypeA"> (viii) </td>
                                    <td colspan="1" class="subtblDescWidth"> Percentage of Moisture Content % (w) </td>
