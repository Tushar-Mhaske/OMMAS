<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/MasterDataEntry/Master_MappedMLAConstituencyBlocks.js")"> </script>

<div id="dvhdSearch_Mapped" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; margin-left: 0px; margin-right: 0px; margin-top: 10px; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;Mapped Blocks </span>

    <span id="spCollapseIconS_Mapped" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>

<div id="dvSearchParameter_Mapped" class="ui-corner-bottom" style="margin-left: 0px; margin-right: 0px; display: block; border: 1px solid #B5B8C8;">

    <form id="frmSearchMappedMLAConstituencyBlock">

        <table style="width: 100%;" class="rowstyle">

            <tr>               
                <td style="width: 25%;">
                    @Html.Label("State Name")

                </td>
                <td style="width: 25%;">
                    @if (!(string.IsNullOrEmpty(ViewBag.StateName)))
                    {
                        <b>@Html.Label("StateName", ViewBag.StateName as String)   </b>
                    }
                    else
                    {
                        <b>@Html.Label("StateName", "NA")   </b>
                    }

                </td>

                <td style="width: 25%;">
                    @Html.Label("MLA Constituency Name")

                </td>
                <td style="width: 25%;">
                    @if (!(string.IsNullOrEmpty(ViewBag.MLAConstituencyName)))
                    {
@* <blink>   </blink> *@
                        <b>@Html.Label("MLAConstituencyName", ViewBag.MLAConstituencyName as String) </b>
                    }
                    else
                    {
                        <b>@Html.Label("MLAConstituencyName", "NA")   </b>
                    }

                </td>

            </tr>

            <tr>
                <td colspan="4">
                    @if (!(string.IsNullOrEmpty(ViewBag.EncryptedMLAConstituencyCode_Mapped)))
                    {
                        @Html.Hidden("EncryptedMLAConstituencyCode_Mapped", ViewBag.EncryptedMLAConstituencyCode_Mapped as String)     
                    }


                </td>

            </tr>

        </table>

    </form>




    <br />

    @* <center>*@
    <div style="width: 95%;">
        <table id="tbMappedMLAConstituencyBlockList">
        </table>

        <div id="dvMappedMLAConstituencyBlockListPager"></div>
    </div>
    @*</center>*@


    <br />
    <table style="width: 100%;" class="rowstyle">
        <tr>
        </tr>
        <tr>
            <td style="text-align: center">
                <input type="button" id="btnMappedCancel" name="btnMapCancel" value="Back" class="jqueryButton" title="Back" tabindex="300" />
            </td>
        </tr>
        <tr></tr>
    </table>

</div>
