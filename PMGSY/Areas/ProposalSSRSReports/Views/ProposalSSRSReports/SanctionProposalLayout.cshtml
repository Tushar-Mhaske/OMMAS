@model PMGSY.Areas.ProposalSSRSReports.Models.ProposalListViewModel

@if (Layout == null)
{
    Layout = Request.IsAjaxRequest() ? null : "~/Areas/AccountReports/Views/Account/_Layout.cshtml";
}

<script type="text/javascript" src="@Url.Content("~/Scripts/jquery.ui.dialog.js")"> </script>

<script src="~/Areas/ProposalSSRSReports/Scripts/SanctionProposalLayout.js"></script>

<script src="~/Scripts/custom/Generic.js"></script>

<div id="dvhdSanctionProposalLayout" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;Sanction Proposal</span>

    <span id="spCollapseIconCN" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>
<div id="dvSanctionProposalLayout" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">

    <form id="frmSanctionProposalLayout">

        @Html.HiddenFor(model => model.Level)
        @Html.HiddenFor(model => model.State)
        @Html.HiddenFor(model => model.District)
        @Html.HiddenFor(model => model.Block)
        @*@Html.HiddenFor(model => model.Year)
        @Html.HiddenFor(model => model.Batch)
        @Html.HiddenFor(model => model.Collaboration)
        @Html.HiddenFor(model => model.Status)*@
        @Html.HiddenFor(model => model.StateName)
        @Html.HiddenFor(model => model.DistrictName)
        @Html.HiddenFor(model => model.BlockName)
        @Html.HiddenFor(model => model.CollabName)
        @Html.HiddenFor(model => model.StatusName)
        <div style="width: 100%">
            <table class="rowstyle" style="width: 100%;">
                <tr>
                    <td style="width: 6%;"></td>
                    <td style="text-align: left; width: 5%;">
                        @Html.Label("Year")
                    </td>
                    <td style="width: 15%;">
                        @*@Html.DropDownList("ddYear_PropSanctionDetails", ViewData["YEAR"] as IEnumerable<SelectListItem>, new { @title = "Year", @tabindex = 1 })*@
                        @Html.DropDownListFor(model => model.Year, new SelectList(Model.YearList, "Value", "Text"), new { @id = "ddYear_PropSanctionDetails", @title = "Year", @tabindex = 1 })
                        <br />
                        @Html.ValidationMessageFor(m => m.Year)
                    </td>
                    <td style="text-align: left; width: 5%;">
                        @Html.Label("Batch")
                    </td>
                    <td style="width: 15%;">
                        @*@Html.DropDownList("ddBatch_PropSanctionDetails", ViewData["BATCH"] as IEnumerable<SelectListItem>, new { @title = "Batch", @tabindex = 2 })*@
                        @Html.DropDownListFor(model => model.Batch, new SelectList(Model.BatchList, "Value", "Text"), new { @id = "ddBatch_PropSanctionDetails", @title = "Batch", @tabindex = 2 })
                        <br />
                        @Html.ValidationMessageFor(m => m.Batch)
                    </td>
                    <td style="text-align: left; width: 9%;">
                        @Html.Label("Collaboration")
                    </td>
                    <td style="width: 15%;">
                        @*@Html.DropDownList("ddAgency_PropSanctionDetails", ViewData["AGENCY"] as IEnumerable<SelectListItem>, new { @title = "Collaboration", @tabindex = 3 })*@
                        @Html.DropDownListFor(model => model.Collaboration, new SelectList(Model.CollabList, "Value", "Text"), new { @id = "ddAgency_PropSanctionDetails", @title = "Collaboration", @tabindex = 3 })
                        <br />
                        @Html.ValidationMessageFor(m => m.Collaboration)
                    </td>
                    <td>
                        <input type="button" value="View" class="jqueryButton" id="btnPropSanctionDetails" title="View Details" tabindex="4" />
                    </td>
                </tr>
            </table>
        </div>
    </form>
</div>
<div id="dvLoadSanctionProposalReport" style="width: 100%;">
</div>









