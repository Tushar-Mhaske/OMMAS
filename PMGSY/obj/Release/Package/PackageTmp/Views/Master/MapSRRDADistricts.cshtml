@model PMGSY.Models.Master.AdminDepartmentViewModel

<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script src="@Url.Content("~/Scripts/Master/Master_MapSRRDADistricts.js")" type="text/javascript" ></script>


<div id="dvhdSearch_Map_District" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; margin-left: 0px; margin-right: 0px; margin-top: 10px; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;Map District </span>

    <span id="spCollapseIconS_Map" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>

<div id="dvSearchParameter_Map_District" class="ui-corner-bottom" style="margin-left: 0px; margin-right: 0px; display: block; border: 1px solid #B5B8C8;">

    <form id="frmSearchMapSRRDADistrict">

        @{
                    //string encryptedCode = ViewBag.EncryptedAdminCode;
        }


        <table style="width: 100%;" class="rowstyle">
            <tr style="display: none;">
                <td colspan="4">
                    @Html.Hidden("EncryptedDistrictCodes")
                </td>

            </tr>
            <tr style="display: none;"></tr>
            <tr>

                <td style="width:5%;"></td>
                <td style="width:20%;">@Html.Label("State Name")</td>
                 <td style="width:20%;">
                    @if (!(string.IsNullOrEmpty(ViewBag.StateName)))
                    {

                        <b>@Html.Label("StateName", ViewBag.StateName as String) </b>
                    }
                    else
                    {
                        <b>@Html.Label("StateName", "NA")   </b>
                    }
                </td>
                <td style="width: 20%;">
                    @Html.Label("SRRDA Name")

                </td>
                <td style="width:20%;">
                    @if (!(string.IsNullOrEmpty(ViewBag.SRRDAName)))
                    {
                        <b>@Html.Label("SRRDAName", ViewBag.SRRDAName as String)   </b>
                    }
                    else
                    {
                        <b>@Html.Label("SRRDAName", "NA")   </b>
                    }

                </td>
            </tr>
            <tr>
                <td colspan="5">
                    @Html.HiddenFor(m=>m.EncryptedMapAdminCode)
                    @*@if (!(string.IsNullOrEmpty(ViewBag.EncryptedAdminCode)))
                    {

                        string encryptedCode = ViewBag.EncryptedAdminCode;
                        @Html.Hidden("EncryptedAdminCode",encryptedCode)
                             
                    }*@
                </td>

            </tr>

        </table>

    </form>

    <br />
    <div style="width: 95%;">
        <table id="tbMapSRRDADistrictList">
        </table>
        <div id="dvMapSRRDADistrictListPager"></div>
    </div>
    <br />

    <table style="width: 100%;" class="rowstyle">
        <tr></tr>
        <tr>
            <td style="text-align: center">
                <input type="button" id="btnMapDistrict" name="btnMapDistrict" value="Map District" class="jqueryButton" title="Map District" tabindex="201" />
                <input type="button" id="btnMapDistrictCancel" name="btnMapDistrictCancel" value="Back" class="jqueryButton" title="Back" tabindex="202" />
            </td>
        </tr>
        <tr></tr>
    </table>
</div>
