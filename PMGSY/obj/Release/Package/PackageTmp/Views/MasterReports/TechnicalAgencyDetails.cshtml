@{
    ViewBag.Title = "_TechnicalAgencyDetails";
}
<div style="width: 100%">
    <table class="ui-state-default ui-corner-all" style="width: 100%;">
        <tr>
            <td style="width: 2%;"></td>
            <td style="text-align: left; width: 5%;">
                @Html.Label("State")
            </td>
            <td style="width: 15%;">
                @Html.DropDownList("StateList_TechnicalAgencyDetails", ViewData["STATE"] as IEnumerable<SelectListItem>, new { @title = "State", @tabindex = 1 })
            </td>
            <td style="text-align: left; width: 5%;">
                @Html.Label("TA Type")
            </td>
            <td style="width: 15%;">
                @Html.DropDownList("TATypeList_TechnicalAgencyDetails", ViewData["TATYPE"] as IEnumerable<SelectListItem>, new { @title = "TA Type", @tabindex = 2 })
            </td>
            <td style="text-align: center; width: 20%; margin-left: 5px;">
                <input type="button" id="TechnicalAgencyDetailsButton" value="View" class="jqueryButton" title="View Details" tabindex="3" />
            </td>
            <td style="width: 60%;"></td>
        </tr>
    </table>
</div>
<br />



<div id="TechnicalAgencyDetails">
    <table id="TechnicalAgencyDetailsTable"></table>
</div> 
<div id="TechnicalAgencyDetailsPager"></div>

<script src="~/Scripts/MasterReports/TechnicalAgencyDetails.js" type="text/javascript"></script>
<script src="~/Scripts/custom/Generic.js"></script>