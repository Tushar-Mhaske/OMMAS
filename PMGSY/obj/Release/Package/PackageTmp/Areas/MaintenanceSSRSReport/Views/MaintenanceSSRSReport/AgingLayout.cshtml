@model PMGSY.Areas.MaintenanceSSRSReport.Models.Aging
<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>

<script src="~/Areas/MaintenanceSSRSReport/Scripts/Aging.js"></script>

<div id="dvhdCreateNewReportDetails" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">@*margin-left:0px;margin-right:0px;margin-top:0px; *@
    <span class="ui-jqgrid-title" style="float: left">&nbsp;Ageing of Works Details</span>

    <span id="spCollapseIconCN" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>
<div id="dvSearchParameter" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmPackageWiseReport">
        @Html.ValidationSummary(true)
     
         @Html.HiddenFor(model => model.Mast_State_Code)

        <table style="width: 100%;" class="rowstyle">
            <tr>
                <td style="width: 2%;"></td>
                <td style="text-align: left; width: 4%;">
                    @Html.LabelFor(model => model.StateCode)
                </td>

                <td style="width: 25%;">
                    @if (Model.Mast_State_Code == 0)
                    {
                        @Html.DropDownListFor(model => model.StateCode, new SelectList(Model.StateList, "Value", "Text"), new { @title = "State", @tabindex = 1, @id = "StateList_PackageWiseDetails", @style = "width:150px" })
                        @Html.ValidationMessageFor(m => m.StateCode)
                    }
                    else
                    {
                        @Html.DisplayFor(model => model.StateName) 
                        @Html.Hidden("StateCode", Model.Mast_State_Code)                            
                    }
                </td>
                <td style="text-align: left; width: 4%;">
                    @Html.LabelFor(model => model.DelayGroupCode)
                </td>
                <td style="width: 25%;">
                  @Html.DropDownListFor(model => model.DelayGroupCode, new SelectList(Model.DelayGroupList, "Value", "Text"), new { @title = "Delay Group", @id = "Delayddl", @tabindex = 2, @style = "width:150px" })
                @Html.ValidationMessageFor(m => m.DelayGroupCode)

                </td>
                <td style="text-align: left; width: 4%;">
                    
                </td>

                <td style="width: 25%; ">
                   
                </td>
            </tr>

             <tr>
                <td style="width: 2%;"></td>
                <td style="text-align: left; width: 4%;">
                 @Html.LabelFor(model => model.EXP_START_RANGE)<b style="color: red">*</b>
                </td>

                <td style="width: 25%;">
                   @Html.TextBoxFor(model => model.EXP_START_RANGE, new { @id = "ID_EXP_START_RANGE", @tabindex = 3,@placeholder = "Enter Exp Start Range Value",@maxlength=9 })
                     @Html.ValidationMessageFor(m => m.EXP_START_RANGE)
                </td>
                <td style="text-align: left; width: 4%;">
                     @Html.LabelFor(model => model.EXP_END_RANGE)<b style="color: red">*</b>
                </td>
                <td style="width: 25%;">
                 @Html.TextBoxFor(model => model.EXP_END_RANGE, new { @id = "ID_EXP_END_RANGE", @tabindex = 4, @placeholder = "Enter Exp End Range Value",@maxlength=9 })
                     @Html.ValidationMessageFor(m => m.EXP_END_RANGE)

                </td>
                <td style="text-align: left; width: 4%;">
                       <input type="button" id="btnViewPackageWiseDetails" value="View" class="jqueryButton" title="View Details" tabindex="5" />
                </td>

                <td style="width: 25%; ">
                   
                </td>
            </tr>

                   


        </table>
    </form>

</div>

<div id="loadPackageWiseReport" class="divLoadReport"></div>


