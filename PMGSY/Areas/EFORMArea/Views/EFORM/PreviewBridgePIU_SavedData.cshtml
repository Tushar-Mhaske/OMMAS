@{
    ViewBag.Title = "PreviewBridgePIU_SavedData";
}

@model PMGSY.Areas.EFORMArea.PiuBridgeViewPdfModel.EFORM_BRIDGE_PIU_PREVIEW_MODEL

<head>
    <title>PIU Bridge PDF View</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <link href="~/Content/Bootstrap/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/Bootstrap/bootstrap.min.js" type="text/javascript"></script>

    <style type="text/css">

        /*=========== Common Stylesheet ============*/

        input[type="text"] {
            font-size: 18px;
        }

        /* inputbox width for values like codes,length,dates, */
        .inputCodeWidth {
            width: 100px;
            min-height: 32px;
        }

        .inputShortWidth {
            width: 150px;
            min-height: 32px;
        }

        .inputMediumWidth {
            width: 250px;
            min-height: 32px;
        }

        .inputMidlLargeWidth {
            width: 400px;
            min-height: 32px;
        }

        .inputLargeWidth {
            width: 700px;
            min-height: 32px;
        }

        .SectionUnderline {
            border: 0.5px solid gray;
        }

        .PageBreakLine {
            border: 3px solid gray;
            margin-top: 35px;
            margin-bottom: 35px;
        }

        .table-borderless td {
            border: none;
        }

        .table-bordered thead th {
            vertical-align: top;
            text-align: center;
            background-color: #8c8c8c24;
        }

        .table-hover tbody tr:hover td {
            background-color: #d5bcbc24;
        }

        .fontPIUPdf {
            font-size: 18px;
            font-family: Calibri;
        }

        .PiuLeftHeading {
            width: 300px;
            text-align: left;
        }

        /*-------------------- new added --------------------*/
        .piuleftSrNo {
            width: 5px;
            font-weight: bold;
        }
    </style>
</head>



<body>
    @* --------------------------------- PIU start --------------------------------- *@

    <div class="container-fluid" style="background-color:lightgray; border: 1px solid gray; margin-top: auto;margin-bottom: auto;">
        <div class="container-fluid mt-6 p-4" style="width: 75%; background-color: #fff; margin-top: 50px; margin-bottom: 50px; margin-left: 150px">

            <!-- 1. Headings and General Details -->
            <div id="GeneralDetailsBridge_PIU">
                <div class="container-fluid">
                    <!-- HEADLINES -->
                    <div class="post-container" id="HeadingsPIU" style="text-align: center;">
                       @*<div class="post-thumb"><img src="~/Areas/EFORMArea/Content/LionAshokaChakra.png" style="width: 10%; margin-top: 40px; " /></div>*@
                        <div class="post-content">
                            <h1> <b> National Rural Infrastructure Development Agency </b> </h1>
                            <h2>Ministry of Rural Development, Government of India</h2>
                            <h4 style="text-align:left;"> EFORM ID <input type="text" value="@Model.BRIDGE_PIU_PREFILLED_VIEW.Eform_Id" class="inputShortWidth" disabled data-toggle="tooltip" data-placement="bottom" title="EFORM ID"> <span style="float:right;color:red">Template Version <input type="text" value="@Model.PDF_UPLOAD_DETAIL_VIEW.TEMPLATE_VERSION" class="inputCodeWidth" data-toggle="tooltip" data-placement="bottom" title="Template Version" disabled></span>  </h4>
                            <h4> <span style="float:right;">Format QB Version 2021.0- Part I </span>  </h4>
                            <br />
                            <h3> <b> Format for information to QM for Inspection of Ongoing/Completed Bridge Work </b> </h3>
                            <h3> <u><b> PART I- Work Information  </b> <i> (To be filled-up by PIU) </i> </u> </h3>
                        </div>
                    </div>

                    <h3><b> 1. GENERAL DETAILS </b></h3>
                    <hr style="border: 0.5px solid gray">
                    <table class="table table-hover table-borderless fontPIUPdf">
                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> I </td>
                            <td class="PiuLeftHeading">Work: (check✓ any one box) </td>
                            <td colspan="2">
                                @if (Model.BRIDGE_PIU_PREFILLED_VIEW.WORK_STATUS.Contains("P") || Model.BRIDGE_PIU_PREFILLED_VIEW.WORK_STATUS.Contains("M"))
                                {
                                    <input type="checkbox" checked disabled /> <label> In-Progress  &emsp; &emsp;</label>
                                    <input type="checkbox" disabled /> <label>Completed </label>
                                }
                                else if (Model.BRIDGE_PIU_PREFILLED_VIEW.WORK_STATUS.Contains("C"))
                                {
                                    <input type="checkbox" disabled /> <label> In-Progress  &emsp; &emsp;</label>
                                    <input type="checkbox" checked disabled /> <label> Completed </label>
                                }
                                else
                                {
                                    <input type="checkbox" disabled /> <label> In-Progress &emsp; &emsp;</label>
                                    <input type="checkbox" disabled /> <label> Completed </label>
                                }


                            </td>
                        </tr>


                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> II </td>
                            <td class="PiuLeftHeading">Date of inspection </td>
                            <td> <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="Date of inspection" class="inputShortWidth" disabled> </td>
                        </tr>

                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> III </td>
                            <td class="PiuLeftHeading">Name of Quality Monitor (QM)</td>
                            <td colspan="2">
                                <input type="text" value="@Model.BRIDGE_PIU_PREFILLED_VIEW.QM_NAME" data-toggle="tooltip" data-placement="bottom" title="Name of Quality Monitor (QM)" class="inputMidlLargeWidth" disabled>
                                &emsp;&emsp;

                                @if (Model.BRIDGE_PIU_PREFILLED_VIEW.QM_TYPE.Equals("I"))
                                {
                                    <input type="checkbox" checked disabled> <label> NQM &emsp; </label> <input type="checkbox" disabled> <label> SQM </label>
                                }
                                else if (Model.BRIDGE_PIU_PREFILLED_VIEW.QM_TYPE.Equals("S"))
                                {
                                    <input type="checkbox" disabled> <label> NQM &emsp; </label> <input type="checkbox" checked disabled> <label> SQM </label>
                                }
                                else
                                {
                                    <input type="checkbox" disabled> <label> NQM &emsp; </label> <input type="checkbox" disabled> <label> SQM </label>
                                }



                            </td>
                        </tr>

                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> IV </td>
                            <td class="PiuLeftHeading">State </td>
                            <td colspan="2">
                                <input type="text" value="@Model.BRIDGE_PIU_PREFILLED_VIEW.State" data-toggle="tooltip" data-placement="bottom" title="State" disabled>
                                &emsp;&emsp;
                                District   <input type="text" value="@Model.BRIDGE_PIU_PREFILLED_VIEW.District" data-toggle="tooltip" data-placement="bottom" title="District" disabled>
                                &emsp;&emsp;
                                Block  <input type="text" value="@Model.BRIDGE_PIU_PREFILLED_VIEW.Block" data-toggle="tooltip" data-placement="bottom" title="Block" disabled>
                            </td>
                        </tr>
                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> V </td>
                            <td class="PiuLeftHeading">Name of of bridge & Location </td>
                            <td colspan="2">
                                <div class="grow-wrap p-2 border border-grey text-wrap fontPIUPdf" style="border: 1px solid lightgray; color: #666; background-color: #eee; min-height: 30px; max-width: 780px;" data-toggle="tooltip" data-placement="bottom" title="Name of of bridge & Location">
                                    <p class="fontPIUPdf px-2">
                                        @Model.BRIDGE_PIU_PREFILLED_VIEW.RoadName
                                    </p>
                                </div>
                            </td>
                        </tr>

                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> VI </td>
                            <td class="PiuLeftHeading">Package Number </td>
                            <td colspan="2"> <input type="text" value="  @Model.BRIDGE_PIU_PREFILLED_VIEW.PkgNumber" data-toggle="tooltip" data-placement="bottom" title="Package Number" class="inputMediumWidth" disabled> </td>
                        </tr>

                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> VII </td>
                            <td class="PiuLeftHeading">Sanctioned length of the bridge </td>
                            <td> <input type="text" value="  @Model.BRIDGE_PIU_PREFILLED_VIEW.SancLength" data-toggle="tooltip" data-placement="bottom" title="Sanctioned Length" class="inputShortWidth" disabled> m </td>
                        </tr>

                        <tr>
                            <td></td>
                            <td class="PiuLeftHeading"> Deviation in length of the bridge <br />(If any) </td>
                            <td> <input type="text" value="  @Model.BRIDGE_PIU_GENERAL_INFO_VIEW.DEVIATION_LENGTH" data-toggle="tooltip" data-placement="bottom" title="Deviation in length of the bridge" class="inputShortWidth" disabled> m </td>
                        </tr>

                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> VIII </td>
                            <td class="PiuLeftHeading"> Reasons for deviation <i>(if any)</i> </td>
                            <td colspan="2">
                                <div class="grow-wrap p-2" style="border:1px solid grey; background-color: #cccccc3d; color: #333333cf; min-height: 30px; max-width: 780px;" data-toggle="tooltip" data-placement="bottom" title="Reasons for deviation">
                                    @Model.BRIDGE_PIU_GENERAL_INFO_VIEW.DEVIATION_REASON
                                </div>
                            </td>
                        </tr>

                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> IX </td>
                            <td class="PiuLeftHeading">Estimated cost (as cleared by GOI) </td>
                            <td> <input type="text" value="&#8377; @Model.BRIDGE_PIU_PREFILLED_VIEW.ESTIMATED_COST " data-toggle="tooltip" data-placement="bottom" title="Estimated cost (as cleared by GOI)" class="inputShortWidth" disabled> Lakh</td>
                        </tr>
                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> X </td>
                            <td class="PiuLeftHeading">Technical sanction cost </td>
                            <td> <input type="text" value="&#8377;@Model.BRIDGE_PIU_GENERAL_INFO_VIEW.TECHNICAL_SANC_COST " data-toggle="tooltip" data-placement="bottom" title="Technical sanction cost" class="inputShortWidth" disabled> Lakh</td>
                        </tr>
                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> XI </td>
                            <td class="PiuLeftHeading">Awarded cost </td>
                            <td> <input type="text" value="&#8377;@Model.BRIDGE_PIU_PREFILLED_VIEW.AWARDED_COST " data-toggle="tooltip" data-placement="bottom" title="Awarded cost" class="inputShortWidth" disabled> Lakh</td>
                        </tr>

                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> XII </td>
                            <td class="PiuLeftHeading"> Expenditure <b><i>(If work is ongoing)</i></b> </td>
                        </tr>
                        @if (Model.BRIDGE_PIU_PREFILLED_VIEW.WORK_STATUS == "C")
                        {
                            <tr>
                                <td></td>
                                <td class="PiuLeftHeading"> &emsp;&emsp; a. Expenditure done </td>
                                <td> <input type="text" value="&#8377;" data-toggle="tooltip" data-placement="bottom" title="Expenditure done" class="inputShortWidth" disabled> Lakh</td>
                            </tr>
                            <tr>
                                <td></td>
                                <td class="PiuLeftHeading"> &emsp;&emsp; b. Bills pending </td>
                                <td> <input type="text" value="&#8377;" data-toggle="tooltip" data-placement="bottom" title="Bills Pending" class="inputShortWidth" disabled> Lakh</td>
                            </tr>
                            <tr>
                                <td></td>
                                <td class="PiuLeftHeading"> &emsp;&emsp; c. Total expenditure (a+b) </td>
                                <td> <input type="text" value="&#8377;" data-toggle="tooltip" data-placement="bottom" title="Total expenditure" class="inputShortWidth" disabled> Lakh</td>
                            </tr>
                        }
                        else
                        {
                            <tr>
                                <td></td>
                                <td class="PiuLeftHeading"> &emsp;&emsp; a. Expenditure done </td>
                                <td> <input type="text" value="&#8377; @Model.BRIDGE_PIU_PREFILLED_VIEW.EXPENDITURE_DONE" data-toggle="tooltip" data-placement="bottom" title="Expenditure done" class="inputShortWidth" disabled> Lakh</td>
                            </tr>
                            <tr>
                                <td></td>
                                <td class="PiuLeftHeading"> &emsp;&emsp; b. Bills pending </td>
                                <td> <input type="text" value="&#8377; @Model.BRIDGE_PIU_GENERAL_INFO_VIEW.BILLS_PENDING" data-toggle="tooltip" data-placement="bottom" title="Bills Pending" class="inputShortWidth" disabled> Lakh</td>
                            </tr>
                            <tr>
                                <td></td>
                                <td class="PiuLeftHeading"> &emsp;&emsp; c. Total expenditure (a+b) </td>
                                <td> <input type="text" value="&#8377; @Model.BRIDGE_PIU_GENERAL_INFO_VIEW.TOTAL_EXPENDITURE" data-toggle="tooltip" data-placement="bottom" title="Total expenditure" class="inputShortWidth" disabled> Lakh</td>
                            </tr>
                        }

                        @if (Model.BRIDGE_PIU_PREFILLED_VIEW.WORK_STATUS == "C")
                        {
                            <tr>
                                <td class="piuleftSrNo" style="text-align:center"> XIII </td>
                                <td class="PiuLeftHeading"> Completion cost<b> <br /> <i>(If work is completed)</i></b> </td>
                                <td> <input type="text" value="&#8377; @Model.BRIDGE_PIU_PREFILLED_VIEW.COMPLETION_COST" data-toggle="tooltip" data-placement="bottom" title="Completion cost" class="inputShortWidth" disabled> Lakh</td>
                            </tr>
                        }
                        else
                        {
                            <tr>
                                <td class="piuleftSrNo" style="text-align:center"> XIII </td>
                                <td class="PiuLeftHeading"> Completion cost<b> <br /> <i>(If work is completed)</i></b> </td>
                                <td> <input type="text" value="&#8377; " data-toggle="tooltip" data-placement="bottom" title="Completion cost" class="inputShortWidth" disabled> Lakh</td>
                            </tr>
                        }






                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> XIV </td>
                            <td class="detailtablesPiuLeftHeading"> Date of award of work <br /> <i>(date/ month/ year)</i></td>
                            <td> <input type="text" value="@Model.BRIDGE_PIU_GENERAL_INFO_VIEW.AWARD_OF_WORK_DATE" data-toggle="tooltip" data-placement="bottom" title="Date of award of work" class="inputShortWidth" disabled> </td>
                        </tr>
                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> XV </td>
                            <td class="detailtablesPiuLeftHeading"> Date of start of work <br /> <i>(date/ month/ year)</i></td>
                            <td> <input type="text" value="@Model.BRIDGE_PIU_GENERAL_INFO_VIEW.START_OF_WORK_DATE" data-toggle="tooltip" data-placement="bottom" title="Date of start of work" class="inputShortWidth" disabled> </td>
                        </tr>
                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> XVI </td>
                            <td class="detailtablesPiuLeftHeading"> Stipulated date of completion <br /> <i>(date/ month/ year)</i></td>
                            <td> <input type="text" value="@Model.BRIDGE_PIU_GENERAL_INFO_VIEW.STIPULATED_COMPLETION_DATE" data-toggle="tooltip" data-placement="bottom" title="Stipulated date of completion" class="inputShortWidth" disabled> </td>
                        </tr>

                        @if (Model.BRIDGE_PIU_PREFILLED_VIEW.WORK_STATUS == "C")
                        {
                            <tr>
                                <td class="piuleftSrNo" style="text-align:center"> XVII </td>
                                <td class="detailtablesPiuLeftHeading"> Actual date of completion <br /> <i>(date/ month/ year)</i> <br> <b><i>(in case of completed work)</i></b></td>
                                <td> <input type="text" value="@Model.BRIDGE_PIU_GENERAL_INFO_VIEW.ACTUAL_COMPLETION_DATE" data-toggle="tooltip" data-placement="bottom" title="Actual date of completion" class="inputShortWidth" disabled> </td>
                            </tr>
                        }
                        else
                        {
                            <tr>
                                <td class="piuleftSrNo" style="text-align:center"> XVII </td>
                                <td class="detailtablesPiuLeftHeading"> Actual date of completion <br /> <i>(date/ month/ year)</i> <br> <b><i>(in case of completed work)</i></b></td>
                                <td> <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="Actual date of completion" class="inputShortWidth" disabled> </td>
                            </tr>
                        }
                    </table>
                </div>
            </div> <!-- Close Headings and General Details -->
            <br>

            <hr class="PageBreakLine">

            <!-- 2. PHYSICAL PROGRESS: (Ongoing/Completed) -->
            <div id="PhysicalProgressBridge_PIU">
                <div class="container-fluid">
                    <h3><b> 2. PHYSICAL PROGRESS (Ongoing/Completed) </b></h3>
                    <hr style="border: 0.5px solid gray">

                    <div class="table-responsive m-3">
                        <p class="fontPIUPdf"> Construction Programme and Physical Progress </p>
                        <table class="table table-hover table-bordered w-auto text-wrap fontPIUPdf">
                            <thead>
                                <tr>
                                    <th rowspan="2"> Item of work <br /> (1) </th>
                                    <th rowspan="2"> Unit <br /> (2) </th>
                                    <th rowspan="2"> Quantity As per DPR <br /> (3) </th>
                                    <th rowspan="2"> Quantity executed <br /> (4) </th>
                                    <th rowspan="2"> Completed percentage of item <br /> (5) </th>
                                    <th colspan="3"> Fill in start and completion dates </th>
                                    <th rowspan="2"> Delay in months <br /> (9) </th>

                                <tr>
                                    <th> Progress status <br /> (6) </th>
                                    <th> Start Date <br /> (7) </th>
                                    <th> Completion Date <br /> (8) </th>
                                </tr>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in @Model.BRIDGE_PIU_PRGS_DETAILS_VIEW)
                                {
                                    foreach (var thd in @Model.BRIDGE_MASTER_WORK_ITEM_VIEW)
                                    {
                                        if (thd.ITEM_ID == item.ITEM_ID)
                                        {


                                            <tr>
                                                <td>@(@thd.ITEM_DESC == null || thd.ITEM_DESC =="" ? "" : thd.ITEM_DESC)</td>
                                                <td>@(@item.IEM_UNIT  == null || @item.IEM_UNIT  =="" ? "" : @item.IEM_UNIT)</td>
                                                <td>@(@item.DPR_QUANTITY)</td>
                                                <td>@(@item.EXECUTED_QUANTITY)</td>
                                                <td>@(@item.COMPLETED_PERC)</td>

                                                <td>
                                                    Due
                                                    <hr style="border: 0.5px solid lightgrey" />
                                                    Actual
                                                </td>
                                                <td>
                                                    @(@item.DUE_START_DATE == null ? "" : @item.DUE_START_DATE)
                                                    <hr style="border: 0.5px solid lightgrey" />
                                                    @(@item.ACTUAL_START_DATE == null ? "" : @item.ACTUAL_START_DATE)

                                                </td>

                                                <td>
                                                    @(@item.DUE_END_DATE == null ? "" : @item.DUE_END_DATE)
                                                    <hr style="border: 0.5px solid lightgrey" />
                                                    @(@item.ACTUAL_END_DATE == null ? "" : @item.ACTUAL_END_DATE)

                                                </td>
                                                <td>
                                                    @item.DELAY_MONTH

                                                </td>

                                            </tr>
                                        }
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div> <!-- Close PHYSICAL PROGRESS: (Ongoing/Completed) -->
            <br>

            <hr class="PageBreakLine">

            <!-- 3. PARTICULARS OF THE BRIDGE -->
            <div id="ParticularsBridge_PIU">
                <div class="container-fluid">
                    <h3><b> 3. PARTICULARS OF THE BRIDGE </b></h3>
                    <hr style="border: 0.5px solid gray">
                    <table class="table table-hover table-borderless fontPIUPdf">
                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> 3.1 </td>
                            <td class="PiuLeftHeading"> Type of the bridge (according to HFL) </td>
                            <td colspan="2">
                                @if (Model.BRIDGE_PIU_PARTICULAR_VIEW.BRIDGE_TYPE_HFL.Equals("Submersible"))
                                {
                                    <input type="checkbox" checked disabled /> <label> Submersible  &emsp; &emsp;</label>
                                    <input type="checkbox" disabled /> <label> Non-Submersible </label>
                                }
                                else if (Model.BRIDGE_PIU_PARTICULAR_VIEW.BRIDGE_TYPE_HFL.Equals("Non-submersible"))
                                {
                                    <input type="checkbox" disabled /> <label> Submersible  &emsp; &emsp;</label>
                                    <input type="checkbox" checked disabled /> <label> Non-Submersible </label>
                                }
                                else
                                {
                                    <input type="checkbox" disabled /> <label> Submersible  &emsp; &emsp;</label>
                                    <input type="checkbox" disabled /> <label> Non-Submersible </label>
                                }

                            </td>
                        </tr>

                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> </td>
                            <td class="PiuLeftHeading"> (according to Material) </td>
                            <td colspan="2">
                                @if (Model.BRIDGE_PIU_PARTICULAR_VIEW.BRIDGE_TYPE_MATERIAL.Equals("RCC"))
                                {
                                    <input type="checkbox" checked disabled /> <label> RCC  &emsp; &emsp;</label>  
                                    <input type="checkbox" disabled /> <label> Steel </label>
                                }
                                else if (Model.BRIDGE_PIU_PARTICULAR_VIEW.BRIDGE_TYPE_MATERIAL.Equals("Steel"))
                                {
                                    <input type="checkbox" disabled /> <label> RCC  &emsp; &emsp;</label> 
                                    <input type="checkbox" checked disabled /> <label> Steel </label>
                                }
                                else
                                {
                                    <input type="checkbox" disabled /> <label> RCC  &emsp; &emsp;</label>  
                                    <input type="checkbox" disabled /> <label> Steel </label>
                                }

                            </td>
                        </tr>
                        <tr>
                            <td></td>
                            <td></td>
                            <td colspan="2">
                                @if (Model.BRIDGE_PIU_PARTICULAR_VIEW.BRIDGE_TYPE_MATERIAL.Equals("Composite"))
                                {
                                    <input type="checkbox" checked disabled /> <label> Composite  &emsp; &emsp;</label>
                                    <input type="checkbox" disabled /> <label> Pre-stress </label>
                                }
                                else if (Model.BRIDGE_PIU_PARTICULAR_VIEW.BRIDGE_TYPE_MATERIAL.Equals("Pre-Stress"))
                                {
                                    <input type="checkbox" disabled /> <label> Composite  &emsp; &emsp;</label>
                                    <input type="checkbox" checked disabled /> <label> Pre-stress </label>
                                }
                                else
                                {
                                    <input type="checkbox" disabled /> <label> Composite  &emsp; &emsp;</label>
                                    <input type="checkbox" disabled /> <label> Pre-stress </label>
                                }

                            </td>
                        </tr>
                        <tr>
                            <td class="piuleftSrNo" style="text-align:center"> 3.2 </td>
                            <td class="PiuLeftHeading"> Type of foundation </td>
                            <td colspan="2">
                                @if (Model.BRIDGE_PIU_PARTICULAR_VIEW.FOUNDATION_TYPE.Equals("Open"))
                                {
                                    <input type="checkbox" checked disabled /> <label> Open  &emsp; &emsp;</label> 
                                    <input type="checkbox" disabled /> <label> Raft </label>
                                }
                                else if (Model.BRIDGE_PIU_PARTICULAR_VIEW.FOUNDATION_TYPE.Equals("Raft"))
                                {
                                    <input type="checkbox" disabled /> <label> Open  &emsp; &emsp;</label>  
                                    <input type="checkbox" checked disabled /> <label> Raft </label>
                                }
                                else
                                {
                                    <input type="checkbox" disabled /> <label> Open  &emsp; &emsp;</label> 
                                    <input type="checkbox" disabled /> <label> Raft </label>
                                }

                            </td>
                        </tr>
                        <tr>
                            <td></td>
                            <td> </td>
                            <td colspan="2">
                                @if (Model.BRIDGE_PIU_PARTICULAR_VIEW.FOUNDATION_TYPE.Equals("Pile"))
                                {
                                    <input type="checkbox" checked disabled /> <label> Pile  &emsp; &emsp;</label>  
                                    <input type="checkbox" disabled /> <label> Well </label>
                                }
                                else if (Model.BRIDGE_PIU_PARTICULAR_VIEW.FOUNDATION_TYPE.Equals("Well"))
