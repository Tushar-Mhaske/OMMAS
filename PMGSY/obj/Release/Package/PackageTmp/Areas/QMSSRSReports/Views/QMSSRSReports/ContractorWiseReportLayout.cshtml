@model PMGSY.Areas.QMSSRSReports.Models.ContractorWiseReportModel
<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script src="~/Areas/QMSSRSReports/Scripts/ContractorwiseInspection.js"></script>

<div id="dvhdContractorwiseInspectionsLayout" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">Contractorwise Inspections</span>
</div>
<div id="dvSearchParameter" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmLoadContractorwiseReport">
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)
        @Html.Hidden("hdnRoleCode", PMGSY.Extensions.PMGSYSession.Current.RoleCode.ToString())

        <table style="width: 100%;" class="rowstyle">
            <tr>
                <td style="width: 12%;"></td>
                <td style="width: 8%;">
                    @Html.LabelFor(model => model.YEAR_INSPECTION)
                </td>
                <td style="width: 23%;">
                    @Html.DropDownListFor(model => model.YEAR_INSPECTION, new SelectList(Model.YEAR_INSPECTION_LIST, "Value", "Text"), new { @title = "Inspection Year", @tabindex = 6, @id = "ddlYearInsp", @style = "width:155px" })
                    @Html.ValidationMessageFor(m => m.YEAR_INSPECTION)
                </td>
                <td style="width: 12%;">
                    <input type="button" value="View" title="View Details" id="btn_View_Report" class="jqueryButton ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only" />
                </td>
                <td style="width: 12%; float: right;"></td>
                <td style="width: 12%; float: right;"></td>
                <td style="width: 12%; float: right;"></td>
            </tr>
        </table>
    </form>
</div>

<div id="LoadContractorwiseReport" class="divLoadReport"></div>
