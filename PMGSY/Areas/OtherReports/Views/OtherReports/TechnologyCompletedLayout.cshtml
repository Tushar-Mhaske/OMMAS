 @model PMGSY.Areas.OtherReports.Models.TechnologyModel
<script src="~/Areas/OtherReports/Scripts/TechnologyReports.js"></script>
 <script src="~/Scripts/custom/Generic.js"></script>
<div id="loadFilters">
    <div id="dvhdCreateNewReportDetails" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; width: 99.9%;height: 20px;margin-top:3px;">
    <span class="ui-jqgrid-title" style="float: left;margin-top:1px;">&nbsp;R&D Targets And Achievements</span>
   </div>
    <form id="RndTargetsForm">
       
        @Html.HiddenFor(model => model.StateName)
        @Html.HiddenFor(model => model.DistName)
        @Html.HiddenFor(model => model.BlockName)
         @Html.AntiForgeryToken()

        <table style="width: 100%;" class=" ui-state-default ui-corner-bottom">
            <tr>
                <td style="width: 2%;"></td>
                <td style="text-align: left; width: 8%;">
                    @Html.LabelFor(model => model.StateCode)
                </td>

                <td style="width: 25%;">
                  
                        @Html.DropDownListFor(model => model.StateCode, new SelectList(Model.StateList, "Value", "Text"), new {  @tabindex = 1, @id = "StateList", @style = "width:150px" })
                        @Html.ValidationMessageFor(m => m.StateCode)
                   
                </td>
                <td style="text-align: left; width: 5%;">
                    @Html.LabelFor(model => model.DistrictCode)
                </td>
                <td style="width: 25%;">
                   
                        @Html.DropDownListFor(model => model.DistrictCode, new SelectList(Model.DistrictList, "Value", "Text"), new {  @id = "DistrictList", @tabindex = 2, @style = "width:150px" })
                        @Html.ValidationMessageFor(m => m.DistrictCode)

                    
                </td>
                <td style="text-align: left; width: 5%;">
                    @Html.LabelFor(model => model.BlockCode)
                </td>

                <td style="width: 25%;">
                     
                        @Html.DropDownListFor(model => model.BlockCode, new SelectList(Model.BlockList, "Value", "Text"), new {  @id = "BlockList", @tabindex = 3, @style = "width:150px" })
                        @Html.ValidationMessageFor(m => m.BlockCode)
                </td>
                <td class="test"></td>
            </tr>
            <tr>
                <td style="width: 2%;"></td>

                <td style="text-align: left; width: 8%;">
                    @Html.LabelFor(model => model.Year)
                </td>
                <td style="width: 25%;">
                    @Html.DropDownListFor(model => model.Year, new SelectList(Model.YearList, "Value", "Text"), new {  @id = "YearList", @tabindex = 4, @style = "width:150px" })
                    @Html.ValidationMessageFor(model => model.Year)
                </td>

                <td style="text-align: left; width: 5%;">
                    @Html.LabelFor(model => model.Batch)
                </td>
                <td style="width: 25%;">
                    @Html.DropDownListFor(model => model.Batch, new SelectList(Model.BatchList, "Value", "Text"), new { @id = "BatchList", @tabindex = 5, @style = "width:150px" })
                    @Html.ValidationMessageFor(model => model.Batch)
                </td>

                <td style="text-align: left; width: 5%;">
                    @Html.LabelFor(model => model.FundingAgency)
                </td>
                <td style="width: 25%;">
                    @Html.DropDownListFor(model => model.FundingAgency, new SelectList(Model.FundingAgencyList, "Value", "Text"), new {  @id = "FundingAgencyList", @tabindex = 6, @style = "width:150px" })
                    @Html.ValidationMessageFor(model => model.FundingAgency)
                </td>
            </tr>
            <tr>
                <td style="width: 2%;"></td>
        

                <td style="text-align: left; width: 5%;">
                    @Html.LabelFor(model => model.ConnectivityCode)
                </td>
                <td style="width: 25%;">
                    @Html.DropDownListFor(model => model.ConnectivityCode, new SelectList(Model.ConnectivitysList, "Value", "Text"), new { @id = "Connectivity1", @tabindex = 5, @style = "width:150px" })
                    @Html.ValidationMessageFor(model => model.ConnectivityCode)
                </td>

                <td class="test1" style="text-align: left; width: 5%;">
                    @Html.LabelFor(model => model.RoadStatus)
                </td>
                <td class="test1" style="width: 25%;">
                    @Html.DropDownListFor(model => model.RoadStatus, new SelectList(Model.RoadStatusList, "Value", "Text"), new {  @tabindex = 5, @style = "width:150px" })
                    @Html.ValidationMessageFor(model => model.RoadStatus)
                </td>
                 <td>
                    <input type="button" id="btnRndViewStateListRoad" value="View" class="button" title="View" style="margin-left:101%" tabindex="7" />
                </td>
            </tr>
            <tr>
                <td style="width: 2%;"></td>
              @*  <td>
                    <input type="button" id="btnRndViewStateListRoad" value="View" class="button" title="View" style="margin-left:101%" tabindex="7" />
                </td>*@
            </tr>
        </table>
    </form>
    <div id="divRndStateListRoad" class="modal" style="margin-top: 12px; float: right; margin-left: 870px">
        <span id="spnRndStateListRoad" class="close">X</span>
    </div>
</div>

<div id="loadRndReport" class="divLoadReport">

</div>


