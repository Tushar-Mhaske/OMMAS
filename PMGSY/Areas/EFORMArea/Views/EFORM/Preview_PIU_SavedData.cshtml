                                    <th rowspan="2"> Sl No. </th>
                                    <th rowspan="2"> Name of Assistant engineer </th>
                                    <th rowspan="2"> Employ number </th>
                                    <th rowspan="2"> Mobile number </th>
                                    <th colspan="2"> Duration of posting at site </th>

                                <tr>
                                    <th> From </th>
                                    <th> To </th>
                                </tr>
                                </tr>
                            </thead>
                            <tbody>
                                @{int rowcountAssistant = 1;}
                                
                                @foreach (var item in @Model.QC_OFFICIAL_DETAILS_LIST)
                                {
                                    if (item.OFFICIAL_TYPE.Equals("A"))
                                    {
                                        <tr>
                                            <td> @rowcountAssistant @{rowcountAssistant++;} </td>
                                            <td>@item.OFFICIAL_NAME </td>
                                            <td>@item.IDENTITY_NUMBER </td>
                                            <td>@item.MOBILE_NO </td>
                                            <td>@item.FROM_DATE </td>
                                            <td>@item.TO_DATE </td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>

                    <p class="fontPIUPdf m-3"> IX. Details of the Junior engineer supervising the work </p>
                    <div class="table-responsive m-3">
                        <table class="table table-hover table-bordered fontPIUPdf" align="center" style="width: 75%;">
                            <thead style="vertical-align: top; text-align: center;">
                                <tr>
                                    <th rowspan="2"> Sl No. </th>
                                    <th rowspan="2"> Name of Junior engineer </th>
                                    <th rowspan="2"> Employ number </th>
                                    <th rowspan="2"> Mobile number </th>
                                    <th colspan="2"> Duration of posting at site </th>

                                <tr>
                                    <th> From </th>
                                    <th> To </th>
                                </tr>
                                </tr>
                            </thead>
                            <tbody>
                                @{int rowcountJuniorEng = 1;}                                                                
                                @foreach (var item in @Model.QC_OFFICIAL_DETAILS_LIST)
                                {
                                    if (item.OFFICIAL_TYPE.Equals("J"))
                                    {
                                        <tr>
                                            <td> @rowcountJuniorEng @{rowcountJuniorEng++;} </td>
                                            <td>@item.OFFICIAL_NAME</td>
                                            <td>@item.IDENTITY_NUMBER</td>
                                            <td>@item.MOBILE_NO</td>
                                            <td>@item.FROM_DATE</td>
                                            <td>@item.TO_DATE</td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>

                    <div class="container-fluid">
                        <table class="table table-hover table-borderless fontPIUPdf">
                            <tbody>
                                <tr>
                                    <td class="detailtablesPiuLeftHeading">X. List of equipments available in field lab</td>
                                    <td>
                                        <div class="grow-wrap rounded p-2 border border-grey text-wrap fontPIUPdf" style="border: 1px solid lightgray; color: #666; background-color: #eee; min-height: 50px;" data-toggle="tooltip" data-placement="bottom" title="List of equipments available in field lab">
                                            @Model.QC_DETAILS_PIU_view.LAB_EQUIP_AVBL
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="detailtablesPiuLeftHeading"> Table (IX) a </td>
                                </tr>
                                <tr style="border: 1px solid grey;">
                                    <th class="detailtablesPiuLeftHeading" style="border: 1px solid grey; vertical-align: text-bottom;text-align: center;"> Available equipments that are in working condition (1) </th>
                                    <th class="detailtablesPiuLeftHeading" style="border: 1px solid grey; vertical-align: text-bottom;text-align: center;"> Available equipments that are not in working condition (2) </th>
                                </tr>
                                <tr style="border: 1px solid grey;">
                                    <td style="border: 1px solid grey;">
                                        <div class="grow-wrap rounded p-2 border border-grey text-wrap fontPIUPdf" style="border: 1px solid lightgray; color: #666; background-color: #eee; min-height: 80px;" data-toggle="tooltip" data-placement="bottom" title="Available equipments that are in working condition (1)">
                                            @Model.QC_DETAILS_PIU_view.EQUIP_WORKING
                                        </div>
                                    </td>
                                    <td style="border: 1px solid grey;">
                                        <div class="grow-wrap rounded p-2 border border-grey text-wrap fontPIUPdf" style="border: 1px solid lightgray; color: #666; background-color: #eee; min-height: 80px;" data-toggle="tooltip" data-placement="bottom" title="Available equipments that are not in working condition (2)">
                                            @Model.QC_DETAILS_PIU_view.EQUIP_NOT_WORKING
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="detailtablesPiuLeftHeading">XI. List of equipments not available in field lab</td>
                                    <td>
                                        <div class="grow-wrap rounded p-2 border border-grey text-wrap fontPIUPdf" style="border: 1px solid lightgray; color: #666; background-color: #eee; min-height: 50px;" data-toggle="tooltip" data-placement="bottom" title="List of equipments not available in field lab">
                                            @Model.QC_DETAILS_PIU_view.LAB_EQUIP_NOT_AVBL
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2">XII. Reasons put forth by PIU for non availability of equipments in field lab</td>
                                </tr>
                                <tr>
                                    <td colspan="2">
                                        <div class="grow-wrap rounded p-2 border border-grey text-wrap fontPIUPdf" style="border: 1px solid lightgray; color: #666; background-color: #eee; min-height: 50px;" data-toggle="tooltip" data-placement="bottom" title="Reasons put forth by PIU for non availability of equipments in field lab">
                                            @Model.QC_DETAILS_PIU_view.REASON_LAB_EQUIP_NOT_AVBL
                                        </div>
                                    </td>
                                </tr>

                                <tr>
                                    <td colspan="2">XIII. Equipments and documents ready to be made available to QM before or during the inspection</td>
                                </tr>
                                <tr>
                                    <td colspan="2">
                                        <div class="grow-wrap rounded p-2 border border-grey text-wrap fontPIUPdf" style="border: 1px solid lightgray; color: #666; background-color: #eee; min-height: 50px;" data-toggle="tooltip" data-placement="bottom" title="Equipments and documents ready to be made available to QM before or during the inspection">
                                            @Model.QC_DETAILS_PIU_view.DOCUMENT_FOR_QM
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div> <!-- Close QUALITY CONTROL -->
            <br>

            <hr class="PageBreakLine">

            <!-- 4.DETAILS OF MIX DESIGN(s)-->
            <div id="PhysicalProgress_PIU">
                <div class="container-fluid">
                    <h3><b> 4. DETAILS OF MIX DESIGN(s) <span style="font-size: medium;">(if provided in the sanctioned project)</span> </b></h3>
                    <hr style="border: 0.5px solid gray">
                    @if (Model.GENERAL_INFO_PIU_view.MIX_DESIGN_APPLICABLE.Equals("Y"))
                    {
                        <p class="fontPIUPdf m-3">Is it applicable <b>(check the box)</b> &emsp;&emsp;&emsp;&emsp; <input type="checkbox" checked disabled /> Yes &emsp;&emsp; <input type="checkbox" disabled /> No </p>
                    }
                    else if (Model.GENERAL_INFO_PIU_view.MIX_DESIGN_APPLICABLE.Equals("N"))
                    {
                        <p class="fontPIUPdf m-3">Is it applicable <b>(check the box)</b> &emsp;&emsp;&emsp;&emsp; <input type="checkbox" disabled /> Yes &emsp;&emsp; <input type="checkbox" checked disabled /> No </p>
                    }
                    else {
                        <p class="fontPIUPdf m-3">Is it applicable <b>(check the box)</b> &emsp;&emsp;&emsp;&emsp; <input type="checkbox" disabled /> Yes &emsp;&emsp; <input type="checkbox" disabled /> No </p>
                    }

                    <div class="table-responsive m-3">
                        <table class="table table-hover table-bordered fontPIUPdf">
                            <thead style="vertical-align: top; text-align: center;">
                            <th> Sl No. </th>
                            <th> Mix Design </th>
                            <th> Mix Design Strength </th>
                            <th> Institute/laboratory where mix design was done </th>
                            <th> Date </th>
                            </thead>
                            <tbody>
                                @{int rowcountMixDesign = 1;}
                                @foreach (var item in @Model.MIX_DESIGN_DETAILS_LIST)
                                {
                                    foreach (var thd in @Model.Work_Item_List)
                                    {
                                        if (thd.ITEM_ID == item.ITEM_ID)
                                        {
                                            <tr>
                                                <td> @rowcountMixDesign @{rowcountMixDesign++;} </td>
                                                <td> @thd.ITEM_DESC </td>
                                                <td> @item.DESIGN_STRENGTH </td>
                                                <td> @item.DESIGN_LAB </td>
                                                <td> @item.DESIGN_DATE </td>
                                            </tr>
                                        }
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div> <!-- Close DETAILS OF MIX DESIGN(s) -->
            <br>

            <hr class="PageBreakLine">

            <!-- 5.EARLIER INSPECTIONS BY NQM, SQM or SENIOR DEPARTMENTAL OFFICERS (SEs & CEs) AND ACTION TAKEN STATEMENT -->
            <div id="PhysicalProgress_PIU">
                <div class="container-fluid">
                    <h3><b> 5. EARLIER INSPECTIONS BY NQM, SQM or SENIOR DEPARTMENTAL OFFICERS (SEs & CEs) AND ACTION TAKEN STATEMENT </b></h3>
                    <p class="fontPIUPdf">  (Details about departmental officers who inspected the work should be added after the NQM/SQM details.) </p>
                    <hr style="border: 0.5px solid gray">

                    <div class="table-responsive m-3">
                        <table class="table table-hover table-bordered fontPIUPdf">
                            <thead style="vertical-align: top; text-align: center;">
                                <tr>
                                    <th> Date of visit </th>
                                    <th> Name and Designation of inspecting officer (NQM/SQM/CE/SE) </th>
                                    <th colspan="2"> Road distance (RD) </th>
                                    <th> Level of work at the time of inspection </th>
                                    <th> Major observations </th>
                                    <th> Action taken by PIU with  date </th>

                                <tr>
                                    <th> (1) </th>
                                    <th> (2) </th>
                                    <th> From <br> (3) </th>
                                    <th> To <br> (4) </th>
                                    <th> (5) </th>
                                    <th> (6) </th>
                                    <th> (7) </th>
                                </tr>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in @Model.PREVIOUS_INSP_DETAILS_LIST)
                                {
                                    <tr>
                                        <td>@(@item.VISIT_DATE == null ? "" : @item.VISIT_DATE)</td>
                                        <td>@(@item.VISITOR_NAME_DESG == null || @item.VISITOR_NAME_DESG == "" ? "" : @item.VISITOR_NAME_DESG)</td>
                                        <td>@(@item.ROAD_FROM)</td>
                                        <td>@(@item.ROAD_TO)</td>
                                        <td>@(@item.INSP_LEVEL  == null || @item.INSP_LEVEL  =="" ? "" : @item.INSP_LEVEL)</td>
                                        <td>@(@item.OBSERVATIONS  == null || @item.OBSERVATIONS  == "" ? "" : @item.OBSERVATIONS)</td>
                                        <td>@(@item.ACTION  == null || @item.ACTION  == "" ? "" : @item.ACTION)</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div> <!-- Close EARLIER INSPECTIONS BY NQM, SQM or SENIOR DEPARTMENTAL OFFICERS (SEs & CEs) AND ACTION TAKEN STATEMENT -->
            <br>

            <p class="fontPIUPdf" style="margin-left: 130px;"> <b>Seal of PIU</b> </p>
            <div class="container-fluid m-3" style="display: flex; justify-content: space-between;">

                <div class="m-3" id="piuSEAL" style="border: 1px solid black; height: 200px; width: 20%; border-radius: 4px;">

                </div>
                <div id="piuInformation" style="margin-left:auto;margin-right: 25px;">
                    <table class="table table-hover table-borderless fontPIUPdf w-30">
                        <tr>
                            <td>Signature of the Head of PIU</td>
                            <td></td>
                        </tr>
                        <tr>
                            <td>Name and Designation of the Head of PIU</td>
                            <td>
                                <div class="grow-wrap text-wrap rounded p-1 border border-grey" style="border: 1px solid lightgray; color: #666; background-color: #eee; min-width: 350px; min-height: 30px; " data-toggle="tooltip" data-placement="bottom" title="Name and Designation of the Head of PIU">
                                    @Model.GENERAL_INFO_PIU_view.PIU_HEAD_NAME
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td>Mobile Number of the Head of PIU</td>
                            <td><input type="text" value="@Model.GENERAL_INFO_PIU_view.PIU_HEAD_MOBILE_NO" data-toggle="tooltip" data-placement="bottom" title="Mobile Number of the Head of PIU" class="inputShortWidth" disabled>  </td>
                        </tr>
                        <tr>
                            <td>E-mail of the Head of PIU</td>
                            <td><input type="text" value="@Model.GENERAL_INFO_PIU_view.PIU_HEAD_EMAIL" data-toggle="tooltip" data-placement="bottom" title="E-mail of the Head of PIU" class="inputMediumWidth" disabled>  </td>
                        </tr>
                        <tr>
                            <td>Address of the PIU</td>
                            <td>
                                <div class="grow-wrap text-wrap rounded p-1 border border-grey" style="border: 1px solid lightgray; color: #666; background-color: #eee; min-width: 350px; min-height: 30px;" data-toggle="tooltip" data-placement="bottom" title="Address of the PIU">
                                    @Model.GENERAL_INFO_PIU_view.PIU_ADDR
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td>Date</td>
                            <td><input type="text" value="@Model.GENERAL_INFO_PIU_view.PIU_SIGN_DATE" data-toggle="tooltip" data-placement="bottom" title="Date" class="inputShortWidth" disabled>  </td>
                        </tr>
                    </table>
                </div>
            </div>

        </div>  <!-- Main div -->
    </div>
</body>
</html>