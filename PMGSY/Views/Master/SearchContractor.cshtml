@{
    ViewBag.Title = "SearchContractor";
}

<script src="~/Scripts/custom/Generic.js" type="text/javascript"></script>
<script src="~/Scripts/Master/SearchContractor.js" type="text/javascript"></script>
<form id="frmSearchContractor">
<div id="dvhdSearch" class="ui-corner-top ui-widget-header panelheader" style="text-align:left; margin-left:10px;margin-right:10px;margin-top:10px; height:20px;"> 
     <span class="ui-jqgrid-title" style="float:left">&nbsp;Search</span>
         <span  id="spCollapseIconS" class="ui-icon ui-icon-circle-triangle-n" style="float:right"></span>
      </div>
    gfdg
    <div id="dvSearchParameter" class="ui-corner-bottom" style="margin-left: 10px; margin-right: 10px; display: block; border: 1px solid #B5B8C8;">
     <table style="width:96%" class="rowstyle">
            <tr>
               <td style="text-align:right;width:8%;">@Html.Label("State")</td>
               <td style="text-align:left;width:15%">@Html.DropDownList("State", null, new { @title = "State" })</td>               
               <td style="text-align:right;width:8%";>@Html.Label("District")</td>
               <td style="text-align:left;width:15%;">@Html.DropDownList("District", null, new { @title = "District" })</td>                
               <td style="text-align:left;width:8%">@Html.Label("Status")</td>
               <td style="text-align:left;width:15%">@Html.DropDownList("Status", null, "--All--", new { @title = "Status" })</td>
               <td style="text-align:left;width:12%;">@Html.Label("Contractor Name")</td>
               <td style="text-align:left;">@Html.TextBox("Contractor", "", new { @title = "Contractor Name", @id = "txtContractor" })</td>  
               
            </tr>
            <tr>
                <td colspan="8" style="text-align:center"><input type="button" id="loadGrid" name="btnSearch" value="Search" class= "jqueryButton" title="Search Contractor Information" tabindex="4"  /></td>
            </tr>
      </table>
    </div>
</form>