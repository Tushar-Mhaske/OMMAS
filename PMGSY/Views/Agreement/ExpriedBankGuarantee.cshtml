<div id="exfilterForm">
       
<form id="frmFilterbankGuaranteeExpired">
    <div style="margin-left: 100px">
        @Html.ValidationSummary(true)
    </div>
    @Html.AntiForgeryToken()
    <table id="tb" class=" " cellpadding="3px" style="margin-top: 10px; text-align: left;  width: 100%; padding-top: 5px 5px 5px 5px;">
        <tr>
            <td>@Html.LabelFor(model => model.Year)</td>
            <td>@Html.DropDownListFor(model => model.Year, Model.lstYears, new { @id = "ddYears", @title = "Please select year", @tabindex = 1 })
                @Html.ValidationMessageFor(model => model.Year)
            </td>
            @if(PMGSY.Extensions.PMGSYSession.Current.RoleName=="SRRDA"){
            <td>@Html.LabelFor(model => model.District)</td>
            <td>@Html.DropDownListFor(model => model.District, Model.lstDistricts, new { @id = "ddldistricts", @title = "Please select block", @tabindex = 2 })
                @Html.ValidationMessageFor(model => model.District)
            </td>
            }

            <td>@Html.LabelFor(model => model.Block)</td>
            <td>@Html.DropDownListFor(model => model.Block, Model.lstBlocks, new { @id = "ddBlocks", @title = "Please select block", @tabindex = 2 })
                @Html.ValidationMessageFor(model => model.Block)
            </td>

            <td>@Html.LabelFor(model => model.Package)</td>
            <td>@Html.DropDownListFor(model => model.Package, Model.lstPackages, new { @id = "ddPackages", @title = "Please select package", @tabindex = 3 })
                @Html.ValidationMessageFor(model => model.Package)
            </td>
            <td>@Html.LabelFor(model => model.ProposalType)</td>
            <td>@Html.DropDownListFor(model => model.ProposalType, Model.lstProposalType, new { @id = "ddProposalType", @title = "Please select proposal type", @tabindex = 4 })
                @Html.ValidationMessageFor(model => model.ProposalType)
            </td>
        </tr>
        <tr>
            <td>@Html.LabelFor(model => model.AgreementStatus)</td>
            <td>@Html.DropDownListFor(model => model.AgreementStatus, Model.lstStatus, new { @id = "ddAgreementStatus", @title = "Please select Agreement Status", @tabindex = 5 })
                @Html.ValidationMessageFor(model => model.AgreementStatus)
            </td>
            <td>
                @Html.LabelFor(model=>model.ActiveStatus)
            </td>
            <td>
                @Html.DropDownListFor(model => model.ActiveStatus, Model.lstActiveStatus, new { @id = "ddActiveStatus", @tabindex = 5 })
                @Html.ValidationMessageFor(model => model.ActiveStatus)
            </td>
            <td></td>
            <td></td>
        </tr>
        <tr>
            <td colspan="12">
                <center>
                <input type="button" id="btnListExpAgreementsBankGuarantee" name="btnListAgreementsBankGuarantee" value="View Details" class= "jqueryButton" title="List Agreement Details" tabindex="5"  />  
                </center>
            </td>
        </tr>
    </table>
</form>
    </div>
        </div>
    </center>
<center>
