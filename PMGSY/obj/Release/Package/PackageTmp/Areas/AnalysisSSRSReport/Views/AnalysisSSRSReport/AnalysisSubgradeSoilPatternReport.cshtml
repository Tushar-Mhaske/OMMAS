@using MvcReportViewer

@model PMGSY.Areas.AnalysisSSRSReport.Models.AnalysisSubgradeSoilModel


@if (Model.RoadWise == false)
{
    <div id="PropSoilPattern" style="width: 100%; height: 590px; overflow-x: hidden; overflow-y: hidden;">

        @Html.MvcReportViewer("/PMGSYCitizen/PropSoilPattern",
     new
     {
         Level = Model.LevelCode,
         State = Model.Mast_State_Code,
         District = Model.Mast_District_Code,
         Block = Model.Mast_Block_Code,
         Year = Model.FromYear,
         ToYear = Model.ToYear,
         Batch = Model.Batch,
         Collaboration = Model.FundingAgency,
         CBR = Model.CBR,
         Status = Model.Status,
         PMGSY = PMGSY.Extensions.PMGSYSession.Current.PMGSYScheme,
         StateName = Model.StateName,
         DistName = Model.DistName,
         BlockName = Model.BlockName,
         BatchName = Model.BatchName,
         CollaborationName = Model.FundingAgencyName,
         CBRName = "All",
         StatusName = Model.StatusName

     },
     new { Height = 600, Width = 1198, style = "border:none;" })
    </div>
}
else
{
    <div id="PropSoilPatternLVL" style="width: 100%; height: 590px; overflow-x: hidden; overflow-y: hidden;">

        @Html.MvcReportViewer("/PMGSYCitizen/PropSoilPatternLVL",
     new
     {
         Level = Model.LevelCode,
         State = Model.Mast_State_Code,
         District = Model.Mast_District_Code,
         Block = Model.Mast_Block_Code,
         Year = Model.FromYear,
         ToYear = Model.ToYear,
         Batch = Model.Batch,
         Collaboration = Model.FundingAgency,
         CBR = Model.CBR,
         Status = Model.Status,
         PMGSY = PMGSY.Extensions.PMGSYSession.Current.PMGSYScheme,
         StateName = Model.StateName,
         DistName = Model.DistName,
         BlockName = Model.BlockName,
         BatchName = Model.BatchName,
         CollaborationName = Model.FundingAgencyName,
         CBRName = "All",
         StatusName = Model.StatusName

     },
     new { Height = 600, Width = 1198, style = "border:none;" })
    </div>
}
