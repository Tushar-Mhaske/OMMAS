@model PMGSY.Areas.LocationSSRSReports.Models.LocationMasterReports

@if (Layout == null)
{
    Layout = Request.IsAjaxRequest() ? null : "~/Areas/AccountReports/Views/Account/_Layout.cshtml";
}

<script src="~/Scripts/MasterReports/jquery.multi-select.js" type="text/javascript"></script>
<script src="~/Areas/LocationSSRSReports/Scripts/HabitationLayout.js"></script>
<script src="~/Scripts/custom/Generic.js"></script>

@Html.HiddenFor(model => model.BlockName)
@Html.HiddenFor(model => model.Mast_State_Code)
@Html.HiddenFor(model => model.Mast_District_Code)
<div id="dvhdHabitationLayout" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">Habitation</span>
    <span id="spCollapseIconCN" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>
<div id="dvHabitationLayout" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmHabitationLayout">
        <table class="rowstyle" style="width: 100%;">
            <tr>
                <td style="width: 2%;"></td>
                <td style="text-align: left; width: 10%;">
                    @Html.Label("State")
                    <label style="color: red;">*</label>
                </td>
                <td style="width: 15%;">
                    @*@Html.DropDownList("StateList_HabitationDetails", ViewData["STATE"] as IEnumerable<SelectListItem>, new { @title = "State", @tabindex = 1 })*@
                    @Html.DropDownListFor(model => model.State, new SelectList(Model.StateList, "Value", "Text"), new { @id = "StateList_HabitationDetails", @title = "State", @tabindex = 1 })
                    <br />
                    @Html.ValidationMessageFor(m => m.State)
                </td>
                <td style="text-align: left; width: 10%;">
                    @Html.Label("District")
                    <label style="color: red;">*</label>
                </td>
                <td style="width: 15%;">
                    @*@Html.DropDownList("DistrictList_HabitationDetails", ViewData["DISTRICT"] as IEnumerable<SelectListItem>, new { @title = "District", @tabindex = 2 })*@
                    @Html.DropDownListFor(model => model.District, new SelectList(Model.DistrictList, "Value", "Text"), new { @id = "DistrictList_HabitationDetails", @title = "District", @tabindex = 2 })
                    <br />
                    @Html.ValidationMessageFor(m => m.District)
                </td>
                <td style="text-align: left; width: 5%;">
                    @Html.Label("Block")
                    <label style="color: red;">*</label>
                </td>
                <td style="width: 15%;">
                    @*@Html.DropDownList("BlockList_HabitationDetails", ViewData["BLOCK"] as IEnumerable<SelectListItem>, new { @title = "Block", @tabindex = 3 })*@
                    @Html.DropDownListFor(model => model.Block, new SelectList(Model.BlockList, "Value", "Text"), new { @id = "BlockList_HabitationDetails", @title = "Block", @tabindex = 3 })
                    <br />
                    @Html.ValidationMessageFor(m => m.Block)
                </td>
                <td style="text-align: left; width: 5%;">
                    @Html.Label("Village")
                </td>
                <td style="width: 15%;">
                    @*@Html.DropDownList("VillageList_HabitationDetails", ViewData["VILLAGE"] as IEnumerable<SelectListItem>, new { @title = "Village", @tabindex = 4 })*@
                    @Html.DropDownListFor(model => model.Village, new SelectList(Model.VillageList, "Value", "Text"), new { @id = "VillageList_HabitationDetails", @title = "Village", @tabindex = 4 })
                    <br />
                    @Html.ValidationMessageFor(m => m.Village)
                </td>
                <td style="width: 15%;"></td>
            </tr>
            <tr>
                <td style="width: 2%;"></td>
                <td style="text-align: left; width: 10%;">
                    @Html.Label("Habitation Status")
                </td>
                <td style="width: 15%;">
                    @*@Html.DropDownList("HAB_STATUS_HabitationDetails", ViewData["HAB_STATUS"] as IEnumerable<SelectListItem>, new { @title = "Habitation Status", @tabindex = 5 })*@
                    @Html.DropDownListFor(model => model.HabitationStatus, new SelectList(Model.HabitationStatusList, "Value", "Text"), new { @id = "HAB_STATUS_HabitationDetails", @title = "Habitation Status", @tabindex = 5 })
                    <br />
                    @Html.ValidationMessageFor(m => m.HabitationStatus)
                </td>

                <td style="text-align: left; width: 10%;">
                    @Html.Label("Is Schedule5")
                </td>
                <td style="width: 15%;">
                    @*@Html.DropDownList("IS_SCHEDULE5_HabitationDetails", ViewData["IS_SCHEDULE5"] as IEnumerable<SelectListItem>, new { @title = "Is Schedule5", @tabindex = 6 })*@
                    @Html.DropDownListFor(model => model.Schedule, new SelectList(Model.ScheduleList, "Value", "Text"), new { @id = "IS_SCHEDULE5_HabitationDetails", @title = "Is Schedule5", @tabindex = 6 })
                    <br />
                    @Html.ValidationMessageFor(m => m.Schedule)
                </td>
                <td style="text-align: left; width: 10%;">
                    @Html.Label("Census Year")
                </td>
                <td style="width: 15%;">
                    @*@Html.DropDownList("CENSUS_YEAR_HabitationDetails", ViewData["CENSUS_YEAR"] as IEnumerable<SelectListItem>, new { @title = "Census Year", @tabindex = 7 })*@
                    @Html.DropDownListFor(model => model.CensusYear, new SelectList(Model.CensusYearList, "Value", "Text"), new { @id = "CENSUS_YEAR_HabitationDetails", @title = "Census Year", @tabindex = 7 })
                    <br />
                    @Html.ValidationMessageFor(m => m.CensusYear)
                </td>
                <td style="text-align: left; width: 5%;">
                    @Html.Label("Active")
                </td>
                <td style="width: 15%;">
                    @*@Html.DropDownList("ActiveType_HabitationDetails", ViewData["Active_TYPE"] as IEnumerable<SelectListItem>, new { @title = "Active", @tabindex = 8 })*@
                    @Html.DropDownListFor(model => model.ActiveFlag, new SelectList(Model.ActiveFlagList, "Value", "Text"), new { @id = "ActiveType_HabitationDetails", @title = "Active", @tabindex = 8 })
                    <br />
                    @Html.ValidationMessageFor(m => m.ActiveFlag)
                </td>
                <td style="text-align: center; width: 15%; margin-left: 5px;">
                    <input type="button" id="HabitationDetailsButton" value="View" class="jqueryButton" title="View Details" tabindex="9" />
                </td>
            </tr>
        </table>
    </form>
</div>

<div id="dvLoadHabitationReport"></div>

