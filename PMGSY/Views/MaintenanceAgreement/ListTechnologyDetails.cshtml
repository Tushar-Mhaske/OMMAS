@{
    ViewBag.Title = "ListTechnologyDetails";
}


<script src="~/Scripts/custom/Generic.js"></script>
<script src="~/Scripts/MaintenanceAgreement/ListTechnologyDetails.js"></script>

<div id="divError" class="ui-state-error ui-corner-all" style="text-align: left; display: none; width: 90%">
        <span class="ui-icon ui-icon-alert" style="float: left;"></span>                   
        <span></span>
</div>

@Html.Hidden("ProposalCode",ViewBag.ProposalCode as String)
@Html.Hidden("ContractCode",ViewBag.ContractCode as String)
<div class="ui-corner-all ui-state-highlight" style="border:solid 1px black; text-align:left; width:90%">
    
    <table class="rowstyle ui-corner-all" style="width: 100%; border: solid black 1px;">
        <tr>
            <td style="width: 5%"></td>
            <td style="width: 12%; font-weight: bold">Block</td>
            <td style="width: 15%">@ViewBag.BlockName</td>
            <td style="width: 12%; font-weight: bold">Package No.</td>
            <td style="width: 15%">@ViewBag.Package</td>
        </tr>
        <tr>
            <td></td>
            <td style="font-weight: bold">Road Name:</td>
            <td>@ViewBag.RoadName</td>
            <td style="font-weight: bold">Agreement Date</td>
            <td><label id="lblAggDate">@ViewBag.AgreementDate</label></td>
        </tr>
        <tr>
            <td></td>
            <td style="font-weight:bold">Overall Cost(Rs. in Lakh):</td>
            <td><label id="lblOverAllCost">@ViewBag.OverallCost</label></td>
            <td style="font-weight: bold">Sanction Length(in Km)</td>
            <td>@ViewBag.SanctionLength</td>
        </tr>
    </table>
    
</div>

<div id="divAddTechnologyDetails" style="width:90%">
</div>


<div style="margin-top:20px;width:100%">
    <table id="tblistTechnologyDetails"></table>
    <div id="dvpagerTechnology"></div>
</div>


