<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/MasterDataEntry/Master_MappedMPConstituencyBlocks.js")"> </script>

<div id="dvhdSearch_Mapped" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; margin-left: 0px; margin-right: 0px; margin-top: 10px; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;Mapped Blocks </span>

    <span id="spCollapseIconS_Mapped" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>

<div id="dvSearchParameter_Mapped" class="ui-corner-bottom" style="margin-left: 0px; margin-right: 0px; display: block; border: 1px solid #B5B8C8;">

    <form id="frmSearchMappedMPConstituencyBlock">
        @if (!(string.IsNullOrEmpty(ViewBag.EncryptedMPConstituencyCode_Mapped)))
        {
            @Html.Hidden("EncryptedMPConstituencyCode_Mapped", ViewBag.EncryptedMPConstituencyCode_Mapped as String)     
        }
        <table style="width: 100%;" class="rowstyle">

            <tr>

                <td style="text-align: left; width: 20%;">
                    @Html.Label("State Name")

                </td>
                <td>
                    @if (!(string.IsNullOrEmpty(ViewBag.StateName)))
                    {
                        <b>@Html.Label("StateName", ViewBag.StateName as String)   </b>
                    }
                    else
                    {
                        <b>@Html.Label("StateName", "NA")   </b>
                    }

                </td>
                <td style="width: 20%;">
                    @Html.Label("MP Constituency Name")

                </td>
                <td>
                    @if (!(string.IsNullOrEmpty(ViewBag.MPConstituencyName)))
                    {
@* <blink>   </blink> *@
                        <b>@Html.Label("MPConstituencyName", ViewBag.MPConstituencyName as String) </b>
                    }
                    else
                    {
                        <b>@Html.Label("MPConstituencyName", "NA")   </b>
                    }

                </td>

            </tr>



        </table>

    </form>




    <br />

    @* <center>*@
    <div style="width: 95%;">
        <table id="tbMappedMPConstituencyBlockList">
        </table>

        <div id="dvMappedMPConstituencyBlockListPager"></div>
    </div>
    @*</center>*@


    <br />
    <table style="width: 100%;" class="rowstyle">
        <tr>
        </tr>
        <tr>
            <td style="text-align: center">
                <input type="button" id="btnMappedCancel" name="btnMapCancel" value="Back" class="jqueryButton" title="Back" tabindex="300" />
            </td>
        </tr>
        <tr></tr>
    </table>

</div>
