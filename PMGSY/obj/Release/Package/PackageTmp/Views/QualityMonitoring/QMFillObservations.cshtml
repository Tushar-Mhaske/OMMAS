                         <td style="width:10%">                            
                            @Html.DisplayFor(model => model.DISTRICT_NAME)
                        </td>
                        @if (Model.IMS_PROPOSAL_TYPE.Equals("P"))
                        {
                            <td colspan="3">
                                @Html.LabelFor(model => model.IMS_ROAD_NAME)
