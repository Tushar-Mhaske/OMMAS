@using MvcReportViewer
@model PMGSY.Areas.AccountReports.Models.RegisterViewModel
@{
    if (Model.HeadCategoryId==4)
{   <div id="AccStatutoryDeduction" style="width: 100%; height: 600px; overflow-x: hidden; overflow-y: hidden">
     @Html.MvcReportViewer("/PMGSYCitizen/AccStatutoryDeduction",
     new 
     {
         PINTStateCode=Model.StateCode,
         PINTAgencyCode=Model.Collaboration,
         PINTAdminNDCode=Model.AdminCode,
         PINTAccMonth=Model.Month,
         PINTAccYear=Model.Year,
         PCHARfundType=PMGSY.Extensions.PMGSYSession.Current.FundType,
         PINTLvlID=Model.LevelId,
         PINTHeadID=Model.HeadId,
         PINTCollab=Model.FundingAgencyCode,
         SRRDACode=Model.SRRDACode,
         HeadName=Model.HeadName
         
         },
         
     new { Height = 600, Width = 1220, style = "border:none;" })
    </div>
}
     else if (Model.HeadCategoryId == 5)
{   <div id="AccDeposits" style="width: 100%; height: 600px; overflow-x: hidden; overflow-y: hidden">
    @Html.MvcReportViewer("/PMGSYCitizen/AccDeposits",
     new 
     {   PINTStateCode=Model.StateCode,
         PINTAgencyCode=Model.Collaboration,
         PINTAdminNDCode=Model.AdminCode,
         PINTAccMonth=Model.Month,
         PINTAccYear=Model.Year,
         PCHARfundType=PMGSY.Extensions.PMGSYSession.Current.FundType,
         PINTLvlID=Model.LevelId,
         PINTHeadID=Model.HeadId,
         PINTCollab=Model.FundingAgencyCode,
         HeadName=Model.HeadName,
         SRRDACode=Model.SRRDACode
         
         },
         
     new { Height = 600, Width = 1220, style = "border:none;" })
    </div>
    
    }
    else if  (Model.HeadCategoryId == 6)
{   <div id="AccMiscellaneousAdvances" style="width: 100%; height: 600px; overflow-x: hidden; overflow-y: hidden">
    @Html.MvcReportViewer("/PMGSYCitizen/AccMiscellaneousAdvances",
     new 
     {  
         
         PINTStateCode=Model.StateCode,
         PINTAgencyCode=Model.Collaboration,
         PINTAdminNDCode=Model.AdminCode,
         PINTAccMonth=Model.Month,
         PINTAccYear=Model.Year,
         PCHARfundType=PMGSY.Extensions.PMGSYSession.Current.FundType,
         PINTLvlID= Model.LevelId,
         PINTHeadID=Model.HeadId,
         PINTCollab=Model.FundingAgencyCode,
         SRRDACode=Model.SRRDACode,
         HeadName=Model.HeadName
        
         },
         
     new { Height = 600, Width = 1220, style = "border:none;" })
    </div>
    
    }
   
}
