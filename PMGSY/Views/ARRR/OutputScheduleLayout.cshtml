@model PMGSY.Models.ARRR.TaxScheduleViewModel

<script src="~/Scripts/custom/Generic.js"></script>
<script src="~/Scripts/jquery.ui.datepicker.js" type="text/javascript"></script>
<script src="~/Scripts/ARRR/OutputScheduleLayout.js"></script>

<div id="dvOutputSchedule">
    <div id="dvhdOutputSchedule" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
        Add Output<span style="float: right; margin-right: 2Px; margin-top: 2px" id="idFilterDivOutput" class="ui-icon ui-icon-circle-triangle-n"></span>
    </div>
    <form id="frmOutputSchedule">
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)
        @Html.HiddenFor(m => m.User_Action)
        @Html.HiddenFor(m => m.Rate)
        @Html.HiddenFor(m => m.itemCode)
        <table class="rowstyle" style="width: 100%; margin-bottom: 5px; border: 1px solid #B5B8C8;">
             <tr class="output">
                <td style="width: 2%;"></td>
                <td style="width: 5%;">
                    <label>Value</label><label style="color: red;"> *</label></td>
                <td style="width: 5%;">
                    @Html.TextBoxFor(m => m.Ratelmsm, new { @id = "txtRatelOutput", @maxlength = "6", placeholder = "Please enter output value", @tabindex = 3 })
                    <br />
                    @Html.ValidationMessageFor(m => m.Ratelmsm)
                </td>
                <td colspan="2"></td>
            </tr>
             <tr style="text-align: center;">
                <td colspan="5">
                    <input type="button" id="btnSaveOutput" name="Save" value="Save" class="jqueryButton" title="Save" tabindex="4" />
                    <input type="reset" id="btnResetOutput" name="Reset" value="Reset" class="jqueryButton" title="Reset" tabindex="5" />
                </td>
            </tr>
            </table>
    </form>
</div>

