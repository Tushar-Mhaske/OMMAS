@model PMGSY.Models.ARRR.AnalysisOfRates

<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script src="~/Scripts/ARRR/AnalysisofRatesReportLayout.js"></script>

<div id="dvhd" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    Analysis of Rates Report<span style="float: right; margin-right: 2Px; margin-top: 2px" id="idFilterDivI" class="ui-icon ui-icon-circle-triangle-n"></span>
</div>
<form id="frmAnalysisofRatesReportLayout">
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

        <table style="width: 100%;" class="rowstyle">
            <tr>
                <td style="text-align: left; width: 4%;"></td>
                @if (PMGSY.Extensions.PMGSYSession.Current.StateCode == 0)
                {
                  <td style="text-align: left; width: 4%;">
                    @Html.LabelFor(model => model.StateCode)
                </td>
                <td style="width: 25%;">
                  @Html.DropDownListFor(model => model.StateCode, new SelectList(Model.StateList, "Value", "Text"), new { @title = "State", @id = "ddlState", @tabindex = 4, @style = "width:150px" })
                  @Html.ValidationMessageFor(m => m.StateCode)

                </td>
                }
                <td style="width: 5%;">
                    @Html.Label("Chapter")<label style="color: red;"> *</label>
                </td>
                <td colspan="6">
                    @Html.DropDownListFor(model => model.Chapter, new SelectList(Model.ChapterList, "Value", "Text"), new { @title = "Chapter", @id = "ddlChapter", @style = "width:300px;", @tabindex = 1 })
                    <br />
                    @Html.ValidationMessageFor(m => m.Chapter)
                </td>
                 <td style="width: 5%;"></td>
                 <td style="text-align: left; width: 4%;">
                    @Html.LabelFor(model => model.PhaseYear)
                </td>
                <td style="width: 25%;">
                  @Html.DropDownListFor(model => model.PhaseYear, new SelectList(Model.PhaseYearList, "Value", "Text"), new { @title = "Year", @id = "Year_PackageWiseDetails", @tabindex = 4, @style = "width:150px" })
                  @Html.ValidationMessageFor(m => m.PhaseYear)

                </td>
                  <td style="width: 105%;">
                    <input type="button" id="btnView" name="View" value="View" class="jqueryButton" title="View" tabindex="4" />                   
                </td>
            </tr>
        </table>
    </form>
<div id="divLoadAnalysisRatesReport"></div>