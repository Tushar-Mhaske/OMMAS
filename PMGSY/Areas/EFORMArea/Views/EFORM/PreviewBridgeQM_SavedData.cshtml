@{
    ViewBag.Title = "PreviewBridgeQM_SavedData";
}
@model PMGSY.Areas.EFORMArea.QMBridgeViewPdfModel.EFORM_BRIDGE_QM_PREVIEW_MODEL
<head>
    <title>QM Bridge PDF View</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <link href="~/Content/Bootstrap/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/Bootstrap/bootstrap.min.js" type="text/javascript"></script>

    <style>

        /*=========== Common Stylesheet ============*/

        input[type="text"] {
            font-size: 18px;
        }

        /* inputbox width for values like codes,length,dates, */
        .inputCodeWidth {
            width: 100px;
            min-height: 32px;
        }

        .inputShortWidth {
            width: 150px;
            min-height: 32px;
        }

        .inputMediumWidth {
            width: 250px;
            min-height: 32px;
        }

        .inputMidlLargeWidth {
            width: 400px;
            min-height: 32px;
        }

        .inputLargeWidth {
            width: 700px;
            min-height: 32px;
        }

        .SectionUnderline {
            border: 0.5px solid gray;
        }

        .PageBreakLine {
            border: 3px solid gray;
            margin-top: 35px;
            margin-bottom: 35px;
        }

        .table-borderless td {
            border: none;
        }

        .table-bordered thead th {
            vertical-align: top;
            text-align: center;
            background-color: #8c8c8c24;
        }

        .table-hover tbody tr:hover td {
            background-color: #d5bcbc24;
        }

        /*=========== QM Page CSS ===========*/

        .fontQMPDF {
            font-size: 18px;
            font-family: Calibri;
        }
        /* 2. QUALITY ARRANGEMENTS-OBSERVATIONS Tables */
        /*.tab1_2Pt td {
                width: 80%;
            }*/

        /* 3. ATTENTION TO QUALITY */
        .tab1_3Pt td {
            width: 80%;
        }

        .Applicablechecks {
            font-size: large;
            font-style: normal;
            text-align: center;
            background-color: #ffefc6a1;
            padding: 5px;
        }

        /*-------------------- new added --------------------*/
        .QMLeftHeading {
            width: 300px;
            text-align: left;
        }

        .QMleftSrNo {
            width: 5px;
            font-weight: bold;
        }

        /*commeneted for table 6 I. RCC vertical column headers */

        /*.vert_theader p {
                writing-mode: sideways-lr;
                text-align: left;
                padding: 10px 5px 0;
                max-height:350px;
                text-wrap:normal;

                white-space: wrap;
                width: 60px;
                overflow: hidden;
            }*/

    </style>

</head>


<body>
    @* --------------------------------- QM start --------------------------------- *@

    <div class="container-fluid" style="background-color:lightgray; border: 1px solid gray; margin-top: auto;margin-bottom: auto;">
        <div class="container-fluid mt-6 p-4" style="width: 75%; background-color: #fff; margin-top: 50px; margin-bottom: 50px; margin-left: 150px">

            <!-- 1. Headings and QM General Details -->
            <div id="GeneralDetailsBridge_QM">
                <div class="container-fluid">
                    <!-- HEADLINES -->
                    <div class="post-container" id="HeadingsPIU" style="text-align: center;">
                        @*<div class="post-thumb"><img src="~/Areas/EFORMArea/Content/LionAshokaChakra.png" style="width: 10%; margin-top: 40px; " /></div>*@
                        <div class="post-content">
                            <h1> <b> National Rural Infrastructure Development Agency </b> </h1>
                            <h2>Ministry of Rural Development, Government of India</h2>
                            <h4 style="text-align:left;"> EFORM ID <input type="text" value="@Model.BRIDGE_PIU_PREFILLED_VIEW.Eform_Id" class="inputShortWidth" disabled data-toggle="tooltip" data-placement="bottom" title="EFORM ID"> <span style="float:right;color:red">Template Version <input type="text" value="@Model.BRIDGE_QM_GENERAL_DETAILS_VIEW.TemplateVersion" class="inputCodeWidth" data-toggle="tooltip" data-placement="bottom" title="Template Version" disabled></span>  </h4>
                            <h4> <span style="float:right;">Format QB Version 2021.0- Part II </span>  </h4>
                            <br />
                            <h3> <b> Report of National Quality Monitor (NQM)/State Quality Monitor (SQM) </b> </h3>
                            <h3> <u><b> PART II– Observations of NQM/SQM for Ongoing/Completed Bridge Work  </b> <br /><i> (To be filled-up by QM) </i> </u> </h3>
                        </div>
                    </div>

                    <h3><b> 1. GENERAL DETAILS </b></h3>
                    <hr style="border: 0.5px solid gray">
                    <table class="table table-hover table-borderless fontQMPDF">
                        <tbody>
                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> I </td>
                                <td class="QMLeftHeading">Date of inspection </td>
                                <td> <input type="text" value="@Model.BRIDGE_QM_GENERAL_DETAILS_VIEW.INSPECTION_DATE" data-toggle="tooltip" data-placement="bottom" title="Date of inspection" class="inputShortWidth" disabled> </td>
                            </tr>

                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> II </td>
                                <td class="QMLeftHeading">Name of Quality Monitor (QM)</td>
                                <td colspan="2">
                                    <input type="text" value="@Model.BRIDGE_PIU_PREFILLED_VIEW.QM_NAME" data-toggle="tooltip" data-placement="bottom" title="Name of Quality Monitor (QM)" class="inputMidlLargeWidth" disabled>
                                    &emsp;&emsp;
                                    @if (Model.BRIDGE_PIU_PREFILLED_VIEW.QM_TYPE.Equals("I"))
                                    {
                                        <input type="checkbox" checked disabled> <label> NQM &emsp; </label> <input type="checkbox" disabled> <label> SQM </label>
                                    }
                                    else if (Model.BRIDGE_PIU_PREFILLED_VIEW.QM_TYPE.Equals("S"))
                                    {
                                        <input type="checkbox" disabled> <label> NQM &emsp; </label> <input type="checkbox" checked disabled> <label> SQM </label>
                                    }
                                    else
                                    {
                                        <input type="checkbox" disabled> <label> NQM &emsp; </label> <input type="checkbox" disabled> <label> SQM </label>
                                    }
                                </td>
                            </tr>

                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> III </td>
                                <td class="QMLeftHeading">State </td>
                                <td colspan="2">
                                    <input type="text" value="@Model.BRIDGE_PIU_PREFILLED_VIEW.State" data-toggle="tooltip" data-placement="bottom" title="State" disabled>
                                    &emsp;&emsp;
                                    District   <input type="text" value="@Model.BRIDGE_PIU_PREFILLED_VIEW.District" data-toggle="tooltip" data-placement="bottom" title="District" disabled>
                                    &emsp;&emsp;
                                    Block  <input type="text" value="@Model.BRIDGE_PIU_PREFILLED_VIEW.Block" data-toggle="tooltip" data-placement="bottom" title="Block" disabled>
                                </td>
                            </tr>
                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> IV </td>
                                <td class="QMLeftHeading">Name of of bridge & Location </td>
                                <td colspan="2">
                                    <div class="grow-wrap p-2 border border-grey text-wrap fontQMPDF" style="border: 1px solid lightgray; color: #666; background-color: #eee; min-height: 30px; max-width: 780px;" data-toggle="tooltip" data-placement="bottom" title="Name of of bridge & Location">
                                        <p class="fontQMPDF px-2">
                                            @Model.BRIDGE_PIU_PREFILLED_VIEW.RoadName
                                        </p>
                                    </div>
                                </td>
                            </tr>

                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> V </td>
                                <td class="QMLeftHeading">Package Number </td>
                                <td colspan="2"> <input type="text" value=" @Model.BRIDGE_PIU_PREFILLED_VIEW.PkgNumber" data-toggle="tooltip" data-placement="bottom" title="Package Number" class="inputMediumWidth" disabled> </td>
                            </tr>
                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> VI </td>
                                <td class="QMLeftHeading"> Physical status of work <br /> (check✓ any one box) </td>
                                <td colspan="2">
                                    @if (Model.BRIDGE_PIU_PREFILLED_VIEW.WORK_STATUS.Contains("P") || Model.BRIDGE_PIU_PREFILLED_VIEW.WORK_STATUS.Contains("M"))
                                    {
                                        <input type="checkbox" checked disabled /> <label> Ongoing  &emsp; &emsp;</label>
                                        <input type="checkbox" disabled /> <label> Completed </label>
                                    }
                                    else if (Model.BRIDGE_PIU_PREFILLED_VIEW.WORK_STATUS.Contains("C"))
                                    {
                                        <input type="checkbox" disabled /> <label> Ongoing  &emsp; &emsp;</label>
                                        <input type="checkbox" checked disabled /> <label> Completed </label>
                                    }
                                    else
                                    {
                                        <input type="checkbox" disabled /> <label> Ongoing  &emsp; &emsp;</label>
                                        <input type="checkbox" disabled /> <label> Completed </label>
                                    }

                                </td>
                            </tr>


                            <tr>
                                <td rowspan="2" class="QMleftSrNo" style="text-align:center"> VII </td>
                                <td rowspan="2" class="QMLeftHeading"> Present status of work <br /> (check ✓ boxes as applicable) </td>
                                <td colspan="2">
                                    @if (Model.BRIDGE_QM_GENERAL_DETAILS_VIEW.IS_WORK_STAT_LAYOUT.Equals("Y"))
                                    {
                                        <input type="checkbox" checked disabled /> <label> Layout  &emsp; &emsp; </label>
                                    }
                                    else
                                    {
                                        <input type="checkbox" disabled /> <label> Layout &emsp; &emsp; </label>
                                    }
                                    @if (Model.BRIDGE_QM_GENERAL_DETAILS_VIEW.IS_WORK_STAT_FOUNDATION.Equals("Y"))
                                    {
                                        <input type="checkbox" checked disabled /> <label> Foundation &emsp; &emsp; </label>
                                    }
                                    else
                                    {
                                        <input type="checkbox" disabled /> <label> Foundation &emsp; &emsp; </label>
                                    }
                                    @if (Model.BRIDGE_QM_GENERAL_DETAILS_VIEW.IS_WORK_STAT_SUBSTRUCTURE.Equals("Y"))
                                    {
                                        <input type="checkbox" checked disabled /> <label> Substructure </label>
                                    }
                                    else
                                    {
                                        <input type="checkbox" disabled /> <label> Substructure </label>
                                    }


                                </td>
                            <tr>
                                <td colspan="2">
                                    @if (Model.BRIDGE_QM_GENERAL_DETAILS_VIEW.IS_WORK_STAT_SUPERSTRUCTURE.Equals("Y"))
                                    {
                                        <input type="checkbox" checked disabled /> <label> Superstructure &emsp; &emsp;</label>
                                    }
                                    else
                                    {
                                        <input type="checkbox" disabled /> <label> Superstructure &emsp; &emsp;</label>
                                    }
                                    @if (Model.BRIDGE_QM_GENERAL_DETAILS_VIEW.IS_WORK_STAT_PROT_WORK.Equals("Y"))
                                    {
                                        <input type="checkbox" checked disabled /> <label> Protection work  &emsp; &emsp;</label>
                                    }
                                    else
                                    {
                                        <input type="checkbox" disabled /> <label> Protection work  &emsp; &emsp;</label>
                                    }
                                    @if (Model.BRIDGE_QM_GENERAL_DETAILS_VIEW.IS_WORK_STAT_APPROACH.Equals("Y"))
                                    {
                                        <input type="checkbox" checked disabled /> <label> Approaches &emsp; &emsp;</label>
                                    }
                                    else
                                    {
                                        <input type="checkbox" disabled /> <label> Approaches &emsp; &emsp;</label>
                                    }
                                    @if (Model.BRIDGE_QM_GENERAL_DETAILS_VIEW.IS_WORK_STAT_FINISHING_STAGE.Equals("Y"))
                                    {
                                        <input type="checkbox" checked disabled /> <label> Finishing stage </label>
                                    }
                                    else
                                    {
                                        <input type="checkbox" disabled /> <label> Finishing stage </label>
                                    }


                                </td>
                            </tr>
                            </tr>

                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> VIII </td>
                                <td colspan="2" class="QMLeftHeading"> Photographic or video-graphic records (optional) of all hidden items like reinforcements of foundations/ substructures/ super structures etc. are kept by PIU, till Defect liability period is over for bridge for any futurereference or not: (check ✓ any one box) </td>
                            </tr>
                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> </td>
                                <td colspan="3">
                                    @if (Model.BRIDGE_QM_GENERAL_DETAILS_VIEW.IS_VIDEO_RECORDS.Equals("Yes"))
                                    {
                                        <input type="checkbox" checked disabled /> <label> Yes  &emsp; &emsp;</label>
                                        <input type="checkbox" disabled /> <label> No </label>
                                    }
                                    else if (Model.BRIDGE_QM_GENERAL_DETAILS_VIEW.IS_VIDEO_RECORDS.Equals("No"))
                                    {
                                        <input type="checkbox" disabled /> <label> Yes  &emsp; &emsp;</label>
                                        <input type="checkbox" checked disabled /> <label> No </label>
                                    }
                                    else
                                    {
                                        <input type="checkbox" disabled /> <label> Yes  &emsp; &emsp;</label>
                                        <input type="checkbox" disabled /> <label> No </label>
                                    }

                                </td>
                            </tr>

                        </tbody>
                    </table>
                </div>
            </div> <!-- Close Headings and General Details -->
            <br>


            <!-- 2. QUALITY ARRANGEMENTS-OBSERVATIONS -->
        @if (Model.ApplicableCheck.isApplicable_2.Equals("Y"))
        {   
               <hr class="PageBreakLine">
            <div id="QualityArrangementsObservations">
                <div class="container-fluid">
                    <h3><b>2. QUALITY ARRANGEMENTS-OBSERVATIONS (in case of ongoing works only) </b></h3>
                    <h4>(Geo-tagged photograph of the laboratory showing the available equipment’s to be uploaded)</h4>

                        <span class="Applicablechecks"> <input type="checkbox" checked disabled /> If Applicable (Check the box) </span>
                    
                    <hr class="SectionUnderline">
                    @if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW != null)
                    {
                        <table class="table table-hover table-borderless fontQMPDF ">
                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> I </td>
                                <td> Whether field laboratory established:(check✓any one box)</td>
                                @if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_FIELD_LAB_ESTD.Equals("Y"))
                                {
                                    <td> <input type="checkbox" checked disabled> Yes  &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_FIELD_LAB_ESTD.Equals("N"))
                                {
                                    <td> <input type="checkbox" disabled> Yes  &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" checked disabled> No </td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_FIELD_LAB_ESTD.Equals("P"))
                                {
                                    <td> <input type="checkbox" disabled> Yes  &emsp13; <input type="checkbox" checked disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                }
                                else
                                {
                                    <td> <input type="checkbox" disabled> Yes  &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                }

                            </tr>
                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> II </td>
                                <td> Whether location of field laboratory is same as indicated by PIU in format part-I: (check✓ any one box) </td>
                                @if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_LAB_LOC_SAME.Equals("Y"))
                                {
                                    <td> <input type="checkbox" checked disabled> Yes &emsp13; <input type="checkbox" disabled> No </td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_LAB_LOC_SAME.Equals("N"))
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" checked disabled> No </td>
                                }
                                else
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> No </td>
                                }


                            </tr>
                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> III </td>
                                <td> Whether necessary equipments as indicated in part-1 are actually available: (check ✓any one box)</td>
                                @if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_EQUIP_AVAILABLE.Equals("Y"))
                                {
                                    <td> <input type="checkbox" checked disabled> Yes  &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_EQUIP_AVAILABLE.Equals("N"))
                                {
                                    <td> <input type="checkbox" disabled> Yes  &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" checked disabled> No </td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_EQUIP_AVAILABLE.Equals("P"))
                                {
                                    <td> <input type="checkbox" disabled> Yes  &emsp13; <input type="checkbox" checked disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                }
                                else
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                }

                            </tr>
                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> IV </td>
                                <td> Whether equipment’s have been used: (check✓ any one box) </td>
                                @if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_EQUIP_USED.Equals("Y"))
                                {
                                    <td> <input type="checkbox" checked disabled> Yes  &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_EQUIP_USED.Equals("N"))
                                {
                                    <td> <input type="checkbox" disabled> Yes  &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" checked disabled> No </td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_EQUIP_USED.Equals("P"))
                                {
                                    <td> <input type="checkbox" disabled> Yes  &emsp13; <input type="checkbox" checked disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                }
                                else
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                }

                            </tr>
                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> V </td>
                                <td> If all necessary equipments are not available, whether you have verified them with the list of deficient equipment’s provided by PIU in format Part-I:  (check✓ any one box)</td>
                                @if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_EQUIP_AVAIL_VERIFY.Equals("Y"))
                                {
                                    <td> <input type="checkbox" checked disabled> Yes &emsp13; <input type="checkbox" disabled> No &emsp13; <input type="checkbox" disabled> NA</td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_EQUIP_AVAIL_VERIFY.Equals("N"))
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" checked disabled> No &emsp13; <input type="checkbox" disabled> NA</td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_EQUIP_AVAIL_VERIFY.Equals("NA"))
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> No &emsp13; <input type="checkbox" checked disabled> NA</td>
                                }
                                else
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> No &emsp13; <input type="checkbox" disabled> NA</td>
                                }

                            </tr>
                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> VI </td>
                                <td> Calibration of equipment done or not: (check✓ any one box)</td>
                                @if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_CALIBRATION_OF_EQUIPMENT.Equals("Y"))
                                {
                                    <td> <input type="checkbox" checked disabled> Yes &emsp13; <input type="checkbox" disabled> No </td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_CALIBRATION_OF_EQUIPMENT.Equals("N"))
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" checked disabled> No </td>
                                }
                                else
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> No </td>
                                }


                            </tr>
                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> VII </td>
                                <td> Whether contractor’s engineer as per Part-I of this format, is available at site: (check✓ any one box)</td>
                                @if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_ENGG_AVAILABLE.Equals("Y"))
                                {
                                    <td> <input type="checkbox" checked disabled> Yes &emsp13; <input type="checkbox" disabled> No </td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_ENGG_AVAILABLE.Equals("N"))
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" checked disabled> No </td>
                                }
                                else
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> No </td>
                                }
                            </tr>
                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> VIII </td>
                                <td> If contractor’s engineer as per Part-I of this report is not available, whether you are satisfied with alternative arrangement made to maintain QCR-I: (check✓ any one box)</td>
                                @if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_ALT_ENGG_ARR_SATISFIED.Equals("Y"))
                                {
                                    <td> <input type="checkbox" checked disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No &emsp13; <input type="checkbox" disabled> NA </td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_ALT_ENGG_ARR_SATISFIED.Equals("N"))
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" checked disabled> No &emsp13; <input type="checkbox" disabled> NA </td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_ALT_ENGG_ARR_SATISFIED.Equals("NA"))
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No &emsp13; <input type="checkbox" checked disabled> NA </td>
                                }
                                else if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_ALT_ENGG_ARR_SATISFIED.Equals("P"))
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" checked disabled> Partly &emsp13; <input type="checkbox" disabled> No &emsp13; <input type="checkbox" disabled> NA </td>
                                }
                                else
                                {
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No &emsp13; <input type="checkbox" disabled> NA </td>
                                }

                            </tr>
                            <tr>
                                <td class="QMleftSrNo" style="text-align:center"> IX </td>
                                <td> Whether lab technician and other staff is available for testing in the laboratory (check✓ any one box)</td>
                                @if (Model.BRIDGE_QM_ARRANGEMENT_OBS_DETAIL_VIEW.IS_LAB_TECH_AVAILABLE.Equals("Y"))
                                {
                                    <td> <input type="checkbox" checked disabled> Yes &emsp13; <input type="checkbox" disabled> No </td>
                                }
