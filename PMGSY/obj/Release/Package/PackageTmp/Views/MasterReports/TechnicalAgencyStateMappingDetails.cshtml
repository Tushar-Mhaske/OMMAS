              <td style="text-align: left; width: 5%;">
                @Html.Label("Active")
            </td>
            <td style="width: 15%;">
                @Html.DropDownList("Active_TechnicalAgencyStateMapDetails", ViewData["ACTIVE"] as IEnumerable<SelectListItem>, new { @title = "Active", @tabindex = 3 })
            </td>
            <td style="text-align: center; width: 20%; margin-left: 5px;">
                <input type="button" id="TechnicalAgencyStateMapDetailsButton" value="View" class="jqueryButton" title="View Details" tabindex="4" />
