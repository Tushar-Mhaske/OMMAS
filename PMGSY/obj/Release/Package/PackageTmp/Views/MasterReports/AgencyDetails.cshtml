@{
    ViewBag.Title = "_AgencyDetails";
}
<div style="width: 100%">
    <table class="ui-state-default ui-corner-all" style="width: 100%;">
        <tr>
            <td style="width: 6%;"></td>
            <td style="text-align: left; width: 7%;">
                @Html.Label("Agency Type")
            </td>
            <td style="width: 15%;">
                @Html.DropDownList("MAST_AGENCY_TYPE", ViewData["MAST_AGENCY_TYPE"] as IEnumerable<SelectListItem>, new { @title = "Agency Type", @tabindex = 1 })
            </td>
            <td style="text-align: center; width: 10%; margin-left: 5px;">
                <input type="button" id="AgencyDetailsButton" value="View" class="jqueryButton" title="View Details" tabindex="2" />
            </td>
            <td style="width: 60%"></td>
        </tr>
    </table>
</div>
