@{
    ViewBag.Title = "DSCFileVerifyLayout";
}

<script src="~/Scripts/custom/Generic.js" type="text/javascript"></script>
<script src="~/Areas/REAT/Scripts/DSCFileVerify.js"></script>
</br>
<div id="dvhdDSCFileVerifyLayout" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">@*margin-left:0px;margin-right:0px;margin-top:0px; *@
    <span class="ui-jqgrid-title" style="float: left">&nbsp;DSC File Verification</span>
</div>
<div id="dvDSCFileVerifyLayout" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmDSCFileVerify">
        @Html.ValidationSummary(true)
        @Html.AntiForgeryToken()
        
        <table style="margin-top: 0px; margin-bottom: 0px; width:100%;" class="rowstyle">
            <tr>
                <td style="width:10%">
                   
                </td>
                <td>
                    @Html.Label("Browse File")<span style="color:red">*</span>
                </td>
                <td style="width:5%">
                    @Html.TextBox("File","", new { @type="file",@id ="DSCFile", multiple="multiple", @tabindex = 1 })
                    <br />
                    
                </td>
                <td>
                    <input type="button" class="jqueryButton" value="Verify File" title="Verify File" id="btnVerifyDSCFile" name ="Verify File" tabindex="3"/>
                </td>
                 <td style="width:40%">
                  
                </td>
            </tr>
        </table>
    </form>
</div>




