@{
    ViewBag.Title = "ExecutionItemDetails";
}
<div style="width: 100%">
    <table class="ui-state-default ui-corner-all" style="width: 100%;">
        <tr>
            <td style="width: 6%;"></td>
            <td style="text-align: left; width: 5%;">
                @Html.Label("Type")
            </td>
             <td style="width: 15%;">
                @Html.DropDownList("MAST_HEAD_TYPE", ViewData["MAST_HEAD_TYPE"] as IEnumerable<SelectListItem>, new { @title = "Type", @tabindex = 1 })
            </td>
           <td style="text-align: center; width: 5%; margin-left: 5px;">
                <input type="button" id="ExecutionItemDetailsButton" value="View" class="jqueryButton" title="View Details" tabindex="2" />
            </td>
            <td style="width: 60%;"></td>
        </tr>
        
    </table>
</div>
