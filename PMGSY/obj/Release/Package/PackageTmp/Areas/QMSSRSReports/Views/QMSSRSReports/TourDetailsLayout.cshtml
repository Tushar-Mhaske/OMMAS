@model PMGSY.Areas.QMSSRSReports.Models.QualityReportsViewModel
@{
    ViewBag.Title = "TourDetailsLayout";
}
<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script src="~/Areas/QMSSRSReports/Scripts/TourDetails.js"></script>

<div id="dvhdNqmTourDetails" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;NQM Tour program Report</span>
</div>
<div id="loadFilters" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmNqmTourDetails">
        @Html.ValidationSummary(true)

        <table style="width: 100%;" class="rowstyle">
             <tr>
            <td style="width: 2%; vertical-align: top"></td>
                <td style="text-align: left; width: 5%; vertical-align: top"></td>
            <td style="width: 10%; vertical-align: top; ">
                    @Html.Label("Schedule Month :")
                </td>
                <td style="width: 10%; vertical-align: top">
                  @Html.DropDownListFor(model => model.Month, new SelectList(Model.MonthList, "Value", "Text"), new { @title = "Month", @id = "ddlMonth", @style = "width:150px" })
                    <br />
                    @Html.ValidationMessageFor(m => m.Month)
                </td>
                 <td style="width: 10%; vertical-align: top; ">
                    @Html.Label("Schedule Year :")
                </td>
                <td style="width: 10%; vertical-align: top">
                  @Html.DropDownListFor(model => model.Year, new SelectList(Model.YearList, "Value", "Text"), new { @title = "Year", @id = "ddlYear", @style = "width:150px" })
                    <br />
                    @Html.ValidationMessageFor(m => m.Year)
                </td>
                 
              <td style="width: 5%; vertical-align: top; text-align:right">
                    <input type="button" class="jqueryButton"  value="View" id="btnViewReport" title="View" />
                </td>
                <td style="width: 5%; vertical-align: top"></td>
            </tr>
        </table>
    </form>
</div>
<div id="loadReport" class="divLoadReport"></div>


