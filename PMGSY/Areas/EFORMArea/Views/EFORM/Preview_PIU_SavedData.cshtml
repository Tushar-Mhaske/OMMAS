@model PMGSY.Areas.EFORMArea.Model.EFORM_PIU_VIEWMODEL

<!DOCTYPE html>
<html lang="en">

<head>
    <title>PIU PDF View</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link href="~/Content/Bootstrap/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/Bootstrap/bootstrap.min.js"></script>
</head>
<style>

    /*=========== Common Stylesheet ============*/

    input[type="text"] {
        font-size: 22px;
    }

    /* inputbox width for values like codes,length,dates, */
    .inputCodeWidth {
        width: 100px;
        min-height: 32px;
    }

    .inputShortWidth {
        width: 150px;
        min-height: 32px;
    }

    .inputMediumWidth {
        width: 250px;
        min-height: 32px;
    }

    .inputMidlLargeWidth {
        width: 400px;
        min-height: 32px;
    }

    .inputLargeWidth {
        width: 700px;
        min-height: 32px;
    }

    .SectionUnderline {
        border: 0.5px solid gray;
    }

    .PageBreakLine {
        border: 3px solid gray;
        margin-top: 35px;
        margin-bottom: 35px;
    }

    .table-borderless td {
        border: none;
    }

    .table-bordered thead th {
        vertical-align: top;
        text-align: center;
        background-color: #8c8c8c24;
    }

    .table-hover tbody tr:hover td {
        background-color: #d5bcbc24;        
    }

    .fontPIUPdf {
        font-size: 22px;
        font-family: Calibri;
    }

    .detailtablesPiuLeftHeading {
        width: 300px;
    }

</style>
<body>
    <div class="container-fluid" style="background-color:lightgray; border: 1px solid gray; margin-top: auto;margin-bottom: auto;">
        <div class="container-fluid mt-6 p-4" style="width: 90%; background-color: #fff; margin-top: 50px; margin-bottom: 50px; margin-left: 60px">

            <!-- 1. Headings and General Details -->
            <div id="GeneralDetails_PIU">
                <div class="container-fluid">
                    <!-- HEADLINES -->
                    <div id="HeadingsPIU" style="text-align: center;">
                        <h1> <b> National Rural Infrastructure Development Agency </b> </h1>
                        <h2>Ministry of Rural Development, Government of India</h2>
                        <h4 style="text-align:left;"> FORM ID <input type="text" value="@Model.Prefilled_Details.EFORM_ID" class="inputShortWidth" disabled> <span style="float:right;">Format QR Version 2021.0- Part I </span>   </h4>
                        <h3> <b> Format for information to QM for Inspection of Ongoing/Completed Work </b> </h3>
                        <h3> <u><b> PART I- Work Information  </b> <i> (To be filled-up by PIU) </i> </u> </h3>
                    </div>

                    <h3><b> 1. GENERAL DETAILS </b></h3>
                    <hr style="border: 0.5px solid gray">
                    <div class="container-fluid">
                        <table class="table table-hover table-borderless fontPIUPdf">
                            <tr>
                                <td class="detailtablesPiuLeftHeading"> I. Work is </td>
                                <td colspan="2">
                                    @if (Model.Prefilled_Details.PHYSICAL_WORK_STATUS.Contains("P") || Model.Prefilled_Details.PHYSICAL_WORK_STATUS.Contains("M"))
                                    {
                                        <input type="checkbox" checked disabled /> <label> ONGOING  &emsp; &emsp;</label>
                                        <input type="checkbox" disabled /> <label>COMPLETED </label>
                                    }
                                    else if (Model.Prefilled_Details.PHYSICAL_WORK_STATUS.Contains("C"))
                                    {
                                        <input type="checkbox" disabled /> <label> ONGOING  &emsp; &emsp;</label>
                                        <input type="checkbox" checked disabled /> <label> COMPLETED </label>
                                    }
                                    else
                                    {
                                        <input type="checkbox" disabled /> <label> ONGOING &emsp; &emsp;</label>
                                        <input type="checkbox" disabled /> <label> COMPLETED </label>
                                    }
                                </td>
                            </tr>
                            <tr>
                                <td class="detailtablesPiuLeftHeading"> II. Current stage of work </td>
                                <td colspan="2">
                                    @if (Model.Prefilled_Details.CURRENT_STAGE == null)
                                    {
                                        <input type="checkbox" disabled /> <label> Stage-1 &emsp;&emsp; </label>
                                        <input type="checkbox" disabled /> <label> Stage-2 &emsp;&emsp;</label>
                                        <input type="checkbox" disabled /> <label> Complete </label>
                                    }
                                    else if (Model.Prefilled_Details.CURRENT_STAGE == "1")
                                    {
                                        <input type="checkbox" checked disabled /> <label> Stage-1 &emsp;&emsp; </label>
                                        <input type="checkbox" disabled /> <label> Stage-2 &emsp;&emsp;</label>
                                        <input type="checkbox" disabled /> <label> Complete </label>
                                    }
                                    else if (Model.Prefilled_Details.CURRENT_STAGE == "2")
                                    {
                                        <input type="checkbox" disabled /> <label> Stage-1 &emsp;&emsp; </label>
                                        <input type="checkbox" checked disabled /> <label> Stage-2 &emsp;&emsp; </label>
                                        <input type="checkbox" disabled /> <label> Complete </label>
                                    }
                                    else if (Model.Prefilled_Details.CURRENT_STAGE == "3")
                                    {
                                        <input type="checkbox" disabled /> <label> Stage-1  &emsp;&emsp;</label>
                                        <input type="checkbox" disabled /> <label> Stage-2 &emsp;&emsp;</label>
                                        <input type="checkbox" checked disabled /> <label> Complete </label>
                                    }
                                </td>
                            </tr>

                            <tr>
                                <td class="detailtablesPiuLeftHeading"> IV. Name of Quality Monitor (QM)</td>
                                <td colspan="2">
                                    <input type="text" value="@Model.Prefilled_Details.QM_NAME" data-toggle="tooltip" data-placement="bottom" title="Name of Quality Monitor (QM)" class="inputMidlLargeWidth" disabled>
                                    &emsp;&emsp;

                                    @if (Model.Prefilled_Details.QM_TYPE.Equals("I"))
                                    {
                                        <input type="checkbox" checked disabled> <label> NQM &emsp; </label> <input type="checkbox" disabled> <label> SQM </label>
                                    }
                                    else if (Model.Prefilled_Details.QM_TYPE.Equals("S"))
                                    {
                                        <input type="checkbox" disabled> <label> NQM &emsp; </label> <input type="checkbox" checked disabled> <label> SQM </label>
                                    }
                                    else
                                    {
                                        <input type="checkbox" disabled> <label> NQM &emsp; </label> <input type="checkbox" disabled> <label> SQM </label>
                                    }
                                </td>
                            </tr>
                            <tr>
                                <td class="detailtablesPiuLeftHeading"> V. State </td>
                                <td colspan="2">
                                    <input type="text" value="@Model.Prefilled_Details.STATE" data-toggle="tooltip" data-placement="bottom" title="State" disabled>
                                    &emsp;&emsp;
                                    District   <input type="text" value="@Model.Prefilled_Details.DISTRICT" data-toggle="tooltip" data-placement="bottom" title="District" disabled>
                                    &emsp;&emsp;
                                    Block  <input type="text" value="@Model.Prefilled_Details.BLOCK" data-toggle="tooltip" data-placement="bottom" title="Block" disabled>
                                </td>
                            </tr>
                            <tr>
                                <td class="detailtablesPiuLeftHeading"> VI. Name of Road </td>
                                <td colspan="2">
                                    <div class="grow-wrap p-2 border border-grey text-wrap fontPIUPdf" style="border: 1px solid lightgray; color: #666; background-color: #eee;min-height:30px;" data-toggle="tooltip" data-placement="bottom" title="Name of Road">
                                        <p class="fontPIUPdf px-2">
                                            @Model.Prefilled_Details.ROAD_NAME
                                        </p>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td class="detailtablesPiuLeftHeading"> VII. Package Number </td>
                                <td colspan="2"> <input type="text" value="@Model.Prefilled_Details.PACKAGE_ID" data-toggle="tooltip" data-placement="bottom" title="Package Number" class="inputMediumWidth" disabled> </td>
                            </tr>
                            <tr>
                                <td class="detailtablesPiuLeftHeading" rowspan="2"> VIII. Sanctioned Length </td>
                                <td rowspan="2">
                                    <input type="text" value="@Model.Prefilled_Details.SANCTION_LENGTH" data-toggle="tooltip" data-placement="bottom" title="Sanctioned Length" class="inputShortWidth" disabled> km

                                <td> Flexible pavement &emsp;&emsp;&emsp;&emsp; <input type="text" value="@Model.GENERAL_INFO_PIU_view.SAN_FLEX_PAVEMENT" data-toggle="tooltip" data-placement="bottom" title="Flexible pavement" class="inputShortWidth" disabled> km</td>

                            <tr>
                                <td> Rigid/Semi-Rigid pavement  <input type="text" value="@Model.GENERAL_INFO_PIU_view.SAN_RIGID_PAVEMENT" data-toggle="tooltip" data-placement="bottom" title="Rigid/Semi-Rigid pavement" class="inputShortWidth" disabled> km </td>
                            </tr>
                            </td>
                            </tr>
                            <tr>
                                <td class="detailtablesPiuLeftHeading" rowspan="2"> IX. Executed Length <br> <b><i>(in case of completed works only)</i></b></td>
                                <td rowspan="2">
                                    <input type="text" value="@Model.Prefilled_Details.EXECUTED_LENGTH" data-toggle="tooltip" data-placement="bottom" title="Executed Length" class="inputShortWidth" disabled> km

                                <td> Flexible pavement &emsp;&emsp;&emsp;&emsp; <input type="text" value="@Model.GENERAL_INFO_PIU_view.EXEC_FLEX_PAVEMENT" data-toggle="tooltip" data-placement="bottom" title="Flexible pavement" class="inputShortWidth" disabled> km</td>

                            <tr>
                                <td> Rigid/Semi-Rigid pavement  <input type="text" value="@Model.GENERAL_INFO_PIU_view.EXEC_RIGID_PAVEMENT" data-toggle="tooltip" data-placement="bottom" title="Rigid/Semi-Rigid pavement" class="inputShortWidth" disabled> km </td>
                            </tr>
                            </td>
                            </tr>
                            <tr>
                                <td class="detailtablesPiuLeftHeading"> X. Reasons for deviation <i>(if any)</i> </td>
                                <td colspan="2">
                                    <div class="grow-wrap p-2 border border-grey" style="background-color: #cccccc3d; color: #333333cf;min-height:30px;" data-toggle="tooltip" data-placement="bottom" title="Reasons for deviation">
                                        @Model.GENERAL_INFO_PIU_view.DEVIATION_REASON
                                    </div>
                                </td>
                            </tr>
                            @if (Model.NewTechnology_PIU_List != null)
                            {
                                for (int row = 0; row < Model.NewTechnology_PIU_List.Count; row++)
                                {
                                    if (row == 0 && @Model.NewTechnology_PIU_List[row].MAST_TECH_NAME != null && @Model.NewTechnology_PIU_List[row].TECH_ID != 1)
                                    {
                                        <tr>
                                            <td class="detailtablesPiuLeftHeading"> XI. Name of new technology  <i> (if used) </i> </td>

                                            <td>
                                                <input type="checkbox" checked disabled />
                                                <div class="grow-wrap p-2 border border-grey text-wrap" style="float: right;min-width:250px;max-width: 250px;min-height:30px; background-color: #cccccc3d; color: #333333cf;" data-toggle="tooltip" data-placement="bottom" title="New Technology 1">
                                                    @Model.NewTechnology_PIU_List[row].MAST_TECH_NAME
                                                </div>
                                            </td>

                                            <td colspan="2">
                                                <b> RD From </b> <input type="text" value="@Model.NewTechnology_PIU_List[row].RD_FROM" data-toggle="tooltip" data-placement="bottom" title="RD From 1" class="inputShortWidth" disabled /> km
                                                &emsp;
                                                <b> To </b> <input type="text" value="@Model.NewTechnology_PIU_List[row].RD_TO" data-toggle="tooltip" data-placement="bottom" title="RD To 1" class="inputShortWidth" disabled /> km
                                            </td>
                                        </tr>
                                    }
                                    else if (row == 1 && @Model.NewTechnology_PIU_List[row].MAST_TECH_NAME != null && @Model.NewTechnology_PIU_List[row].TECH_ID != 1)
                                    {
                                        <tr>
                                            <td></td>
                                            <td>
                                                <input type="checkbox" checked disabled />
                                                <div class="grow-wrap p-2 border border-grey text-wrap" style="float: right;min-width:250px;max-width: 250px;min-height:30px; background-color: #cccccc3d; color: #333333cf;" data-toggle="tooltip" data-placement="bottom" title="New Technology 2">
                                                    @Model.NewTechnology_PIU_List[row].MAST_TECH_NAME
                                                </div>
                                            </td>
                                            <td colspan="2">
                                                <b> RD From </b> <input type="text" value="@Model.NewTechnology_PIU_List[row].RD_FROM" data-toggle="tooltip" data-placement="bottom" title="RD From 2" class="inputShortWidth" disabled /> km
                                                &emsp;
                                                <b> To </b> <input type="text" value="@Model.NewTechnology_PIU_List[row].RD_TO" data-toggle="tooltip" data-placement="bottom" title="RD To 2" class="inputShortWidth" disabled /> km
                                            </td>
                                        </tr>
                                    }
                                    else if (row == 2 && @Model.NewTechnology_PIU_List[row].MAST_TECH_NAME != null && @Model.NewTechnology_PIU_List[row].TECH_ID != 1)
                                    {
                                        <tr>
                                            <td></td>
                                            <td>
                                                <input type="checkbox" checked disabled />
                                                <div class="grow-wrap p-2 border border-grey text-wrap" style="float: right;min-width:250px;max-width: 250px;min-height:30px; background-color: #cccccc3d; color: #333333cf;" data-toggle="tooltip" data-placement="bottom" title="New Technology 3">
                                                    @Model.NewTechnology_PIU_List[row].MAST_TECH_NAME
                                                </div>
                                            </td>
                                            <td colspan="2">
                                                <b> RD From </b> <input type="text" value="@Model.NewTechnology_PIU_List[row].RD_FROM" data-toggle="tooltip" data-placement="bottom" title="RD From 3" class="inputShortWidth" disabled /> km
                                                &emsp;
                                                <b> To </b> <input type="text" value="@Model.NewTechnology_PIU_List[row].RD_TO" data-toggle="tooltip" data-placement="bottom" title="RD To 3" class="inputShortWidth" disabled /> km
                                            </td>
                                        </tr>
                                    }
                                    else
                                    {
                                        <tr>
                                            <td></td>
                                            <td>
                                                <input type="checkbox" disabled />
                                                <div class="grow-wrap p-2 border border-grey text-wrap" style="float: right;min-width:250px;max-width: 250px;min-height:30px; background-color: #cccccc3d; color: #333333cf;" data-toggle="tooltip" data-placement="bottom" title="New Technology">
                                                    @Model.NewTechnology_PIU_List[row].MAST_TECH_NAME
                                                </div>
                                            </td>
                                            <td colspan="2">
                                                <b> RD From </b> <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="RD From" class="inputShortWidth" disabled /> km
                                                &emsp;
                                                <b> To </b> <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="RD To" class="inputShortWidth" disabled /> km
                                            </td>
                                        </tr>
                                    }
                                }
                            }
                            else
                            {
                                <tr>
                                    <td class="detailtablesPiuLeftHeading"> XI. Name of new technology  <i> (if used) </i> </td>

                                    <td>
                                        <input type="checkbox" disabled />
                                        <div class="grow-wrap p-2 border border-grey text-wrap" style="float: right;min-width:250px;max-width: 250px;min-height:30px; background-color: #cccccc3d; color: #333333cf;" data-toggle="tooltip" data-placement="bottom" title="New Technology 1">

                                        </div>
                                    </td>

                                    <td colspan="2">
                                        <b> RD From </b> <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="RD From 1" class="inputShortWidth" disabled /> km
                                        &emsp;
                                        <b> To </b> <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="RD To 1" class="inputShortWidth" disabled /> km
                                    </td>
                                </tr>
                                <tr>
                                    <td></td>
                                    <td>
                                        <input type="checkbox" disabled />
                                        <div class="grow-wrap p-2 border border-grey text-wrap" style="float: right;min-width:250px;max-width: 250px;min-height:30px; background-color: #cccccc3d; color: #333333cf;" data-toggle="tooltip" data-placement="bottom" title="New Technology 2">

                                        </div>
                                    </td>
                                    <td colspan="2">
                                        <b> RD From </b> <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="RD From 2" class="inputShortWidth" disabled /> km
                                        &emsp;
                                        <b> To </b> <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="RD To 2" class="inputShortWidth" disabled /> km
                                    </td>
                                </tr>
                                <tr>
                                    <td></td>
                                    <td>
                                        <input type="checkbox" disabled />
                                        <div class="grow-wrap p-2 border border-grey text-wrap" style="float: right;min-width:250px;max-width: 250px;min-height:30px; background-color: #cccccc3d; color: #333333cf;" data-toggle="tooltip" data-placement="bottom" title="New Technology 3">

                                        </div>
                                    </td>
                                    <td colspan="2">
                                        <b> RD From </b> <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="RD From 3" class="inputShortWidth" disabled /> km
                                        &emsp;
                                        <b> To </b> <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="RD To 3" class="inputShortWidth" disabled /> km
                                    </td>
                                </tr>
                            }
                            <tr>
                                <td class="detailtablesPiuLeftHeading"> XII. Estimated cost <i>(as cleared by GOI)</i> </td>
                                <td> Rs. <input type="text" value="@Model.Prefilled_Details.ESTIMATED_COST" data-toggle="tooltip" data-placement="bottom" title="Estimated cost (as cleared by GOI)" class="inputShortWidth" disabled> Lakh </td>
                            </tr>
                            <tr>
                                <td class="detailtablesPiuLeftHeading"> XIII. Technical Sanction cost </td>
                                <td> Rs. <input type="text" value="@Model.GENERAL_INFO_PIU_view.TECHNICAL_SANC_COST" data-toggle="tooltip" data-placement="bottom" title="Technical Sanction cost" class="inputShortWidth" disabled> Lakh </td>
                            </tr>
                            <tr>
                                <td class="detailtablesPiuLeftHeading"> XIV. Awarded cost : </td>
                                <td> Rs. <input type="text" value="@Model.Prefilled_Details.AWARDED_COST" data-toggle="tooltip" data-placement="bottom" title="Awarded cost" class="inputShortWidth" disabled> Lakh </td>
                            </tr>
                            <tr>
                                <td class="detailtablesPiuLeftHeading"> XV. Expenditure <b><i>(If work is ongoing)</i></b> </td>
                            </tr>
                            <tr>
                                <td></td>
                                <td class="detailtablesPiuLeftHeading"> a. Expenditure done </td>
                                @if (Model.Prefilled_Details.PHYSICAL_WORK_STATUS.Contains("P"))
                                {
                                    <td> Rs. <input type="text" value="@Model.Prefilled_Details.EXPENDITURE_DONE" data-toggle="tooltip" data-placement="bottom" title="Expenditure done" class="inputShortWidth" disabled> Lakh</td>

                                }
                                else
                                {
                                    <td> Rs. <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="Expenditure done" class="inputShortWidth" disabled> Lakh</td>

                                }
                            </tr>
                            <tr>
                                <td></td>
                                <td class="detailtablesPiuLeftHeading"> b. Bills pending </td>
                                @if (Model.Prefilled_Details.PHYSICAL_WORK_STATUS.Contains("P"))
                                {
                                    <td> Rs. <input type="text" value="@Model.GENERAL_INFO_PIU_view.BILLS_PENDING" data-toggle="tooltip" data-placement="bottom" title="Bills Pending" class="inputShortWidth" disabled> Lakh</td>

                                }
                                else
                                {
                                    <td> Rs. <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="Bills Pending" class="inputShortWidth" disabled> Lakh</td>

                                }
                            </tr>
                            <tr>
                                <td></td>
                                <td class="detailtablesPiuLeftHeading"> c. Total expenditure (a+b) </td>
                                @if (Model.Prefilled_Details.PHYSICAL_WORK_STATUS.Contains("P"))
                                {
                                    <td> Rs. <input type="text" value="@(@Model.Prefilled_Details.EXPENDITURE_DONE + @Model.GENERAL_INFO_PIU_view.BILLS_PENDING)" data-toggle="tooltip" data-placement="bottom" title="Total expenditure" class="inputShortWidth" disabled> Lakh</td>

                                }
                                else
                                {
                                    <td> Rs. <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="Total expenditure" class="inputShortWidth" disabled> Lakh</td>

                                }
                            </tr>
                            @if (Model.Prefilled_Details.PHYSICAL_WORK_STATUS.Contains("C"))
                            {
                                <tr>
                                    <td class="detailtablesPiuLeftHeading" colspan="2"> XVI.Completion cost<b><i>(If work is completed)</i></b> </td>
                                    <td> Rs. <input type="text" value="@Model.Prefilled_Details.COMPLETION_COST" data-toggle="tooltip" data-placement="bottom" title="Completion cost" class="inputShortWidth" disabled> Lakh</td>
                                </tr>
                            }
                            else
                            {
                                <tr>
                                    <td class="detailtablesPiuLeftHeading" colspan="2"> XVI.Completion cost<b><i>(If work is completed)</i></b> </td>
                                    <td> Rs. <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="Completion cost" class="inputShortWidth" disabled> Lakh</td>
                                </tr>
                            }

                            <tr>
                                <td colspan="2" class="detailtablesPiuLeftHeading"> XVII. The work is a case of <b><i>(check boxes as applicable)</i></b> </td>
                            </tr>

                            @if (Model.GENERAL_INFO_PIU_view.WORK_TYPE == null)
                            {
                                <tr>
                                    <td class="detailtablesPiuLeftHeading">
                                        &emsp13; A) <input type="checkbox" disabled /> New Connectivity
                                    </td>
                                    <td colspan="2" class="detailtablesPiuLeftHeading">
                                        Total length &emsp13;&emsp13;
                                        <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title=" New Connectivity Total length" class="inputShortWidth" disabled> km
                                    </td>
                                </tr>
                                <tr>
                                    <td class="detailtablesPiuLeftHeading"> &emsp13;&emsp13; (i) Carriageway width (m) </td>
                                    <td colspan="2">
                                        <input type="checkbox" disabled /> 3m &emsp13; &emsp13;
                                        <input type="checkbox" disabled /> 3.75m &emsp13; &emsp13;
                                        <input type="checkbox" disabled /> 5.5m &emsp13; &emsp13;
                                        <input type="checkbox" disabled /> 7m &emsp13; &emsp13;
                                    </td>
                                </tr>

                                <tr>
                                    <td class="detailtablesPiuLeftHeading">
                                        &emsp13;  B) <input type="checkbox" disabled /> Up-gradation
                                    </td>
                                    <td colspan="2" class="detailtablesPiuLeftHeading">
                                        Total length &emsp13;&emsp13;
                                        <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="Up-gradation Total length" class="inputShortWidth" disabled> km
                                    </td>
                                </tr>
                                <tr>
                                    <td class="detailtablesPiuLeftHeading" colspan="2"> &emsp13;&emsp13; (i) Carriageway width and length </td>
                                </tr>
                                <tr>
                                    <td rowspan="2"> &emsp13; &emsp13; <input type="checkbox" disabled /> With widening </td>
                                    <td colspan="2">
                                        Existing width &emsp13; <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="Existing width" class="inputShortWidth" disabled> m
                                        &emsp;    Length <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="Length" class="inputShortWidth" disabled> km
                                    </td>
                                <tr>
                                    <td colspan="2"> Proposed width <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="Proposed width" class="inputShortWidth" disabled> m </td>
                                </tr>
                                </tr>

                                <tr>
                                    <td> &emsp13; &emsp13; <input type="checkbox" disabled /> Without widening </td>
                                    <td colspan="2">
                                        Proposed width <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="Proposed width" class="inputShortWidth" disabled> m
                                        &emsp; Length <input type="text" value="" data-toggle="tooltip" data-placement="bottom" title="Length" class="inputShortWidth" disabled> km
                                    </td>
                                </tr>
                            }
                            else if (Model.GENERAL_INFO_PIU_view.WORK_TYPE == "N")
                            { <!-- CHECK FOR NEW CONNECTIVITY  -->
                                <tr>
                                    <td class="detailtablesPiuLeftHeading">
                                        &emsp13; A) <input type="checkbox" checked disabled /> New Connectivity
                                    </td>
                                    <td colspan="2" class="detailtablesPiuLeftHeading">
                                        Total length &emsp13;&emsp13;
