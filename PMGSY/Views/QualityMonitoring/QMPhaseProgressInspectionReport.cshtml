@using MvcReportViewer
@model PMGSY.Models.QualityMonitoring.QMPahseInspectionProgressViewModel

<div id="QMPhaseProgressDetails" style="width: 100%; height: 590px; overflow-x: hidden; overflow-y: hidden;">
@if (Model.YearCode == 0)
{
      @Html.MvcReportViewer("/PMGSYCitizen/QM_SANCTION_INSPECTION_DETAILS_2",
     new
     {
               
         state = Model.StateCode,       
         StateName = Model.StateName
     },
     new { Height = 700, Width = "100%", style = "border:none;" })
}
else
{
   @Html.MvcReportViewer("/PMGSYCitizen/QM_SANCTION_INSPECTION_DETAILS_PART_2",
     new
     {
               
         state = Model.StateCode,       
         StateName = Model.StateName,
         YEAR = Model.YearCode
     },
     new { Height = 700, Width = "100%", style = "border:none;" })     
}
</div>
