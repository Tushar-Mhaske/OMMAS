@using MvcReportViewer
@model PMGSY.Models.QualityMonitoring.QMCompletedInspDetailsViewModel

<div id="QMCompletedInspDetails" style="width: 100%; height: 590px; overflow-x: hidden; overflow-y: hidden;">
    @Html.MvcReportViewer("/PMGSYCitizen/QMCompletedInspDetails",
     new
     {


         level = Model.LevelID,
         scheme = PMGSY.Extensions.PMGSYSession.Current.PMGSYScheme,
         state = Model.StateCode,
         StateName = Model.StateName,
         district = 0,
         duration = 0,
         status = 0,
         qmtype = 0

     },
     new { Height = 600, Width = 1120, style = "border:none;" })

</div>

