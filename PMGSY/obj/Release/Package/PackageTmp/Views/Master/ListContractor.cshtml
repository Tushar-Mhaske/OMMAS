
@using System.Collections.Generic

<script type="text/javascript" src="@Url.Content("~/Scripts/jquery.ui.autocomplete.js")"> </script>
<script src="~/Scripts/custom/Generic.js" type="text/javascript"></script>
<script src="~/Scripts/Master/ListContractor.js" type="text/javascript"></script>

<form id="frmListContractor">
    <center>
<div style="width:100%">


<div id="searchContractor">
    <div id="dvhdSearch" class="ui-corner-top ui-widget-header panelheader" style="text-align:left; height:20px;"> 
     <span class="ui-jqgrid-title" style="float:left">&nbsp;Search</span>
     <span  id="spCollapseIconS" class="ui-icon ui-icon-circle-triangle-n" style="float:right"></span>
      </div>
        <div id="dvSearchParameter" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
           @*  <td style="text-align:left;width:0%;display:none;";>@Html.Label("District")</td>
               <td style="text-align:left;width:0%;display:none;">@Html.DropDownList("District", null, new { @title = "District" })</td>*@ 
     <table style="width:100%" class="rowstyle">
            <tr>
               <td style="text-align:left;width:10%;">@Html.Label("State")</td>
               <td style="text-align:left;width:15%">@Html.DropDownList("State", null, new { @title = "State" })</td>       
                <td style="text-align:left;width:15%;";>@Html.Label("Type")</td>
               <td style="text-align:left;width:15%;">@Html.DropDownList("ContractorSupplier", null, new { @title = "Contractor / Supplier Type" })</td>  
               <td style="text-align:left;width:5%"> @Html.Label("Status")</td>
               <td style="text-align:left;width:15%">@Html.DropDownList("Status", null, "All", new { @title = "Status" })</td>
           </tr> 
           <tr>  
              <td style="text-align:left;width:10%;">@Html.Label("PAN / TAN Number")</td>
               <td style="text-align:left;">@Html.TextBox("PAN", "", new { @id = "txtPAN", @placeholder = "Enter PAN / TAN No...",autocomplete = "on", @style = "width:147px;", @title = "Enter PAN / TAN No" })</td>  
                                        
               <td style="text-align:left;width:5%;">@Html.Label("Name")</td>
               <td style="text-align:left;">@Html.TextBox("Contractor", "", new { @id = "txtContractor", @placeholder = "Enter Contractor/Supplier Name...", @title = "Enter Contractor/Supplier Name" })</td>  
              
                <td><input type="button" id="loadGrid" name="Search" value="Search" class= "jqueryButton" title="Search Contractor/Supplier Information"/></td>
           </tr>
            <tr></tr>
         </table>
        </div>
</div>

<div >
    
    <table style="width:100%; padding-top:1.5em;">
    <tr>  
        <td>
             <div class="ui-state-default ui-corner-all jqueryButton" id="btnAddNew" title="Add Contractor/Supplier">
                    <span style="float: left" class="ui-icon ui-icon-circle-plus"></span>
                    <span style="float: left; margin-right: 5px">Add Contractor/Supplier </span>
             </div>

            </td>
        </tr>
     </table>
    <table id="Contractor">
    </table>
    <div id="pager">
    </div>
    </div>
    <br />
    <br />
<div id="registrationDetails">
    <div id="regHeader" class="ui-corner-top ui-widget-header panelheader" style="text-align:left; height:20px;"> 
    <span class="ui-jqgrid-title" style="float:left">&nbsp;Contractor/Supplier Registration</span>
        </div>
    <div id="dvregDetails" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <div id="regDetails">
    </div>
        <br />
        <table>
        <tr>
            <td>
                <input type="button" id="btnAddNewReg" name="btnAddNew" value="Add Contractor/Supplier Registration" class= "jqueryButton" title="Add Contractor/Supplier Registration" /> 
                <input type="button" id="btnRegCancel" name="btnAddNew" value="Cancel" class= "jqueryButton" title="Cancel Registration"  /> 
            </td>
        </tr>
        </table>
    <br />
    </div>
</div>
        <div id="dvlstBankDetails">
         </div>
     </div>
    </center>
</form>
