@model PMGSY.Areas.ProgressReport.Models.CuplExclusionModel

@{
    ViewBag.Title = "GetCuplExclusionView";
}

<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script src="~/Scripts/HighChart/highcharts.js"></script>

<script src="~/Areas/ProgressReport/Scripts/CuplExclusion.js"></script>
<div id="dvhdPhysicalProgressWorkLayout" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">@*margin-left:0px;margin-right:0px;margin-top:0px; *@
    <span class="ui-jqgrid-title" style="float: left">&nbsp;CUPL Exclusion</span>

    <span id="spCollapseIconCN" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>
<div id="loadFilters" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmPhyProgessWork">

         @Html.ValidationSummary(true)

        <table style="width: 100%;" class="rowstyle">
            <tr>
                 <td style="text-align: left; width: 10%;">
                    @Html.LabelFor(model => model.Year)<span style="color: red"> *</span>
                </td>

                <td style="width: 10%;">
                   @Html.DropDownListFor(model => model.Year, new SelectList(Model.YearList, "Value", "Text"), new { @title = "Year", @tabindex = 1, @id = "YearList_PhyProgressWorkDetails", @style = "width:150px" }) 
                   @Html.ValidationMessageFor(m => m.Year)
                </td>

                 <td style="text-align: left; width: 10%;">
                    @Html.LabelFor(model => model.StateName)<span style="color: red"> *</span>
                </td>

                <td style="width: 10%;">
                    @Html.DropDownListFor(model => model.StateCode, new SelectList(Model.StateList, "Value", "Text"), new { @title = "State", @tabindex = 2, @id = "StateList_PhyProgressWorkDetails", @style = "width:150px" })
                    @Html.ValidationMessageFor(m => m.StateCode)
                </td>

                <td style="text-align: left; width: 10%;">
                    @Html.LabelFor(model => model.Batch)<span style="color: red"> *</span>
                </td>

                <td style="width: 10%;">
                    @Html.DropDownListFor(model => model.Batch, new SelectList(Model.BatchList, "Value", "Text"), new { @title = "Batch", @tabindex = 3, @id = "BatchList_PhyProgressWorkDetails", @style = "width:150px" })
                    @Html.ValidationMessageFor(m => m.Batch)
                </td>

                <td>
                    &nbsp;&nbsp;&nbsp;<input type="button" id="btnViewPhyProgressWork" value="View" class="button" title="View" tabindex="7" />
                </td>
                    
            </tr>
        </table>
    </form>

</div>

<div id="loadReport" class="divLoadReport"></div>

