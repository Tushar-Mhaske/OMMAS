@model PMGSY.Models.Master.ViewAdminTechnicalAgencyMapping
<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery.ui.autocomplete.js")"> </script>
<script src="~/Scripts/Master/ViewAdminTechnicalAgencyMapping.js"></script>
<div id="dvhdSearch" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;District Agency  </span>
    <span id="spCollapseIconS" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>

<div id="dvViewAdminTechnical" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmViewAgencyMapping">
        <center>@Html.ValidationSummary(true) </center>
        @Html.Hidden("distAgencyroleCode", PMGSY.Extensions.PMGSYSession.Current.RoleCode)
        <table style="width: 100%;" class="rowstyle">
            <tr>
                <td style="width: 10%"></td>

                <td style="text-align: left; width: 10%;">
                    @Html.LabelFor(m=>m.Agency, new { @id = "lblAgencyType" })
                </td>

                <td style="width: 20%;">
                    @Html.DropDownListFor(m => m.Agency, new SelectList(Model.AgencyList, "Value", "Text"), new { @id = "ddlAgency", @style = "width:120px;" })
                    <br />
                    @Html.ValidationMessageFor(m=>m.Agency)
                </td>

                <td style="text-align: left; width: 10%;">
                    @Html.LabelFor(m=>m.State, new { @id = "lblState" })
                </td>

                <td style="width: 20%;">
                    @Html.DropDownListFor(m => m.State, ViewData["StateList"] as IEnumerable<SelectListItem>, new { @id = "ddlState", @style = "width:120px;" })
                    <br />
                    @Html.ValidationMessageFor(m=>m.State)
                </td>
                <td style="text-align: left; width: 10%;">
                    @Html.LabelFor(m=>m.District, new { @id = "lblDistrict" })
                </td>

                <td style="width: 20%;">
                    @Html.DropDownListFor(m => m.District, ViewData["DistrictList"] as IEnumerable<SelectListItem>, new { @id = "ddlDistrict", @style = "width:120px;" })
                </td>

                <td>
                    <input type="button" id="btnViewDistrictAgency" class="jqueryButton" value="View" title="View District Agency" />
                </td>
            </tr>

        </table>
    </form>
</div>
