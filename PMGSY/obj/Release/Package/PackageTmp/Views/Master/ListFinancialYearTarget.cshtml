@{
    ViewBag.Title = "ListFinancialYearTarget";
}

<script src="~/Scripts/custom/Generic.js"></script>
<script src="~/Scripts/jquery.ui.accordion.js"></script>
<script src="~/Scripts/jquery.ui.tabs.js"></script>
<script src="~/Scripts/Master/FinancialYearTarget.js"></script>

<center>    

        <div style="width:96%; height:20px;border:none; margin-top:5px; margin-bottom:-15px; " class="ui-state-default">
            <span style="float:right;margin-right:2Px; margin-top:2px" id="idFilterDiv" class="ui-icon ui-icon-circle-triangle-n"></span>     
        </div>
        <div style="width: 96%; border:none; margin-top:-15px; margin-bottom:10px;" class="ui-state-default">      
        <div id="divFilterForm">
            <form id="FilterForm">                     
                <center>
                    <table style="margin-top:10px; margin-bottom:5px">
                        <tr>
                            @Html.Hidden("PMGSYScheme", PMGSY.Extensions.PMGSYSession.Current.PMGSYScheme)
                            @Html.Hidden("RoleCode", PMGSY.Extensions.PMGSYSession.Current.RoleCode)
                            @Html.Hidden("isPMGSY3", Convert.ToString(ViewData["isPMGSY3"]))
                             <td style="text-align:left;width:17%">
                             @if (PMGSY.Extensions.PMGSYSession.Current.RoleCode == 25 || PMGSY.Extensions.PMGSYSession.Current.RoleCode == 53 || PMGSY.Extensions.PMGSYSession.Current.RoleCode == 65)//Changes by SAMMED A. PATIL for mordviewuser
                             {
                                 <td style="text-align:right;width:5%">
                                    @Html.Label("State")
                                 </td>
                                 <td style="text-align:center;width:20%">
                                         @Html.DropDownList("ddlState", ViewData["States"] as IEnumerable<SelectListItem>, new { @id = "ddlStates", @title = "States", @tabindex = 1 })
                                 </td>
                                 <td style="text-align:right;width:5%">
                                    @Html.Label("Year")
                                 </td>
                                 <td style="text-align:center;width:20%">
                                         @Html.DropDownList("ddlYear", ViewData["Years"] as IEnumerable<SelectListItem>, new { @id = "ddlYear", @title = "Year", @tabindex = 2 })
                                 </td>
                             }
                            
                                
                            <td style="text-align:left;width:17%">
                                
                            </td>
                        </tr>
                        <tr>
                            <td colspan="10">
                                <center>
                                <table>
                                    <td>
                                        <input type="button" class="jqueryButton" value="List Financial Target"  title="Click here to view existing Financial Target list" id="btnListExistingFinancialTarget" name ="btnListExistingFinancialTarget"/>
                                    </td>
                                   
                                      
                                    <td>
                                        <input type="button" value="Add Financial Target" class="jqueryButton" title="Click here to add Financial Target" id="btnAddFinancialTarget" name="btnAddFinancialTarget"/>
                                    </td>
                                       
                                </table>
                                    </center>
                            </td>
               
                        </tr>
                    </table>                           
                </center>
            </form>  
        </div>
    
        </div>


    <div id="dvExistingRoads" style="margin-top:20px; width:96%">
        <table id="tbFinancialYearTargetList"></table>
        <div id="dvFinancialYearTargetListPager"></div>
    </div>
    <br />
    <br />


        <div id="accordion" style="display:none;width:96%">            
               <h3 style="text-align:left"><a id="" href="#" style=" font-size:.9em;" >Add Financial Year Target Details</a>
             <a href="#"  style="float: right;">
                    <img src="" class="ui-icon ui-icon-closethick" onclick="CloseFinancialYearTargetDetails();" /></a>
            </h3>

            <div id="divFinancialYearTargetForm" style="display:none;">
            </div>                     
       </div>
</center>


