<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>

<script src="~/Scripts/jquery.ui.dialog.js"></script>

<script type="text/javascript" src="@Url.Content("~/Scripts/Master/Master_MappedAgencyStateDistricts.js")"> </script>

<script type="text/javascript">
    $(function () {
        $("#dvEndDateDialogDistrict").dialog({
            autoOpen: false,
            //height: 'auto',
            width: '550px',
            modal: true,
            resizable: false,
            draggable: true,
            title: 'End Date',
            show: {
                effect: "blind",
                duration: 1000
            },
            hide: {
                effect: "explode",
                duration: 1000
            },

            close: function () {
                $(this).dialog("close");
            }
        });
        $("#dvEndDateDialogState").dialog({
            autoOpen: false,
            //height: 'auto',
            width: '550px',
            modal: true,
            resizable: false,
            draggable: true,
            title: 'End Date',
            show: {
                effect: "blind",
                duration: 1000
            },
            hide: {
                effect: "explode",
                duration: 1000
            },

            close: function () {
                $(this).dialog("close");
            }
        });
    });
</script>

<div id="dvhdSearch_Mapped" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; margin-left: 0px; margin-right: 0px; margin-top: 10px; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;Mapped Details </span>

    <span id="spCollapseIconS_Mapped" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>
<div id="dvSearchParameter_Mapped" class="ui-corner-bottom" style="margin-left: 0px; margin-right: 0px; display: block; border: 1px solid #B5B8C8;">
    <form id="frmSearchMappedAgencyStateDistrict">
        <table style="width: 100%;" class="rowstyle">
            <tr>
                <td style="width: 10%;"></td>
                <td style="width: 10%;">
                    @Html.Label("Agency Name")
                </td>
                <td style="width: 10%;">
                    @if (!(string.IsNullOrEmpty(ViewBag.AgencyName)))
                    {
@* <blink>   </blink> *@
                        <b>@Html.Label("AgencyName", ViewBag.AgencyName as String) </b>
                    }
                    else
                    {
                        <b>@Html.Label("AgencyName", "NA")   </b>
                    }
                </td>
                @if (Convert.ToString(ViewBag.AgencyType) == "P")
                { 
                    <td style="width: 05%;">State:</td>
                    <td style="width: 10%;">
                        @Html.DropDownList("StateCode", new SelectList(ViewBag.StateList, "Value", "Text"))
                    </td>
                    <td style="width: 10%;">
                        <input type="button" id="btnShowDetails" name="btnShowDetails" value="View" class="jqueryButton" title="View Details" tabindex="1" />
                    </td>
                    <td style="width: 10%;"></td>
                }
            </tr>
            <tr>
                <td colspan="9">
                    @if (!(string.IsNullOrEmpty(ViewBag.EncryptedAgencyCode_Mapped)))
                    {
                        @Html.Hidden("EncryptedAgencyCode_Mapped", ViewBag.EncryptedAgencyCode_Mapped as String)     
                    }
                </td>
            </tr>
        </table>
        @*    <table style="width: 100%;">

            <tr>
                <td style="width: 15%;"></td>
                <td style="width: 05%;"> State:</td>
                <td style="width: 10%;">
                    @Html.DropDownList("StateCode",new SelectList(ViewBag.StateList,"Value","Text"))
                </td>
                <td style="width: 10%;">
                        <input type="button" id="btnShowDetails" name="btnShowDetails" value="View" class="jqueryButton" title="View Details" tabindex="1" /> 
                 </td>
                <td style="width: 10%;"></td>
            </tr>

         </table>*@

    </form>
    <br />
    @* <center>*@
    <div style="width: 95%;">
        <table id="tbMappedAgencyStateDistrictList">
        </table>
        <div id="dvMappedAgencyStateDistrictListPager"></div>
    </div>
    @*</center>*@
    <br />

    <table style="width: 100%;" class="rowstyle">
        <tr>
        </tr>
        <tr>
            <td style="text-align: center">
                <input type="button" id="btnMappedCancel" name="btnMapCancel" value="Back" class="jqueryButton" title="Back" tabindex="300" />
            </td>
        </tr>
        <tr></tr>
    </table>

</div>
<div id="dvEndDateDialogDistrict" style="display: none; width: 100%">
</div>
<div id="dvEndDateDialogState" style="display: none; width: 100%">
</div>

