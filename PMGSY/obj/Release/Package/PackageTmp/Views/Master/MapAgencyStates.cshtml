@*@model PMGSY.Models.Master.MasterTechnicalAgencyStartAndEndDateViewModel*@
@model System.DateTime?

<script src="~/Scripts/jquery.ui.datepicker.js" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/Master/Master_MapAgencyStates.js")"> </script>

<div id="dvhdSearch_Map_State" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; margin-left: 0px; margin-right: 0px; margin-top: 10px; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;Map States </span>

    <span id="spCollapseIconS_Map" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>
<div id="dvSearchParameter_Map_State" class="ui-corner-bottom" style="margin-left: 0px; margin-right: 0px; display: block; border: 1px solid #B5B8C8;">
    <div id="dvErrorMessage" class="ui-widget ui-state-error ui-corner-all" style="padding: 0.7em .7em; display: none; margin: 5px 5px 5px 5px;">
        <span class="ui-icon ui-icon-alert" style="float: left; margin-right: .3em;"></span>
        @*<strong>Alert:</strong> <span id="message"></span>*@
    </div>
    <form id="frmSearchMapAgencyBlock">

        <table style="width: 100%;" class="rowstyle">
            <tr style="display: none">
                <td colspan="4">
                    @Html.Hidden("EncryptedStateCodes")
                </td>
            </tr>
            <tr style="display: none">
                <td colspan="4">
                    @if (!(string.IsNullOrEmpty(ViewBag.EncryptedAgencyCode)))
                    {
                        @Html.Hidden("EncryptedAgencyCode", ViewBag.EncryptedAgencyCode as String)     
                    }
                </td>

            </tr>
            <tr>

                <td style="width: 25%;"></td>
                <td style="text-align: left; width: 20%;">
                    @Html.Label("Agency Name")

                </td>
                <td colspan="2">
                    @if (!(string.IsNullOrEmpty(ViewBag.AgencyName)))
                    {
@* <blink>   </blink> *@
                        <b>@Html.Label("AgencyName", ViewBag.AgencyName as String) </b>
                    }
                    else
                    {
                        <b>@Html.Label("AgencyName", "NA")   </b>
                    }
                </td>
            </tr>
        </table>
    </form>
    <br />

    @* <center>*@
    <div style="width: 95%;">
        <table id="tbMapAgencyStateList">
        </table>

        <div id="dvMapAgencyStateListPager"></div>
    </div>
    @*</center>*@
    <br />
    <form id="frmStartDateState">
    <div id="startDateState" style="float:inherit;margin-left: 0px; margin-right: 0px; display: none; border: 0px solid #B5B8C8;" class="ui-corner-bottom"   >
       <table style="width: 100%;" class="rowstyle">
           @*<tr>
                <td></td>
                <td style="width:15%;">
                     &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    @Html.LabelFor(model => model.MAST_START_DATE)
                    <label style="color: red;">*</label>
                </td>
                <td style="width:90%;">
                    @Html.TextBoxFor(model => model.MAST_START_DATE, new { @title = "Start date", @tabindex = 1, maxlength = 10, placeholder = "dd/mm/yyyy" })
                    @Html.ValidationMessageFor(model => model.MAST_START_DATE)
                </td>
            </tr>*@
           <tr>
                <td style="width:15%;"> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    @Html.Label("Start Date") <label style="color:red;"> *</label> 
                </td>
                
                <td style="width:90%;">
        
                        @Html.TextBox("StartDateState", (Model.HasValue ? Model.Value.ToString("dd/MM/yyyy") : new DateTime(DateTime.Now.Year, DateTime.Now.Month, DateTime.Now.Day).ToString("dd/MM/yyyy")), new { @id = "strtDateState", @tabindex = 1 , @placeholder="dd/mm/yyyy", })
        
                      <label style="color:red;"> @Html.ValidationMessage("StartDateState", "Please Select a Start Date!!", new { id = "messageState" })</label> 
                </td>
           </tr>
            <tr>
                <td colspan="3">
                    <label style="border: none">[ Note: Fields marked with<label style="color: red;"> * </label>are mandatory.]</label>
                </td>
            </tr>
        </table>
     </div>
   </form>
    <br />
    <table style="width: 100%;" class="rowstyle">
        <tr>
        </tr>
        <tr>
            <td style="text-align: center">

                <input type="button" id="btnMapState" name="btnMapState" value="Map States" class="jqueryButton" title="Map States" tabindex="200" />

                <input type="button" id="btnMapStateCancel" name="btnMapStateCancel" value="Cancel" class="jqueryButton" title="Cancel" tabindex="201" />
            </td>
        </tr>
        <tr></tr>
       
    </table>

</div>
