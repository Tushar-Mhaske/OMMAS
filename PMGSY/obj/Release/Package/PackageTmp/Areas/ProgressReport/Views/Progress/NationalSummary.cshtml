@model PMGSY.Areas.ProgressReport.Models.TargetAchievementViewModel

<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script src="~/Scripts/jquery.ui.datepicker.js"></script>
<script src="~/Areas/ProgressReport/Scripts/NationalSummary.js"></script>

<div id="dvhdMPRNewIAPReportDetails" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;National Summary</span>
</div>
<div id="loadFilters" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmTargetAchievement">

        @Html.ValidationSummary(true)

        <table style="width: 100%;" class="rowstyle">
            <tr>
                <td style="width: 2%; vertical-align: top"></td>
                <td style="text-align: left; width: 5%; vertical-align: top">
                @*    @Html.Label("From Date")*@
                </td>
                <td style="width: 15%; vertical-align: top; display:none;">
                        @Html.TextBoxFor(model => model.fromDate, new { @id="txtFromDate", @maxlength = "10", @tabindex = 1, @style = "width:23%", @placeholder = "Form Date", @readonly="readonly" })
                        <br />
                        @Html.ValidationMessageFor(m => m.fromDate)
                </td>
                <td style="text-align: left; width: 5%; vertical-align: top">
              @*      @Html.Label("To Date")*@
                </td>
                <td style="width: 15%; vertical-align: top; display:none; ">
                        @Html.TextBoxFor(model => model.toDate, new {  @id="txtToDate", @maxlength = "10", @tabindex = 1, @style = "width:23%", @placeholder = "To Date", @readonly="readonly" })
                        <br />
                        @Html.ValidationMessageFor(m => m.toDate)
                </td>
                <td style="width: 5%; vertical-align: top; display:none;">
                    <input type="button" class="button" value="View" id="btnViewTargetAchievementReport" title="View" />
                </td>
                <td style="width: 5%; vertical-align: top"></td>
            </tr>
        </table>
    </form>
</div>
<div id="loadReport" class="dvLoadReport"></div>
