@{
    ViewBag.Title = "ViewClusterHabitation";
}

@model PMGSY.Models.Master.MasterClusterViewEditHabiationModel


<script src="~/Scripts/custom/Generic.js" type="text/javascript"></script>
@*<script src="~/Scripts/jquery.ui.datepicker.js" type="text/javascript"></script>*@
<script src="~/Scripts/Master/ViewClusterHabitation.js" type="text/javascript"></script>

<div id="dvhdAddNewClusterDetails" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;View Cluster Habitation Details</span>

    <span id="spCollapseIconCN" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>

<div id="dvViewClusterHabiationDetails" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8; text-align: left">
    <div id="dvViewErrorMessage" class="ui-widget ui-state-error ui-corner-all" style="padding: 0.7em .7em; display: none; margin: 5px 5px 5px 5px;">
        <span class="ui-icon ui-icon-alert" style="float: left; margin-right: .3em;"></span>
        <strong>Alert:</strong> <span id="message"></span>
    </div>
    <form id="frmViewClusterHabiationDetails">
         @Html.AntiForgeryToken()
        <center>@Html.ValidationSummary(true)</center>
        <table style="width: 98%; margin-top: 10px;" class="rowstyle">
            <tr style="display: none">
                <td colspan="5">
                    @Html.HiddenFor(model => model.EncryptedClusterCode)
                    @Html.HiddenFor(model => model.StatCode)
                    @Html.HiddenFor(model => model.DistrictCode)
                    @Html.HiddenFor(model => model.BlockCode)
                    @Html.HiddenFor(model => model.ClusterName)
                    @Html.HiddenFor(model => model.FinalizeLockStatus)
                      @Html.HiddenFor(model => model.IMS_UNLOCK_Status)
                </td>
            </tr>
            <tr>
                <td style="width: 2%;"></td>
                <td style="text-align: left; width: 15%;">
                    @Html.Label("State Name")
                </td>
                <td style="width: 20%;">

                    <b>@Html.DisplayFor(model => model.StatName)   </b>

                </td>
                <td style="text-align: left; width: 15%;">
                    @Html.Label("District Name")
                </td>
                <td style="width: 20%;">

                    <b>@Html.DisplayFor(model => model.DistrictName)   </b>

                </td>

            </tr>
            <tr>
                <td style="width: 2%;"></td>

                <td style="text-align: left; width: 15%;">
                    @Html.Label("Block Name")
                </td>
                <td style="width: 20%;">

                    <b>@Html.DisplayFor(model => model.BlockName)   </b>

                </td>
                <td style="text-align: left; width: 15%;">
                    @Html.Label("Cluster Name")
                </td>
                <td style="width: 20%;">

                    <b>@Html.DisplayFor(model => model.ClusterName)   </b>

                </td>

            </tr>


            <tr>
                <td></td>
                <td colspan="4">
                    <div id="loadViewHabitationCluster" style="margin-top: 5px;">
                        <table id="tblViewHabitationCluster">
                        </table>
                        <div id="divViewHabitationClusterPager">
                        </div>
                    </div>
                </td>
            </tr>
            <tr>
                <td></td>
                <td colspan="4">
                    <center>
                         @if (Model.IMS_UNLOCK_Status == "U" || Model.FinalizeLockStatus == "N")
                         {
                             
                    <input type="button" id="btnAddClusterHabiation" name="btnAddClusterHabiation" value="Add Cluster Habitation" class="jqueryButton" title="Add Cluster Habitation" tabindex="1" />
                      
                         }
                         @if (Model.FinalizeLockStatus == "N")
                          {
                          <input type="button" id="btnFinalizeCluster" name="btnFinalizeCluster" value="Finalize" class="jqueryButton" title="Finalize Cluster Habitation" tabindex="2" />
                          }
                        <input type="button" id="btnCancelClusterHabiation" name="btnCancelClusterHabiation" value="Back" class="jqueryButton" title="Back Cluster Habitation" tabindex="3" />
                  </center>
                </td>

            </tr>

            <tr>

                <td></td>
                <td colspan="4">
                    <div id="loadViewAddHabitationCluster" style="margin-top: 5px; display: none">
                        <table id="tblViewAddHabitationCluster">
                        </table>
                        <div id="divViewAddHabitationClusterPager">
                        </div>
                    </div>
                </td>
            </tr>
            <tr>
                <td></td>
                <td colspan="4" id="tdbtnUpdateCluster" style="display: none">
                    <center>
                    <input type="button" id="btnUpdateClusterHabiation" name="btnUpdateClusterHabiation" value="Update Cluster Habitation" class="jqueryButton" title="Update Cluster Habitation" tabindex="4" />
                    <input type="button" id="btnUpdateCancelClusterHabiation" name="btnUpdateCancelClusterHabiation" value="Cancel" class="jqueryButton" title="Cancel Cluster Habitation" tabindex="5" />
                    </center>
                </td>
            </tr>




        </table>


    </form>
</div>
