                </td>


                  <td style="text-align: left; width: 4%;">
                    @Html.LabelFor(model => model.MonthCode)
                </td>

                <td style="width: 25%;">
                    @Html.DropDownListFor(model => model.MonthCode, new SelectList(Model.MonthList, "Value", "Text"), new { @title = "Month", @id = "month", @tabindex = 4, @style = "width:150px" })
                    @Html.ValidationMessageFor(m => m.MonthCode)
                </td>


             






                <td>
                    <input type="button" id="btnViewPackageWiseDetails" value="View" class="jqueryButton" title="View Detail" tabindex="5" />

