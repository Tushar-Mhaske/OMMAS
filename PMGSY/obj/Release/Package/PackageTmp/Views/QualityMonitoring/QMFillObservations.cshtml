                            </td>
                            <td style="width:12%;font-weight:bold;" >
                                @Html.LabelFor(model => model.IMS_BRIDGE_NAME)
                            </td>
                            <td>                            
                                @Html.DisplayFor(model => model.IMS_BRIDGE_NAME)
                            </td> 
