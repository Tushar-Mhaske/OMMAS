@using MvcReportViewer
@model PMGSY.Areas.PackageAgreementSanctionList.Models.RSALayout


@Html.MvcReportViewer("/PMGSYCitizen/RSA",
     new
     {
         LEVEL=0,
         STATE = Model.Mast_State_Code,
         DISTRICT = Model.Mast_District_Code,
         YEAR = @Model.Year,
         SCHEME=@Model.schemeCode
     },
     new { Height = 600, Width = "100%", style = "border:none;" })
