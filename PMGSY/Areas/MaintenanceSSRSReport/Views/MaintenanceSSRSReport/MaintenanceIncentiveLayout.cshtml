@model PMGSY.Areas.MaintenanceSSRSReport.Models.MaintenanceIncentiveViewModel

<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>

<script src="~/Areas/MaintenanceSSRSReport/Scripts/MaintenanceIncentiveLayout.js"></script>
<div id="dvhdCreateNewReportDetails" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;Maintenance Incentive Report</span>

</div>

<form id="frmMaintenanceIncentive">
    @Html.ValidationSummary(true)
    @Html.HiddenFor(m => m.StateName)


    <table style="width: 100%;" class="rowstyle">
        <tr>
            <td style="width: 2%;"></td>
            <td style="text-align: left; width: 7%;">
                @Html.Label("State")
            </td>

            <td style="width: 20%;">
                @Html.DropDownListFor(model => model.StateCode, new SelectList(Model.StateList, "Value", "Text"), new { @title = "State", @tabindex = 1, @id = "ddlState", @style = "width:150px" })
                <br />
                @Html.ValidationMessageFor(m => m.StateCode)
            </td>
             <td style="text-align: left; width: 7%;">
                @Html.Label("Year")
            </td>

            <td style="width: 20%;">
                @Html.DropDownListFor(model => model.YearCode, new SelectList(Model.YearList, "Value", "Text"), new { @title = "Year", @tabindex = 2, @id = "ddlYear", @style = "width:150px" })
                <br />
                @Html.ValidationMessageFor(m => m.YearCode)
            </td>
            <td style="width: 4%;">
                <input type="button" id="btnView" value="View" class="jqueryButton" title="View" tabindex="2" />
            </td>
        </tr>
    </table>

    <div id="loadReport" class="divLoadReport"></div>
</form>
