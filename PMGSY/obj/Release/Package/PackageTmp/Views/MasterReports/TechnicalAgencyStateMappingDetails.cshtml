@{
    ViewBag.Title = "_TechnicalAgencyStateMappingDetails";
}
<div style="width: 100%">
    <table class="ui-state-default ui-corner-all" style="width: 100%;">
        <tr>
            <td style="width: 2%;"></td>
            <td style="text-align: left; width: 5%;">
                @Html.Label("State")
            </td>
            <td style="width: 15%;">
                @Html.DropDownList("StateList_TechnicalAgencyStateMapDetails", ViewData["STATE"] as IEnumerable<SelectListItem>, new { @title = "State", @tabindex = 1 })
            </td>
            <td style="text-align: left; width: 5%;">
                @Html.Label("TA Type")
            </td>
            <td style="width: 15%;">
                @Html.DropDownList("TATypeList_TechnicalAgencyStateMapDetails", ViewData["TATYPE"] as IEnumerable<SelectListItem>, new { @title = "TA Type", @tabindex = 2 })
