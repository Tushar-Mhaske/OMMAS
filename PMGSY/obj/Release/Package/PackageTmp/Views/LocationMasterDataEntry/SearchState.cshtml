@model PMGSY.Models.MasterDataEntry.MASTER_STATE
<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script src="~/Scripts/MasterDataEntry/Master_SearchState.js"></script>

<div id="dvhdSearch" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;Search </span>
    <span id="spCollapseIconS" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>
<div id="dvSearchParameter" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmSearchItemtype">
        <center>@Html.ValidationSummary(true) </center>

        <table style="width: 100%;" class="rowstyle">
            <tr>
                <td style="width: 10%"></td>
                <td style="text-align: left; width: 10%;">
                       @Html.LabelFor(model => model.StateUTID)
             
                </td>
                <td style="width: 20%;">
                  @Html.DropDownListFor(model => model.StateUTID, Model.StateUTs, new { @id = "ddlSearchStateUTs", @title = "State/UT", @tabindex = 1 })
                   
                </td>
                  <td style="text-align: left; width: 10%;">
                   @Html.LabelFor(model => model.StateTypeID) 
                </td>
                <td style="width: 20%;">
                     @Html.DropDownListFor(model => model.StateTypeID, Model.StateTypes, new { @id = "ddlSearchStateTypes", @title = "State Type", @tabindex = 2 })
                
                </td>
                <td>
                    <input type="button" id="btnSearch" name="btnSearch" value="Search" class="jqueryButton" title="Search State" />
                </td>
            </tr>
        </table>
    </form>
</div>
