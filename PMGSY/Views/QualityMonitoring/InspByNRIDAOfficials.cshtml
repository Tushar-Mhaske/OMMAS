@model PMGSY.Models.QualityMonitoring.ViewFilterInspectionByNRIDA
@{
    ViewBag.Title = "InspByNRIDAOfficials";
}


<script src="~/Scripts/custom/Generic.js"></script>
<script src="~/Scripts/jquery.ui.accordion.js"></script>
<script src="~/Scripts/QualityMonitoring/InspByNRIDAOfficials.js"></script>

<center>
    <div id="spanDiv" style="width:98%; height:20px;border:none; margin-top:5px; margin-bottom:-15px; " class="ui-corner-top ui-widget-header panelheader">
        @if (PMGSY.Extensions.PMGSYSession.Current.RoleCode == 25 || PMGSY.Extensions.PMGSYSession.Current.RoleCode == 5 || PMGSY.Extensions.PMGSYSession.Current.RoleCode == 9)
        {
            <span class="ui-jqgrid-title" style="float: left">&nbsp; Upload Inspection Report</span>
        }
        else if (PMGSY.Extensions.PMGSYSession.Current.RoleCode == 2)
        {
            <span class="ui-jqgrid-title" style="float: left">&nbsp; Upload ATR</span>
        }
    </div>
    <div id="divSearchExecution">
        <div style="width: 98%; border:none; margin-top:15px; margin-bottom:10px;" class="ui-state-default">
            <center>
                <form id="searchExecution">
                    <div style="width: 98%; border:none; margin-top:10px; margin-bottom:10px;" class="ui-state-default">
                        @Html.Hidden("RoleCode", PMGSY.Extensions.PMGSYSession.Current.RoleCode.ToString())
                        <table style="margin-top:15px; margin-bottom:10px; padding:0.2rem">
                            <tr>
                                <td>
                                    @Html.LabelFor(model => model.MAST_STATE_CODE)
                                </td>
                                <td>
                                    @if (PMGSY.Extensions.PMGSYSession.Current.RoleCode == 25 || PMGSY.Extensions.PMGSYSession.Current.RoleCode == 5 || PMGSY.Extensions.PMGSYSession.Current.RoleCode == 9)
                                    {
                                        @Html.DropDownListFor(model => model.MAST_STATE_CODE, new SelectList(Model.STATES, "Value", "Text"), new { @id = "ddlStates" })
                                        @Html.ValidationMessageFor(model => model.MAST_STATE_CODE)
                                    }
                                    else if (PMGSY.Extensions.PMGSYSession.Current.RoleCode == 2)
                                    {
                                        @Html.DropDownListFor(model => model.MAST_STATE_CODE, new SelectList(Model.STATES, "Value", "Text", Model.MAST_STATE_CODE), new { @id = "ddlState", @disabled = "disabled" })
                                        @*@Html.TextBoxFor(model => model.MAST_STATE_NAME, new { @id = "ddlState", @readonly = "readonly" })*@
                                    }
                                </td>

                                <td style="width: 9%"></td>

                                <td>
                                    @Html.LabelFor(model => model.MAST_DISTRICT_CODE)
                                </td>
                                <td>
                                    @Html.DropDownListFor(model => model.MAST_DISTRICT_CODE, new SelectList(Model.DISTRICTS, "Value", "Text"), new { @id = "ddlDistrict" })
                                    @Html.ValidationMessageFor(model => model.MAST_DISTRICT_CODE)

                                </td>

                                <td style="width: 9%"></td>

                                <td>
                                    @Html.LabelFor(model => model.MAST_BLOCK_CODE)
                                </td>
                                <td>
                                    @Html.DropDownListFor(model => model.MAST_BLOCK_CODE, new SelectList(Model.BLOCKS, "Value", "Text"), new { @id = "ddlBlock" })
                                    @Html.ValidationMessageFor(model => model.MAST_BLOCK_CODE)

                                </td>
                            </tr>

                            <tr>
                                <td>
                                    @Html.LabelFor(model => model.IMS_YEAR)
                                </td>
                                <td>
                                    @Html.DropDownListFor(model => model.IMS_YEAR, new SelectList(Model.Years, "Value", "Text"), new { @id = "ddlYear" })
                                    @Html.ValidationMessageFor(model => model.IMS_YEAR)
                                </td>

                                <td style="width: 9%"></td>

                                <td>
                                    @Html.LabelFor(model => model.IMS_BATCH)
                                </td>
                                <td>
                                    @Html.DropDownListFor(model => model.IMS_BATCH, new SelectList(Model.BATCHS, "Value", "Text"), new { @id = "ddlBatch" })
                                    @Html.ValidationMessageFor(model => model.IMS_BATCH)

                                </td>

                                <td style="width: 9%"></td>

                                <td>
                                    @Html.LabelFor(model => model.PMGSY_SCHEME)
                                </td>
                                <td>
                                    @Html.DropDownListFor(model => model.PMGSY_SCHEME, new SelectList(Model.SCHEMELIST, "Value", "Text"), new { @id = "ddlScheme" })
                                    @Html.ValidationMessageFor(model => model.PMGSY_SCHEME)

                                </td>
                            </tr>

                            <tr>

                                <td>
                                    @Html.LabelFor(model => model.IMS_PROPOSAL_TYPE)
                                </td>
                                <td>
                                    @Html.DropDownListFor(model => model.IMS_PROPOSAL_TYPE, new SelectList(Model.PROPOSAL_TYPES, "Value", "Text"), new { @id = "ddlPropType" })
                                    @Html.ValidationMessageFor(model => model.IMS_PROPOSAL_TYPE)

                                </td>
                                <td></td>
                                <td>
                                    <input type="button" class="jqueryButton" value="List Details" id="btnListDetails" name="btnListDetails" title="Click here to list Details" />
                                </td>
                            </tr>

                        </table>

                    </div>
                </form>
            </center>
        </div>

    </div>
    <div id="divListExecution" style="margin-top:20px; width:100%">
        <table id="tbExecutionList"></table>
        <div id="pagerExecution"></div>
    </div>

    <div id="divListExecutionUploaded" style="margin-top:20px; width:100%">
        <table id="tbExecutionListUploaded"></table>
        <div id="pagerExecutionUploaded"></div>
    </div>

</center>





<center>
    <div id="divAddInspByNRIDA" style="width:100%; margin-top:2%"></div>
</center>





