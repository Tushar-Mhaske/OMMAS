@using MvcReportViewer
@model PMGSY.Areas.AccountReports.Models.RegisterOfWorksModel

<div id="dvRegisterOfWorkReport" style="width: 100%; height: 590px; overflow-x: hidden; overflow-y: hidden">
    @Html.MvcReportViewer("/PMGSYCitizen/RegisterOfWork",
     new
     {
         PrmFundType = PMGSY.Extensions.PMGSYSession.Current.FundType,
         PrmAdminNdCode = Model.ADMIN_ND_CODE,
         PrmConID = Model.MAST_CON_ID,
         PrmAgrCode = Model.TEND_AGREEMENT_CODE,
         NameOfAgency = Model.StateDepartment,
         NameOfDPIU = Model.DistrictDepartment,
         AgreementNumber = Model.AGREEMENT_NUMBER,
         AgreementDate = Model.AGREEMENT_DATE,
         AgreementAmount = Model.AGREEMENT_AMOUNT,
         NameOfContractor=Model.ContractorName
     },
     new { Height = 600, Width = 1201, style = "border:none;" })
</div>
