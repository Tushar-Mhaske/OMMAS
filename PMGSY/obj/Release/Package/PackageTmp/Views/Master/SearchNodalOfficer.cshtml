<script src="~/Scripts/custom/Generic.js" type="text/javascript"></script>
<script src="~/Scripts/Master/SearchNodalOfficer.js" type="text/javascript"></script>
<div id="dvhdSearch" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    @*margin-left:10px;margin-right:10px;margin-top:10px;*@
    <span class="ui-jqgrid-title" style="float: left">&nbsp;Search</span>
    <span id="spCollapseIconS" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>
<div id="dvSearchParameter" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">@*margin-left: 10px; margin-right: 10px;*@

    <form id="frmSearchQualityMonitor">
        @Html.Hidden("hdnStateCode", PMGSY.Extensions.PMGSYSession.Current.StateCode.ToString())
        @Html.Hidden("RoleCode", PMGSY.Extensions.PMGSYSession.Current.RoleCode.ToString())
        <table style="width: 100%;" class="rowstyle">
            <tr>
                <td style="width: 2%"></td>
                <td style="width: 10%">
                    @Html.Label("State")
                </td>
                <td style="width: 14%">
                    @Html.DropDownList("ddlSearchState", ViewData["STATE"] as IEnumerable<SelectListItem>, new { @title = "State", @tabindex = 1 })
                </td>

                <td style="text-align: left; width: 10%;">
                    @Html.Label("Office")

                </td>
                <td style="width: 14%;">
                    @Html.DropDownList("ddlSearchOffice", ViewData["NdNameList"] as IEnumerable<SelectListItem>, new { @title = "Office", @tabindex = 2 })
                </td>
                <td style="text-align: left; width: 7%;">
                    @Html.Label("Designation")
                </td>
                <td style="width: 14%;">
                    @Html.DropDownList("ddlSearchDesignation", ViewData["DesignationList"] as IEnumerable<SelectListItem>, new { @id = "ddlSearchDesignation", @title = "Designation", @tabindex = 3 })
                </td>
            </tr>
            <tr>
                <td style="width: 2%"></td>
                <td style="text-align: left; width: 10%;">
                    @Html.Label("Profile Type")
                </td>
                <td style="width: 15%;">
                    @Html.DropDownList("ddlSearchNOType", ViewData["NOType"] as IEnumerable<SelectListItem>, new { @id = "ddlSearchNOType", @title = "Profile Type", @tabindex = 4 })
                </td>
                <td style="text-align: left; width: 10%;">
                    @Html.Label("Active Status")
                </td>
                <td style="width: 14%;">
                    @Html.DropDownList("ddlSearchActive", ViewData["Active"] as IEnumerable<SelectListItem>, new { @id = "ddlSearchActive", @title = "Status Active", @tabindex = 5 })
                </td>
                <td>
                    <input type="button" id="btnSearch" name="btnSearch" value="Search" class="jqueryButton" title="Search Nodal Officer " tabindex="6" />

                </td>
                <td></td>
            </tr>
            @*  <tr>
                <td style="width: 5%"></td>
               
                <td style="text-align: left; width: 10%;">
                    @Html.Label("Profile Type")
                </td>
                <td style="width: 15%;">
                    @Html.DropDownList("ddlSearchNOType", ViewData["NOType"] as IEnumerable<SelectListItem>, new { @id = "ddlSearchNOType", @title = "Profile Type", @tabindex = 4 })
                </td>
                 <td style="width: 10%">
                    @Html.Label("Module Type")
                </td>
                <td style="width: 15%">
                    @Html.DropDownList("ddlSearchState", ViewData["ModuleType"] as IEnumerable<SelectListItem>, new {@id = "ddlModuleType", @title = "Module Type", @tabindex = 5 })
                </td>
                <td>
                    <input type="button" id="btnSearch" name="btnSearch" value="Search" class="jqueryButton" title="Search Nodal Officer " tabindex="6" />

                </td>
                <td></td>
            </tr>*@
        </table>

    </form>
</div>

