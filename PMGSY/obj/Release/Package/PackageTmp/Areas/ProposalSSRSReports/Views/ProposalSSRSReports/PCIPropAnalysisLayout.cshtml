@model PMGSY.Areas.ProposalSSRSReports.Models.PCIAnalysisViewModel

@if (Layout == null)
{
    Layout = Request.IsAjaxRequest() ? null : "~/Areas/AccountReports/Views/Account/_Layout.cshtml";
}

<script type="text/javascript" src="@Url.Content("~/Scripts/jquery.ui.dialog.js")"> </script>
<script src="~/Areas/ProposalSSRSReports/Scripts/PCIProposalAnalysisLayout.js"></script>
<script src="~/Scripts/custom/Generic.js"></script>

<div id="dvhdPCIPropAnalysisLayout" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;PCI Analysis - PMGSY Proposal</span>

    <span id="spCollapseIconCN" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>
<div id="dvPCIPropAnalysisLayout" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">

    <form id="frmPCIPropAnalysisLayout">
        @Html.HiddenFor(model => model.State)
        @Html.HiddenFor(model => model.District)
        @Html.HiddenFor(model => model.Block)
        @*@Html.HiddenFor(model => model.Route)*@
        @Html.HiddenFor(model => model.flag)
        @Html.HiddenFor(model => model.StateName)
        @Html.HiddenFor(model => model.DistName)
        @Html.HiddenFor(model => model.BlockName)
        @Html.HiddenFor(model => model.RouteName)
        <div style="width: 100%">
            <table class="rowstyle" style="width: 100%;">
                <tr>
                    <td style="width: 6%;"></td>
                    <td style="text-align: left; width: 5%;">
                        @Html.Label("Route")
                    </td>
                    <td style="width: 15%;">
                        @*@Html.DropDownList("ddRoute_PCIAbstractAnalyisDetails", ViewData["ROUTE"] as IEnumerable<SelectListItem>, new { @title = "Route", @tabindex = 1 })*@
                        @Html.DropDownListFor(model => model.Route, new SelectList(Model.RouteList, "Value", "Text"), new { @id = "ddRoute_PCIPropAbstractAnalyisDetails", @title = "Route", @tabindex = 1 })
                        <br />
                        @Html.ValidationMessageFor(m => m.Route)
                    </td>
                    <td>
                        <input type="button" value="View" class="jqueryButton" id="btnPCIPropAbstractAnalyisDetails" title="View Details" tabindex="2" />
                    </td>
                </tr>
            </table>
        </div>
    </form>
</div>
<div id="dvLoadPCIPropAnalysisReport">
</div>
