<div id="dvhdSearch_Mapped" class="ui-corner-top ui-widget-header panelheader" style="text-align:left; margin-left:0px;margin-right:0px;margin-top:10px; height:20px;"> 
     <span class="ui-jqgrid-title" style="float:left">&nbsp;Mapped Habitations </span>
     
      <span  id="spCollapseIconS_Mapped" class="ui-icon ui-icon-circle-triangle-n" style="float:right"></span>
      </div>

<div id="dvSearchParameter_Mapped" class="ui-corner-bottom" style="margin-left: 0px; margin-right: 0px; display: block; border: 1px solid #B5B8C8; ">
    
   <form id="frmSearchMappedPanchayatHabitations">
         @if (!(string.IsNullOrEmpty(ViewBag.EncryptedPanchayatCode_Mapped)))
                    {
                     @Html.Hidden("EncryptedPanchayatCode_Mapped",ViewBag.EncryptedPanchayatCode_Mapped as String)     
                    }
         <table style="width:100%;" class="rowstyle"  >
         
               

            <tr>
                <td  style="width: 2%;"></td>

                <td  style="width: 10%;">
                    @Html.Label("State Name")

                </td>
                <td  style="width: 20%;">
                    @if (!(string.IsNullOrEmpty(ViewBag.StateName)))
                    {
                        <b>@Html.Label("StateName", ViewBag.StateName as String)   </b>
                    }
                    else
                    {
                        <b>@Html.Label("StateName", "NA")   </b>
                    }

                </td>

                <td  style="width: 10%;">
                    @Html.Label("District Name")

                </td>
                <td  style="width: 20%;">
                    @if (!(string.IsNullOrEmpty(ViewBag.DistrictName)))
                    {
                        <b>@Html.Label("DistrictName", ViewBag.DistrictName as String)   </b>
                    }
                    else
                    {
                        <b>@Html.Label("DistrictName", "NA")   </b>
                    }

                </td>

            </tr>
            <tr>
                <td  style="width: 2%;"></td>
                <td>
                    @Html.Label("Block Name")

                </td>
                <td  style="width: 10%;">
                    @if (!(string.IsNullOrEmpty(ViewBag.BlockName)))
                    {
                        <b>@Html.Label("BlockName", ViewBag.BlockName as String)   </b>
                    }
                    else
                    {
                        <b>@Html.Label("BlockName", "NA")   </b>
                    }

                </td>


                <td  style="width: 10%;">
                    @Html.Label("Panchayat Name")

                </td>
                <td  style="width: 20%;">
                    @if (!(string.IsNullOrEmpty(ViewBag.PanchayatName)))
                    {
                        <b>@Html.Label("PanchayatName", ViewBag.PanchayatName as String)   </b>
                    }
                    else
                    {
                        <b>@Html.Label("PanchayatName", "NA")   </b>
                    }

                </td>

            </tr>
         

              
         </table>

   </form>

  

    
