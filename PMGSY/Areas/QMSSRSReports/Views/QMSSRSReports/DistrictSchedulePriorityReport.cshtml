@using MvcReportViewer
@model PMGSY.Areas.QMSSRSReports.Models.SchedulePriorityViewModel


@Html.MvcReportViewer("/PMGSYCitizen/QMSchedulePriorityDistrict",
new
{
    PRIORITY = Model.priority,
    ProgMax = Model.ProgressMax,
    ProgMin = Model.ProgressMin,
    CompMax = Model.CompletedMax,
    CompMin = Model.CompletedMin,
    STATE=Model.StateCode,
    TMONTH=Model.ToMonth,
    FMONTH=Model.FromMonth
         
},
new { Height = 600, Width = "100%", style = "border:none;" })