@model PMGSY.Areas.EFORMArea.Model.PdfPageViewModel
@{
    ViewBag.Title = "Index";
}

@Styles.Render("~/Content/css")




<script src="~/Areas/EFORMArea/Scripts/EformQMscirpt.js"></script>
<div>
    @{
        <div id="spanDiv" style="width:100%; height:20px;border:none; margin-top:5px; margin-bottom:-15px; " class="ui-corner-top ui-widget-header panelheader">
            <span style="float:right;margin-right:2Px; margin-top:2px" id="idFilterDiv" class="ui-icon ui-icon-circle-triangle-n"></span>
        </div>
        <div id="divSearchExecution">
            <div style="width: 98%; border:none; margin-top:20px; margin-bottom:10px;" class="ui-state-default">
                <center>

                    <form id="frmEformIndex">
                        @Html.ValidationSummary(true)
                        @Html.Hidden("RoleCode", PMGSY.Extensions.PMGSYSession.Current.RoleCode)

                        <div style="width: 98%; border:none; margin-top:10px; margin-bottom:10px;" class="ui-state-default">

                            <table style="margin-top: 0px; margin-bottom: 0px; width: 100%;" class="rowstyle">
                                <tr>
                                    <td style="width: 1.5%"></td>



                                    <td style="text-align:right">
                                        @Html.Label("State: ") @*<span style="color: red">*</span> *@
                                    </td>
                                    @if (Model.RoleCode == 22 || Model.RoleCode == 7 || Model.RoleCode == 38 || Model.RoleCode == 54 || Model.RoleCode == 8 || Model.RoleCode == 48 || Model.RoleCode == 69)
                                    {
                                        if (Model.RoleCode == 7 && Model.isInterstateSQM == "Y")
                                        {
                                            <td>
                                                @Html.DropDownListFor(model => model.stateCode, new SelectList(Model.lstState, "Value", "Text"), new { @id = "ddlstate", @tabindex = 1 })
                                                <br />
                                                @Html.ValidationMessageFor(model => model.stateCode)
                                            </td>
                                        }
                                        else
                                        {
                                            @Html.HiddenFor(model => model.stateCode, new { @id = "ddl1state", @tabindex = 1 })
                                            <td>
                                                @Html.DisplayFor(m => m.StateName)
                                            </td>
                                        }
                                    }
                                    else if (Model.RoleCode == 6 || Model.RoleCode == 9 || Model.RoleCode == 5)
                                    {

                                        <td>
                                            @Html.DropDownListFor(model => model.stateCode, new SelectList(Model.lstState, "Value", "Text"), new { @id = "ddlstate", @tabindex = 1 })
                                            <br />
                                            @Html.ValidationMessageFor(model => model.stateCode)
                                        </td>

                                    }

                                    <td style="width: 1.5%"></td>
                                    <td style="text-align:right">
                                        @Html.Label("District: ") @*<span style="color: red">*</span>*@
                                    </td>

                                    @if (Model.RoleCode == 22 || Model.RoleCode == 38 || Model.RoleCode == 54)
                                    {

                                        @Html.HiddenFor(model => model.districtCode, new { @id = "ddl1district", @tabindex = 2 })
                                        <td>
                                            @Html.DisplayFor(m => m.DistrictName)
                                        </td>
                                    }
                                    else if (Model.RoleCode == 6 || Model.RoleCode == 7 || Model.RoleCode == 9 || Model.RoleCode == 5 || Model.RoleCode == 8 || Model.RoleCode == 48 || Model.RoleCode == 69)
                                    {
                                        <td>
                                            @Html.DropDownListFor(model => model.districtCode, new SelectList(Model.lstDistrict, "Value", "Text"), new { @id = "ddldistrict", @tabindex = 2 })
                                            <br />
                                            @Html.ValidationMessageFor(x => x.districtCode)
                                        </td>

                                    }
                                    <td style="width: 1.5%"></td>
                                    <td style="text-align:right">
                                        @Html.Label("Month: ") @*<span style="color: red">*</span>*@
                                    </td>
                                    <td>
                                        @Html.DropDownListFor(model => model.MonthCode, new SelectList(Model.lstMonth, "Value", "Text"), new { @id = "ddlmonth", @tabindex = 4, style = "width: 110px;" })
                                        <br />
                                        @Html.ValidationMessageFor(model => model.MonthCode)
                                    </td>
                                    <td style="width: 1.5%"></td>


                                    <td style="text-align:right">
                                        @Html.Label("Year: ") @*<span style="color: red">*</span>*@
                                    </td>
                                    <td>
                                        @Html.DropDownListFor(model => model.YearCode, new SelectList(Model.lstYear, "Value", "Text"), new { @id = "ddlyear", @tabindex = 3, style = "width: 110px;" })
                                        <br />
                                        @Html.ValidationMessageFor(model => model.YearCode)
                                    </td>

                                    @*added e-form type here*@
                                    <td style="width: 1.5%"></td>
                                    <td style="text-align:right">
                                        @Html.LabelFor(model => model.eFormType) @*<span style="color: red">*</span>*@
                                    </td>
                                    <td style="text-align:right">
                                    <td>
                                        @Html.DropDownListFor(model => model.Eform_Type, new SelectList(Model.Eform_Type, "Value", "Text"), new { @id = "ddleformType", @tabindex = 4, style = "width: 110px;" })
                                        <br />
                                        @Html.ValidationMessageFor(model => model.Eform_Type)
                                    </td>


                                    <td style="width: 1.5%"></td>
                                    <td style="text-align:right">
                                        @Html.Label("e-Form Status: ") @*<span style="color: red">*</span>*@
                                    </td>

                                    @if (Model.RoleCode == 22 || Model.RoleCode == 38 || Model.RoleCode == 54)
                                    {
                                        <td>
                                            @Html.DropDownListFor(model => model.Eform_Status, new SelectList(Model.lstEformStatus, "Value", "Text"), new { @id = "ddlStatus", @tabindex = 5, style = "width: 110px;" })

                                        </td>
                                    }
                                    @if (Model.RoleCode == 6 || Model.RoleCode == 7 || Model.RoleCode == 9 || Model.RoleCode == 5 || Model.RoleCode == 8 || Model.RoleCode == 48 || Model.RoleCode == 69)
                                    {
                                        <td>
                                            @Html.DropDownListFor(model => model.Eform_Status, new SelectList(Model.lstEformStatus, "Value", "Text"), new { @id = "ddlStatusQM", @tabindex = 5, style = "width: 110px;" })

                                        </td>
                                    }

                                    <td style="width: 1.5%"></td>
                                    @if (Model.RoleCode == 22 || Model.RoleCode == 38 || Model.RoleCode == 54)
                                    {

                                        <td>
                                            @*<input type="button" class="jqueryButton" value="Get List" title="Get beneficiary List" id="btnGetBeneficiaryList" name="btnGetBeneficiaryList" tabindex="3" />*@
                                            <input type="button" class="jqueryButton" value="Get List" title="Get Work List" id="btnGetPIUWorkList" name="btnGetPIUWorkList" tabindex="6" />

                                        </td>
                                    }
                                    @if (Model.RoleCode == 6 || Model.RoleCode == 7 || Model.RoleCode == 9 || Model.RoleCode == 5 || Model.RoleCode == 8 || Model.RoleCode == 48 || Model.RoleCode == 69)
                                    {
                                        <td>
                                            @*<input type="button" class="jqueryButton" value="Get List" title="Get beneficiary List" id="btnGetBeneficiaryList" name="btnGetBeneficiaryList" tabindex="3" />*@
                                            <input type="button" class="jqueryButton" value="Get List" title="Get Work List" id="btnGetQMWorkList" name="btnGetQMWorkList" tabindex="6" />

                                        </td>
                                    }

                                    <td style="width: 1.5%"></td>
                                </tr>
                            </table>



                        </div>
                    </form>
                </center>
            </div>
        </div>


        <div id="adobedwnldDiv1" style="display:none">
            @if (Model.RoleCode != 9 && Model.RoleCode != 5)
            {
                <label style="color: blue; font-size: 13px;   background-color: #fbfaf7; text-align: left;  "> Note: Fill the e - Form Pdf in Adobe acrobat reader dc only and then upload.Link to download Adobe acrobat reader dc -</label> <a type='button' href='//get.adobe.com/reader/' title='Click here to Download Adobe acrobat reader dc' id='btnDownload' value='Download' class='jqueryButton ui-corner-all ui-button-text-only' target=_blank><input type='button' value='Download' /> </a>
            }
        </div>


        <div id="divListExecutionQM" style="margin-top:20px; width:100%">
            <table id="tbWorkList"></table>
            <div id="pagerExecutionQM"></div>
        </div>

        <br />
        <br />
        <div id="adobedwnldDiv" style="display: none; width: 100%; border: 0.5px solid #cfcac1; border-radius: 7px ">
            <div style="color: red; background-color: #fccc6b; padding: 2px; color: #880808; font-size: medium; border-top-left-radius: 7px; border-top-right-radius: 7px"> &nbsp; Download Adobe acrobat reader dc <span style="float: right; margin-right: 5px;" id="closeErrorSummaryadobedwnldDiv" class="ui-icon ui-icon-closethick"></span></div>


            <label style="color: red; font-size: 17px; background-color: #fbfaf7; text-align: left; height: 250px; margin-left:10px"> Please Fill the downloaded e - Form Pdf in adobe acrobat reader dc and then upload.Link to download Adobe acrobat reader dc - </label>
            <a type='button' href='//get.adobe.com/reader/' title='Click here to Download Adobe acrobat reader dc' id='btnDownload' value='Download' class='jqueryButton ui-corner-all ui-button-text-only' target=_blank><input type='button' value='Download' /> </a>
        </div>

        <br />
        <br />

        <div id="errSummary" style="display: none; width: 100%; border: 0.5px solid #cfcac1; border-radius: 7px">
            <div style="color: red; background-color: #fccc6b; padding: 2px; color: #880808; font-size: medium; border-top-left-radius: 7px; border-top-right-radius: 7px"> &nbsp; Error Summary <span style="float: right; margin-right: 5px;" id="closeErrorSummary" class="ui-icon ui-icon-closethick"></span></div>

            <h3 style="text-align: left;margin-top:10px; color: #880808;padding:3px; margin-left:10px">Below are the errors generated from the uploaded e-Form file,please correct below errors & upload again:</h3>
            <div id="ErroMsg" style="overflow: scroll; color: red; font-size: 14px; padding: 20px; background-color: #fbfaf7; text-align: left; height: 250px"></div>
        </div>
    }
</div>






