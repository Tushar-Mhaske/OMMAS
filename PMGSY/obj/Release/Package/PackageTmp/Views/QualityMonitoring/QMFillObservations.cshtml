                        <td style="width:10%;">
                            @Html.DisplayFor(model => model.SCHEDULE_MONTH_YEAR)
                        </td>
                        <td style="width:10%;font-weight:bold;">
                            @Html.LabelFor(model => model.IMS_YEAR)
                        </td>
