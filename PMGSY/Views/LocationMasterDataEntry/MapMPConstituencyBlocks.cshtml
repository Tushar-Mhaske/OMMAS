<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/MasterDataEntry/Master_MapMPConstituencyBlocks.js")"> </script>

<div id="dvhdSearch_Map" class="ui-corner-top ui-widget-header panelheader" style="text-align:left; margin-left:0px;margin-right:0px;margin-top:10px; height:20px;"> 
     <span class="ui-jqgrid-title" style="float:left">&nbsp;Map Blocks </span>
     
      <span  id="spCollapseIconS_Map" class="ui-icon ui-icon-circle-triangle-n" style="float:right"></span>
      </div>

<div id="dvSearchParameter_Map" class="ui-corner-bottom" style="margin-left: 0px; margin-right: 0px; display: block; border: 1px solid #B5B8C8; ">
    
   <form id="frmSearchMapMPConstituencyBlock">

         <table style="width:100%;" class="rowstyle"  >
             <tr>
                 <td colspan="4">
                      @Html.Hidden("EncryptedBlockCodes")  
                      @Html.Hidden("MPConstituencyCode")

                 </td>

             </tr>     
             
                  <tr>

                  
                      <td style="width: 25%;">
                    @Html.Label("State Name")

                </td>
                <td style="width: 25%;">
                    @if (!(string.IsNullOrEmpty(ViewBag.StateName)))
                    {
                        <b>@Html.Label("StateName", ViewBag.StateName as String)   </b>
                    }
                    else
                    {
                        <b>@Html.Label("StateName", "NA")   </b>
                    }

                </td>
                 <td style="width: 25%;">
                                         @Html.Label("MP Constituency Name")

                 </td>
                  <td style="width: 25%;">
                    @if (!(string.IsNullOrEmpty(ViewBag.MPConstituencyName)))
                    {
                    <b>  @Html.Label("MPConstituencyName", ViewBag.MPConstituencyName as String)   </b>
                    }
                    else
                    {
                     <b>  @Html.Label("MPConstituencyName","NA")   </b>
                    }
                    
                  </td>

             </tr>     

             <tr>
                <td style="width:25%;">
                    @if (!(string.IsNullOrEmpty(ViewBag.EncryptedStateCode)))
                    {
                     @Html.Hidden("EncryptedStateCode",ViewBag.EncryptedStateCode as String)     
                    }
                  
                    
                 </td>
                <td style="text-align:left;width:25%;" >
                    @Html.Label("District")
                </td>
              
                
                <td style="width:25%;">
                                          
                     @Html.DropDownList("ddlSearchDistrict", ViewData["DistrictList"] as IEnumerable<SelectListItem>, new {@id="ddlSearchDistrict", @title = "District", @tabindex = 1 })
                                     
                </td>
                      <td style="width: 25%;"><input type="button" id="btnSearch_Map" name="btnSearch_Map" value="Search" class= "jqueryButton" title="Search Block  " tabindex="2"  /> 

                      </td>
            </tr>
         
         </table>

   </form>

  

    
<br />

     <div style="width:95%;">
    <table id="tbMapMPConstituencyBlockList">

    </table>

    <div id="dvMapMPConstituencyBlockListPager"></div>

         </div>
<br />
  <table style="width:100%;" class="rowstyle"  >
      <tr>


      </tr>            
      <tr>
                      <td style="text-align:center">

                          <input type="button" id="btnMapConstituency" name="btnMapConstituency" value="Map Blocks" class= "jqueryButton" title="Map Blocks" tabindex="3"  /> 
                            <input type="button" id="btnMapCancel" name="btnMapCancel" value="Back" class= "jqueryButton" title="Back" tabindex="4"  /> 
                      </td>
                      </tr>
      <tr></tr>
      </table>

    </div>