@model PMGSY.Areas.EFORMArea.QM_ViewPdf_Model.QM_ViewPdfModel

<!DOCTYPE html>
<html lang="en">

<head>
    <title>QM PDF View</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="~/Content/Bootstrap/bootstrap.min.css" />
    <script src="~/Scripts/Bootstrap/bootstrap.min.js"></script>    
</head>
<style>

    /*=========== Common Stylesheet ============*/

    input[type="text"] {
        font-size: 22px;
    }
       
    /* inputbox width for values like codes,length,dates, */
    .inputCodeWidth {
        width: 100px;
        min-height: 32px;
    }

    .inputShortWidth {
        width: 150px;
        min-height: 32px;
    }

    .inputMediumWidth {
        width: 250px;
        min-height: 32px;
    }

    .inputMidlLargeWidth {
        width: 400px;
        min-height: 32px;
    }

    .inputLargeWidth {
        width: 700px;
        min-height: 32px;
    }

    .SectionUnderline {
        border: 0.5px solid gray;
    }

    .PageBreakLine {
        border: 3px solid gray;
        margin-top: 35px;
        margin-bottom: 35px;
    }
    .table-borderless td {
        border: none;
    }

    .table-bordered thead th {
        vertical-align: top;
        text-align: center;
        background-color: #8c8c8c24;
    }

    .table-hover tbody tr:hover td {
        background-color: #d5bcbc24;
    }

    /*=========== QM Page CSS ===========*/

    .fontQMPDF {
        font-size: 22px;
        font-family: Calibri;
    }
    /* 2. QUALITY ARRANGEMENTS-OBSERVATIONS Tables */
    .tab1_2Pt td {
        width: 80%;
    }

    /* 3. ATTENTION TO QUALITY */
    .tab1_3Pt td {
        width: 80%;
    }

    .Applicablechecks {
        font-size: large;
        font-style: normal;
        text-align: center;
        background-color: #ffefc6a1;
        padding: 5px;
    }
       
</style>

<body>
    <div class="container-fluid" style="background-color:lightgray">
        <div class="container-fluid mt-6" style="width: 90%; background-color: #fff; margin-top: 50px; margin-bottom: 50px; margin-left:60px">

            <!-- 1. General Section -->
            <div id="GeneralDetails">
                <div class="container-fluid" id="1to8_Points">
                    <!-- HEADLINES -->
                    <center>
                        <h1>National Rural Infrastructure Development Agency</h1>
                        <h2>Ministry of Rural Development, Government of India</h2>
                        <h4 style="text-align:left;"> FORM ID <input type="text" value="@Model.PrefillDetailsViewModel.EFORM_ID" class="inputShortWidth" disabled> <span style="float:right;"> Format QR Version 2021.0- Part II </span>   </h4>
                        <h3>Report of National Quality Monitor (NQM)/State Quality Monitors (SQM) </h3>
                        <h3> <u><b>PART II– Observations of NQM/SQM for Ongoing/Completed Work (To be filled-up by QM)</b></u> </h3>
                        <h3><i>(To be filled-up by QM)</i></h3>
                    </center>

                    <h3><b>1. GENERAL DETAILS </b></h3>
                    <hr style="border: 0.5px solid gray">

                    <table class="table table-hover table-borderless fontQMPDF">
                        <tr>
                            <td> I. Date of Inspection </td>
                            <td> <input type="text" value="@Model.GeneralDetailsViewModel.INSPECTION_DATE" data-toggle="tooltip" data-placement="bottom" title="Date of Inspection" class="inputShortWidth" disabled> </td>
                        </tr>

                        <tr>
                            @if (@Model.PrefillDetailsViewModel.QM_TYPE == null)
                            {
                                <td colspan="2"> II. Name of Quality Monitor (QM)</td>
                                <td>
                                    <input type="text" value="@Model.PrefillDetailsViewModel.QM_NAME" data-toggle="tooltip" data-placement="bottom" title="Name of Quality Monitor (QM)" class="inputMidlLargeWidth" disabled>
                                    &emsp;&emsp; <input type="checkbox" disabled> NQM &emsp; <input type="checkbox" disabled> SQM
                                </td>
                            }
                            else if (@Model.PrefillDetailsViewModel.QM_TYPE.Equals("I"))
                            {
                                <td colspan="2"> II. Name of Quality Monitor (QM)</td>
                                <td>
                                    <input type="text" value="@Model.PrefillDetailsViewModel.QM_NAME" data-toggle="tooltip" data-placement="bottom" title="Name of Quality Monitor (QM)" class="inputMidlLargeWidth" disabled>
                                    &emsp;&emsp; <input type="checkbox" checked disabled> NQM &emsp; <input type="checkbox" disabled> SQM
                                </td>
                            }
                            else if (@Model.PrefillDetailsViewModel.QM_TYPE.Equals("S"))
                            {
                                <td> II. Name of Quality Monitor (QM)</td>
                                <td colspan="2">
                                    <input type="text" value="@Model.PrefillDetailsViewModel.QM_NAME" data-toggle="tooltip" data-placement="bottom" title="Name of Quality Monitor (QM)" class="inputMidlLargeWidth" disabled>
                                    &emsp;&emsp; <input type="checkbox" disabled> NQM &emsp; <input type="checkbox" checked disabled> SQM
                                </td>
                            }

                        </tr>
                        <tr>
                            <td colspan="2">III. State &emsp;&emsp;&emsp; &emsp;&emsp;&emsp; <input type="text" value="@Model.PrefillDetailsViewModel.STATE" data-toggle="tooltip" data-placement="bottom" title="State" disabled></td>
                            <td>
                                District &emsp; <input type="text" value="@Model.PrefillDetailsViewModel.DISTRICT" data-toggle="tooltip" data-placement="bottom" title="District" disabled>
                                &emsp;&emsp;&emsp;
                                Block <input type="text" value="@Model.PrefillDetailsViewModel.BLOCK" data-toggle="tooltip" data-placement="bottom" title="Block" disabled>
                            </td>
                        </tr>

                        <tr>
                            <td colspan> IV. Name of Road </td>
                            <td colspan="2">
                                <div class="grow-wrap border border-gray text-wrap inputLargeWidth" style="background-color:#eee;color:#808080" data-toggle="tooltip" data-placement="bottom" title="Name of Road">
                                    <p class="fontQMPDF px-2">
                                        @Model.PrefillDetailsViewModel.ROAD_NAME
                                    </p>
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td> V. Package Number </td>
                            <td colspan="2"><input type="text" value="@Model.PrefillDetailsViewModel.PACKAGE_ID" data-toggle="tooltip" data-placement="bottom" title="Package Number" class="inputMediumWidth" disabled> </td>
                        </tr>
                        <tr>
                            <td> VI. RD of Inspection </td>
                            <td> From RD &emsp13; <input type="text" value="@Model.GeneralDetailsViewModel.ROAD_FROM" data-toggle="tooltip" data-placement="bottom" title="From RD" class="inputShortWidth" disabled> KM </td>

                            <td> To RD &emsp13; <input type="text" value="@Model.GeneralDetailsViewModel.ROAD_TO" data-toggle="tooltip" data-placement="bottom" title="To RD" class="inputShortWidth" disabled> KM </td>
                        </tr>

                        @if (Model.PrefillDetailsViewModel.CURRENT_STAGE.Equals("1"))
                        {
                            <tr>
                                <td rowspan="3"> VII. Current Stage of Work </td>
                                <td> <input type="checkbox" checked disabled> Stage-I construction </td>
                            </tr>
                            <tr>
                                <td> <input type="checkbox" disabled> Stage-II construction</td>
                            </tr>
                            <tr>
                                <td> <input type="checkbox" disabled> Complete construction</td>
                            </tr>
                        }
                        else if (Model.PrefillDetailsViewModel.CURRENT_STAGE.Equals("2"))
                        {
                            <tr>
                                <td rowspan="3"> VII. Current Stage of Work </td>
                                <td> <input type="checkbox" disabled> Stage-I construction </td>
                            </tr>
                            <tr>
                                <td> <input type="checkbox" checked disabled> Stage-II construction</td>
                            </tr>
                            <tr>
                                <td> <input type="checkbox" disabled> Complete construction</td>
                            </tr>
                        }
                        else if (Model.PrefillDetailsViewModel.CURRENT_STAGE.Equals("3"))
                        {
                            <tr>
                                <td rowspan="3"> VII. Current Stage of Work </td>
                                <td> <input type="checkbox" disabled> Stage-I construction </td>
                            </tr>
                            <tr>
                                <td> <input type="checkbox" disabled> Stage-II construction</td>
                            </tr>
                            <tr>
                                <td> <input type="checkbox" checked disabled> Complete construction</td>
                            </tr>
                        }
                        else
                        {
                            <tr>
                                <td rowspan="3"> VII. Current Stage of Work </td>
                                <td> <input type="checkbox" disabled> Stage-I construction </td>
                            </tr>
                            <tr>
                                <td> <input type="checkbox" disabled> Stage-II construction</td>
                            </tr>
                            <tr>
                                <td> <input type="checkbox" disabled> Complete construction</td>
                            </tr>
                        }

                        @if (Model.PrefillDetailsViewModel.PHYSICAL_WORK_STATUS == null)
                        {
                            <tr>
                                <td>VIII. Physical status of work </td>
                                <td>
                                    <input type="checkbox" disabled> Ongoing
                                </td>
                                <td>
                                    <input type="checkbox" disabled> Completed
                                </td>
                            </tr>
                        }
                        else if (Model.PrefillDetailsViewModel.PHYSICAL_WORK_STATUS.Equals("P") || Model.PrefillDetailsViewModel.PHYSICAL_WORK_STATUS.Equals("M"))
                        {
                            <tr>
                                <td>VIII. Physical status of work </td>
                                <td>
                                    <input type="checkbox" checked disabled> Ongoing
                                </td>
                                <td>
                                    <input type="checkbox" disabled> Completed
                                </td>
                            </tr>
                        }
                        else if (Model.PrefillDetailsViewModel.PHYSICAL_WORK_STATUS.Equals("C"))
                        {
                            <tr>
                                <td>VIII. Physical status of work </td>
                                <td>
                                    <input type="checkbox" disabled> Ongoing
                                </td>
                                <td>
                                    <input type="checkbox" checked disabled> Completed
                                </td>
                            </tr>
                        }
                    </table>
                </div>

                <div class="container-fluid" id="9th_Point">
                    <p class="fontQMPDF">IX. Present status of work: (check✓  boxes as applicable)</p>
                    <!-- Inner Table in General Details -->
                    <table class="table table-hover table-bordered fontQMPDF" align="center" style="width: 80%;">
                        <thead>
                            <tr>
                                <th> Check ✓ </th>
                                <th> Item </th>
                                <th> RD From (km) </th>
                                <th> RD To (km) </th>
                            </tr>
                        </thead>
                        @if (Model.PresentWorkDetailsList != null)
                        {
                            foreach (var item in @Model.PresentWorkDetailsList)
                            {
                                foreach (var thd in @Model.WorkItemlistViewModel)
                                {
                                    if (thd.ITEM_ID == item.ITEM_ID)
                                    {
                                        if (item.ROAD_FROM >= 0 && item.ROAD_TO >= 0)
                                        {
                                            <tr>
                                                <td> <input type="checkbox" checked disabled> </td>
                                                <td> @thd.ITEM_DESC </td>
                                                <td> @item.ROAD_FROM </td>
                                                <td> @item.ROAD_TO </td>
                                            </tr>
                                        }
                                        else
                                        {
                                            <tr>
                                                <td> <input type="checkbox" disabled> </td>
                                                <td> @thd.ITEM_DESC </td>
                                                <td> </td>
                                                <td> </td>
                                            </tr>
                                        }
                                    }
                                }
                            }
                        }
                        else
                        {
                            <tr>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                            </tr>
                        }
                    </table>
                </div>
            </div> <!-- close div General Section -->

            

            <!-- 2. QUALITY ARRANGEMENTS-OBSERVATIONS -->
        @if (Model.ApplicableCheck.isApplicable_2.Equals("Y"))
        {
            <hr class="PageBreakLine">
            <div id="QualityArrangementsObservations">
                <div class="container-fluid">
                    <h3><b>2. QUALITY ARRANGEMENTS-OBSERVATIONS (in case of ongoing works only) </b></h3>
                    <h4>(Geo-tagged photograph of the laboratory showing the available equipment’s to be uploaded)</h4>

                        <span class="Applicablechecks"> <input type="checkbox" checked disabled /> If Applicable (Check the box)</span>
                    
                    <hr class="SectionUnderline">
                    @if (Model.ArrangementObsDetailsViewModel != null)
                    {
                        <table class="table table-hover table-borderless fontQMPDF tab1_2Pt">
                            @if (Model.ArrangementObsDetailsViewModel.IS_FIELD_LAB_ESTD.Equals("Y"))
                            {
                                <tr>
                                    <td>I. Whether field laboratory established:(check✓any one box)</td>
                                    <td> <input type="checkbox" checked disabled> Yes  &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_FIELD_LAB_ESTD.Equals("P"))
                            {
                                <tr>
                                    <td>I. Whether field laboratory established:(check✓any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes  &emsp13; <input type="checkbox" checked disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_FIELD_LAB_ESTD.Equals("N"))
                            {
                                <tr>
                                    <td>I. Whether field laboratory established:(check✓any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes  &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" checked disabled> No </td>
                                </tr>
                            }
                            else
                            {
                                <tr>
                                    <td>I. Whether field laboratory established:(check✓any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes  &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }

                            @if (Model.ArrangementObsDetailsViewModel.IS_LAB_LOC_SAME.Equals("Y"))
                            {
                                <tr>
                                    <td>II. Whether location of field laboratory is same as indicated by PIU in format part-I: (check✓ any one box) </td>
                                    <td> <input type="checkbox" checked disabled> Yes &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_LAB_LOC_SAME.Equals("N"))
                            {
                                <tr>
                                    <td>II. Whether location of field laboratory is same as indicated by PIU in format part-I: (check✓ any one box) </td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" checked disabled> No </td>
                                </tr>
                            }
                            else
                            {
                                <tr>
                                    <td>II. Whether location of field laboratory is same as indicated by PIU in format part-I: (check✓ any one box) </td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }

                            @if (Model.ArrangementObsDetailsViewModel.IS_EQUIP_AVAILABLE.Equals("Y"))
                            {
                                <tr>
                                    <td>III. Whether necessary equipments as indicated in part-1 are actually available: (check ✓any one box)</td>
                                    <td> <input type="checkbox" checked disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_EQUIP_AVAILABLE.Equals("P"))
                            {
                                <tr>
                                    <td>III. Whether necessary equipments as indicated in part-1 are actually available: (check ✓any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" checked disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>

                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_EQUIP_AVAILABLE.Equals("N"))
                            {
                                <tr>
                                    <td>III. Whether necessary equipments as indicated in part-1 are actually available: (check ✓any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" checked disabled> No </td>
                                </tr>
                            }
                            else
                            {
                                <tr>
                                    <td>III. Whether necessary equipments as indicated in part-1 are actually available: (check ✓any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }

                            @if (Model.ArrangementObsDetailsViewModel.IS_EQUIP_USED.Equals("Y"))
                            {
                                <tr>
                                    <td>IV. Whether equipment’s have been used: (check✓ any one box) </td>
                                    <td> <input type="checkbox" checked disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_EQUIP_USED.Equals("P"))
                            {
                                <tr>
                                    <td>IV. Whether equipment’s have been used: (check✓ any one box) </td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" checked disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_EQUIP_USED.Equals("N"))
                            {
                                <tr>
                                    <td>IV. Whether equipment’s have been used: (check✓ any one box) </td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" checked disabled> No </td>
                                </tr>
                            }
                            else
                            {
                                <tr>
                                    <td>IV. Whether equipment’s have been used: (check✓ any one box) </td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }

                            @if (Model.ArrangementObsDetailsViewModel.IS_EQUIP_AVAIL_VERIFY.Equals("Y"))
                            {
                                <tr>
                                    <td>V. If all necessary equipments are not available, whether you have verified them with the list of deficient equipment’s provided by PIU in format Part-I:  (check✓ any one box)</td>
                                    <td> <input type="checkbox" checked disabled> Yes &emsp13; <input type="checkbox" disabled> No &emsp13; <input type="checkbox" disabled> NA </td>
                                </tr>
                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_EQUIP_AVAIL_VERIFY.Equals("N"))
                            {
                                <tr>
                                    <td>V. If all necessary equipments are not available, whether you have verified them with the list of deficient equipment’s provided by PIU in format Part-I:  (check✓ any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" checked disabled> No &emsp13; <input type="checkbox" disabled> NA </td>
                                </tr>
                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_EQUIP_AVAIL_VERIFY.Equals("A"))
                            {
                                <tr>
                                    <td>V. If all necessary equipments are not available, whether you have verified them with the list of deficient equipment’s provided by PIU in format Part-I:  (check✓ any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> No &emsp13; <input type="checkbox" checked disabled> NA </td>
                                </tr>
                            }
                            else
                            {
                                <tr>
                                    <td>V. If all necessary equipments are not available, whether you have verified them with the list of deficient equipment’s provided by PIU in format Part-I:  (check✓ any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> No &emsp13; <input type="checkbox" checked disabled> NA </td>
                                </tr>
                            }

                            @if (Model.ArrangementObsDetailsViewModel.IS_ENGG_AVAILABLE.Equals("Y"))
                            {
                                <tr>
                                    <td>VI. Whether contractor’s engineer as per Part-I of this format, is available at site: (check✓ any one box)</td>
                                    <td> <input type="checkbox" checked disabled> Yes &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_ENGG_AVAILABLE.Equals("N"))
                            {
                                <tr>
                                    <td>VI. Whether contractor’s engineer as per Part-I of this format, is available at site: (check✓ any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" checked disabled> No </td>
                                </tr>
                            }
                            else
                            {
                                <tr>
                                    <td>VI. Whether contractor’s engineer as per Part-I of this format, is available at site: (check✓ any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }

                            @if (Model.ArrangementObsDetailsViewModel.IS_ALT_ENGG_ARR_SATISFIED.Equals("Y"))
                            {
                                <tr>
                                    <td>VII. If contractor’s engineer as per Part-I of this report is not available, whether you are satisfied with alternative arrangement made to maintain QCR-I: (check✓ any one box)</td>
                                    <td> <input type="checkbox" checked disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No <br> <input type="checkbox" disabled> NA </td>
                                </tr>
                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_ALT_ENGG_ARR_SATISFIED.Equals("P"))
                            {
                                <tr>
                                    <td>VII. If contractor’s engineer as per Part-I of this report is not available, whether you are satisfied with alternative arrangement made to maintain QCR-I: (check✓ any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" checked disabled> Partly &emsp13; <input type="checkbox" disabled> No <br> <input type="checkbox" disabled> NA </td>
                                </tr>
                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_ALT_ENGG_ARR_SATISFIED.Equals("N"))
                            {
                                <tr>
                                    <td>VII. If contractor’s engineer as per Part-I of this report is not available, whether you are satisfied with alternative arrangement made to maintain QCR-I: (check✓ any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" checked disabled> No <br> <input type="checkbox" disabled> NA </td>
                                </tr>
                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_ALT_ENGG_ARR_SATISFIED.Equals("A"))
                            {
                                <tr>
                                    <td>VII. If contractor’s engineer as per Part-I of this report is not available, whether you are satisfied with alternative arrangement made to maintain QCR-I: (check✓ any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No <br> <input type="checkbox" checked disabled> NA </td>
                                </tr>
                            }
                            else
                            {
                                <tr>
                                    <td>VII. If contractor’s engineer as per Part-I of this report is not available, whether you are satisfied with alternative arrangement made to maintain QCR-I: (check✓ any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No <br> <input type="checkbox" disabled> NA </td>
                                </tr>
                            }

                            @if (Model.ArrangementObsDetailsViewModel.IS_LAB_TECH_AVAILABLE.Equals("Y"))
                            {
                                <tr>
                                    <td>VIII. Whether lab technician and other staff is available for testing in the laboratory (check✓ any one box)</td>
                                    <td> <input type="checkbox" checked disabled> Yes &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }
                            else if (Model.ArrangementObsDetailsViewModel.IS_LAB_TECH_AVAILABLE.Equals("N"))
                            {
                                <tr>
                                    <td>VIII. Whether lab technician and other staff is available for testing in the laboratory (check✓ any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" checked disabled> No </td>
                                </tr>
                            }
                            else
                            {
                                <tr>
                                    <td>VIII. Whether lab technician and other staff is available for testing in the laboratory (check✓ any one box)</td>
                                    <td> <input type="checkbox" disabled> Yes &emsp13; <input type="checkbox" disabled> No </td>
                                </tr>
                            }

                        </table>
                    }
                    else
                    {
                        <table class="table table-hover table-borderless fontQMPDF tab1_2Pt">

                            <tr>
                                <td>I. Whether field laboratory established:(check✓any one box)</td>
                                <td> <input type="checkbox" disabled> Yes  &emsp13; <input type="checkbox" disabled> Partly &emsp13; <input type="checkbox" disabled> No </td>
