@model PMGSY.Models.Ticket.TicketReportViewModel

<link href="~/Content/dashboard.css" rel="stylesheet" />
<script src="~/Scripts/custom/Generic.js"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery.ui.accordion.js")"> </script>
<script src="~/Scripts/Ticket/TicketingReportLayout.js"></script>
<style>
    body {
        text-align: center;
    }

    .ui-widget-dash {
        position: relative;
        display: inline-block;
        width: 140px;
        height: 150px;
        margin: 30px;
        border: 15px solid #FCD27C /*#00aeef*/ ;
        border-radius: 45px;
        text-align: center;
        font-family: Arial, sans-serif;
        box-shadow: inset 0 1px 0 rgba(255,255,255,.8), inset 0 3px 5px rgba(0,0,0,.25);
        box-sizing: border-box;
        border-bottom-color: #fff;
    }

     .ui-widget-dash .ui-value-dash {
            position: absolute;
            left: 0;
            right: 0;
            top: 15px;
            font-size: 27px;
        }

        .ui-widget-dash .ui-label-dash {
            position: absolute;
            left: 0;
            bottom: -10px;
            width: 100%;
            font-size: 14px;
            text-transform: uppercase;
            color: #888;
            font-weight: 700;
        }


     .flex-container {
        width: 90%;
        min-height: 220px;
        margin: 0 auto;
        display: -webkit-flex; /* Safari */
        display: flex; /* Standard syntax */
        box-shadow:4px 4px 4px rgb(204, 204, 204);
      }

   .flex-container .column {
            padding: 10px;
            /*background: #dbdfe5;*/
            -webkit-flex: 1; /* Safari */
            -ms-flex: 1; /* IE 10 */
            flex: 1; /* Standard syntax */
        }

      .flex-container .column.bg-alt {
                /*background: #b4bac0;*/
        }

    small {
        font-size: 15px;
     }
</style>




<div id="divticketReportLayout" style="margin-top: 20px">
    <form id="frmticketreport">
        <center>

    <div>
        @Html.AntiForgeryToken() 
    </div>       
        <div id="dvhdtktreport" class="ui-corner-top ui-widget-header panelheader" style="text-align: center; height: 20px;">
        <span class="ui-jqgrid-title" style="float: left">&nbsp;Ticket Reporting Details</span>
        <span id="spCollapseIconTktRpt" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
      </div>
 <div id="dvTktReportingMain" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">         
        <div id="divErrortkt" class="ui-state-error ui-corner-all" style="text-align: left; display: none; width: 99.9%;">
            <span class="ui-icon ui-icon-alert" style="float: left;"></span>                   
            <span></span>
    </div>

        <div id="dvTktRptNote" style="height:20px; width: 99.9%; text-align:left; padding-top: 0.4em;padding-bottom:18px;" class="ui-state-default ui-corner-all" > 
                &nbsp;<strong> Note :</strong>  Fields marked with * are mandatory.
        <a href="#"  style="float: right;">
        <img style="margin-right:5px" alt="Close" src="" class="ui-icon ui-icon-closethick" id="imgCloseRptDetails" /></a>
      </div>

       <table id="tbTKTRPTDetails" cellpadding="3px" class="rowstyle" style="margin-top:0px; text-align:left; width:99.9%; padding-top:5px 5px 5px 5px;">
            <tr>
                 <td style="width:5%">
                </td>
                 <td style="width:4%">
                    @Html.LabelFor(model => model.Level)<label style="color:red">*</label>
                </td>
                <td style="width:15%; text-align:left;">
                        @Html.RadioButtonFor(m => m.Level, "0", new { @id = "rdbSLSC", @tabindex = 1, })
                        @Html.Label("All")
                        @Html.RadioButtonFor(m => m.Level, "1", new { @id = "rdbGB", @tabindex = 2 })
                        @Html.Label("Statewise")
                        @Html.RadioButtonFor(m => m.Level, "2", new { @id = "rdbGB", @tabindex = 3 })
                        @Html.Label("National")
                        <br />
                        @Html.ValidationMessageFor(x => x.Level)
                </td>
                 <td class="state" style="width:4%">
                       @Html.LabelFor(model => model.state)<label style="color:red">*</label>
                </td>
                <td class="state" style="width:15%; text-align:left;">
                        @Html.DropDownListFor(m => m.state, new SelectList(Model.stateList, "Value", "Text"), new { @id = "ddlState", @tabindex = 5, })
                        <br />
                        @Html.ValidationMessageFor(model => model.state)
                </td>
                 <td style="width:4%">
                    
                </td>
                <td style="width:15%; text-align:left;">
                       
                </td>
                <td  style="text-align:left;">
               
              </td>
            </tr>
           
           <tr>
                <td style="width:5%">
                </td>
                <td style="width:4%">
                      @Html.LabelFor(model => model.Designation)<label style="color:red">*</label>
                </td>
                <td style="width:15%">
                        @Html.DropDownListFor(m => m.Designation, new SelectList(Model.DesignationList, "Value", "Text"), new { @id = "ddlDesignation", @tabindex = 4, })
                        <br />
                        @Html.ValidationMessageFor(model => model.Designation)
                </td>
               
                 <td style="width:4%">
                      @Html.LabelFor(model => model.Category)<label style="color:red">*</label>
                </td>
                <td style="width:15%">
                        @Html.DropDownListFor(m => m.Category, new SelectList(Model.lstCategory, "Value", "Text"), new { @id = "ddlCategory", @tabindex = 6, })
                        <br />
                        @Html.ValidationMessageFor(model => model.Category)
                </td>
         @* </tr>
        <tr>
                <td style="width:5%">
                </td>*@
               
                 <td style="width:4%">
                       @Html.LabelFor(model => model.ModuleID)<label style="color:red">*</label>
                </td>
                <td style="width:15%; text-align:left;">
                        @Html.DropDownListFor(m => m.ModuleID, new SelectList(Model.LstModule, "Value", "Text"), new { @id = "ddlModule", @tabindex = 7, })
                        <br />
                        @Html.ValidationMessageFor(model => model.ModuleID)
                </td>
               <td  style="text-align:left;">
                <input type="button" value="View" name="ViewReport" id="btnViewTktRpt" class="jqueryButton" tabindex="8" />
              </td>
          </tr>
       
      @*  <tr>
            <td colspan="6" style="text-align:center;">
                <input type="button" value="View" name="ViewReport" id="btnViewTktRpt" class="jqueryButton" tabindex="8" />
            </td>
        </tr>*@
        </table>
      </div>
    </center>
    </form>
</div>
<br />


<div class="flex-container ui-corner-all" style="border: 1px solid #B5B8C8; ">
    <div class="column" style="border-right: 0px solid #CCC">
        <div class="ui-widget-dash" ">
            <h1 class="ui-value-dash" id="totalticket">@Html.Raw(Model.TotalTicket)</h1>
            <span class="ui-label-dash">&nbsp;&nbsp;&nbsp;Total&nbsp;&nbsp;&nbsp;&nbsp; tickets</span>
        </div>

        <div class="ui-widget-dash">
            <h1 class="ui-value-dash" id="totalapprovedtkt">@Html.Raw(Model.TotalApprovedTicket)</h1>
            <span class="ui-label-dash">Approved Tickets</span>
        </div>

        <div class="ui-widget-dash" >
            <h1 class="ui-value-dash" id="totalclosedtkt">@Html.Raw(Model.TotalClosedTicket)</h1>
            <span class="ui-label-dash">&nbsp;&nbsp;Closed &nbsp;&nbsp; tickets</span>
        </div>
     </div>

    <!--<div class="arrow-class">&#x2192;</div>-->
    <div class="column bg-alt">
        <table style="border: 0px solid #B5B8C8; margin-left: 75px">
            <tr>
                <td style="width: 100%">
                    <div class="ui-corner-all" style="border: 1px solid rgb(204, 204, 204); background: rgb(243, 194, 90) none repeat scroll 0 0;/*#FFC300;*//*#80dfff*/">
                        <span class=""></span>
                        <h1 style="color: #fff" >Tickets Pending : <span id="totalnotclosedtkt">@Html.Raw(Model.TotalNotClosedTicket)</span></h1>
                    </div>
                </td>
            </tr>
            <tr>
                <td>
                    <div class="weather-wrapper">
                       <div class="weather-card" style="background:#fef1d5">
                            <p class="title">Not Opened</p>
                            <h1 id="notopened">@Html.Raw(Model.NotOpenedTicket)</h1>
                        </div>
                         <div class="weather-card " style="background:#fdeac1">
                            <p class="title">Opened</p>
                            <h1 id="openedtkt">@Html.Raw(Model.OpenedTicket)</h1>
                        </div>
                        <div class="weather-card " style="background:#fde3ae">
                            <p class="title">In Progress</p>
                            <h1 id="inprogress">@Html.Raw(Model.InProgressTicket)</h1>
                        </div>
                         <div class="weather-card" style="background:#fddb95">
                            <p class="title">Partialy closed</p>
                            <h1 id="partialclosed">@Html.Raw(Model.PartialClosedTicket)</h1>
                        </div>
                    </div>
                </td>

            </tr>
        </table>



    </div>
</div>
      
   
<center>
    <div id="divReportGrid" style="display: block;margin-top:5px;">
        <div id="dvAllTicketList" style="width: 91%; border: hidden grey 1px; margin-top: 25px; margin-bottom: 25px" class="side">
            <table id="tbTktReportList"></table>
            <div id="pgTktReportList"></div>
        </div>
   </div>

     <div id="accordion" style="display:none;width:90%;margin-top:10px;margin-bottom:10px;">            
            <h3 style="text-align:left"><a id="" href="#" style=" font-size:.9em;" >Add Ticket Details</a>
                <a href="#"  style="float: right;">
                    <img style="border:none" src="" class="ui-icon ui-icon-closethick" onclick="CloseTicketDetails();" /></a>
            </h3>
            <div id="dvticketlayout" style="display:none;">
            </div>                     
        </div>

</center>
