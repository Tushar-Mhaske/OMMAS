@model PMGSY.Areas.OtherReports.Models.WorksWithoutAgrModel

@if (Layout == null)
{
    //Layout = "~/Views/Shared/_ExternalLinkLayout.cshtml";
    Layout = Request.IsAjaxRequest() ? null : "~/Areas/AccountReports/Views/Account/_Layout.cshtml";
}

<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script src="~/Scripts/jquery.ui.datepicker.js" type="text/javascript"></script>
<script src="~/Areas/OtherReports/Scripts/WorksWithoutMainAgr.js"></script>
<style>
    .underline {
        border-bottom: 1px solid #000000;
        width: 500px;
        padding-bottom: 5px;
    }
</style>
<div id="dvhdCreateNewReportDetails" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">@*margin-left:0px;margin-right:0px;margin-top:0px; *@
    <span class="ui-jqgrid-title" style="float: left">&nbsp;Works Without Maintenance Agreement Report</span>

    <span id="spCollapseIconCN" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>
<div id="dvECBriefParameter" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmECBriefLayout">
        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.StateName)
        @Html.HiddenFor(model => model.DistName)
        @Html.AntiForgeryToken()

        @*<div align="centre" style="text-align: center;">
            <div style="width: 50%; background-color: floralwhite">
            </div>
            <div style="width: 135%; background-color: #fbf7aa; border-top: 1px solid black; border-left: 1px solid black; border-bottom: 1px solid black;">
                <span style="text-align: center; font: bold; width: 200px">@Html.Label("Enter Completion Details")@Html.Label("")</span>
            </div>
        </div>*@

        <table class="rowstyle">
            <tr class="ui-state-hover">
                <td colspan="5"></td>
                <td colspan="4" style="border-top: 1px solid gray; border-left: 1px solid gray; border-right: 1px solid gray; border-bottom: 1px solid gray; text-align: center;">
                    <label>Enter Completion Details</label>
                </td>
                <td></td>
            </tr>

            <tr class="ui-state-hover">
                <td style="width: 10%;">
                    @Html.LabelFor(model => model.StateCode)
                </td>
                <td style="width: 10%;">
                    @Html.DropDownListFor(model => model.StateCode, new SelectList(Model.StateList, "Value", "Text"), new { @id = "ddlECState", style = "width:170px;", title = "State" })
                    <br />
                    @Html.ValidationMessageFor(m => m.StateCode)
                </td>
                <td style="width: 5%;"></td>
                <td style="width: 10%;">
                    @Html.LabelFor(model => model.DistrictCode)
                </td>
                <td style="width: 15%;">
                    @Html.DropDownListFor(model => model.DistrictCode, new SelectList(Model.DistrictList, "Value", "Text"), new { @id = "ddlECDistrict", style = "width:170px;" })
                    <br />
                    @Html.ValidationMessageFor(m => m.DistrictCode)
                </td>

                <td id="tdlblStDate" style="width: 5%; border-top: 1px solid gray; border-left: 1px solid gray; border-bottom: 1px solid gray;">&nbsp;&nbsp;
                    @Html.Label("Start Date")
                    <label style="color: red">*</label>
                </td>
                <td id="tdtxtStDate" style="width: 20%; border-top: 1px solid gray; border-bottom: 1px solid gray;">
                    @Html.TextBoxFor(m => m.StartDate, new { maxlength = 10, @title = "Start Date", @placeholder = "dd/mm/yyyy...", @tabindex = "9", @readonly = "readonly" })
                    <br />
                    @Html.ValidationMessageFor(m => m.StartDate)
                </td>
                <td id="tdlblEndDate" style="width: 5%; border-top: 1px solid gray; border-bottom: 1px solid gray;">
                    @Html.Label("End Date")
                    <label style="color: red">*</label>
                </td>
                <td id="tdtxtEndDate" style="width: 11%; border-top: 1px solid gray; border-right: 1px solid gray; border-bottom: 1px solid gray;">
                    @Html.TextBoxFor(m => m.EndDate, new { maxlength = 10, @title = "End Date", @placeholder = "dd/mm/yyyy...", @tabindex = "10", @readonly = "readonly" })
                    <br />
                    @Html.ValidationMessageFor(m => m.EndDate)
                </td>


                <td style="width: 15%; text-align: center">
                    <input type="button" id="btnViewEC" value="View" class="button" />
                </td>

                <td style="width: 12%;"></td>
                <td style="width: 12%;"></td>
                <td style="width: 12%;"></td>


            </tr>
            <tr class="ui-state-hover">
                <td style="width: 10%;"></td>

            </tr>
        </table>
    </form>
</div>

<div id="divLoadECReport"></div>


