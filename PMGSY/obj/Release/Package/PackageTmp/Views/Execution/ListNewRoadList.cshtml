@model PMGSY.Models.Execution.NewRoadList
@{
    ViewBag.Title = "ListNewRoadList";
}


<script src="~/Scripts/custom/Generic.js"></script>
<script src="~/Scripts/jquery.ui.accordion.js"></script>
<script src="~/Scripts/Execution/ListNewRoadList.js"></script>
<script src="~/Scripts/Execution/ExecutionAdditionalRoadDetails.js"></script>
@{
    if (PMGSY.Extensions.PMGSYSession.Current.UserId == 5735 || PMGSY.Extensions.PMGSYSession.Current.RoleCode == 65 || PMGSY.Extensions.PMGSYSession.Current.UserId == 24)
    {
    <center>
<div style="width:98%; height:20px;border:none; margin-top:5px; margin-bottom:-15px; " class="ui-state-default">
            <span style="float:right;margin-right:2Px; margin-top:2px" id="idFilterDiv" class="ui-icon ui-icon-circle-triangle-n"></span>     
</div>
    
<div id="divSearchExecution">
<div style="width: 98%; border:none; margin-top:-15px; margin-bottom:10px;" class="ui-state-default">
<center>
<form id="searchExecution">
                    <div style="width: 98%; border:none; margin-top:0px; margin-bottom:10px;" class="ui-state-default">
                        @Html.AntiForgeryToken()
                        @Html.Hidden("hdnRoleCode",PMGSY.Extensions.PMGSYSession.Current.RoleCode)
                        @Html.HiddenFor(model => model.Urlparameter)
                        <table style="margin-top:10px; margin-bottom:5px;">
                            <tr>
                                <td>
                                    @Html.LabelFor(model => model.MAST_STATE_CODE)
                                </td>
                                <td>
                                    @Html.DropDownListFor(model => model.MAST_STATE_CODE, new SelectList(Model.STATES, "Value", "Text"), new { @id = "ddlMastStateCode" })
                                    <br />
                                    @Html.ValidationMessageFor(x => x.MAST_STATE_CODE)
                                </td>
                                <td>
                                    @Html.LabelFor(model => model.MAST_DISTRICT_CODE)
                                </td>
                                <td>
                                    @Html.DropDownListFor(model => model.MAST_DISTRICT_CODE, new SelectList(Model.DISTRICTS, "Value", "Text"), new { @id = "ddlMastDistrictCode" })
                                    <br />
                                    @Html.ValidationMessageFor(x => x.MAST_DISTRICT_CODE)
                                </td>

                                <td>
                                    @Html.LabelFor(model => model.MAST_BLOCK_CODE)
                                </td>
                                <td>
                                    @Html.DropDownListFor(model => model.MAST_BLOCK_CODE, new SelectList(Model.BLOCKS, "Value", "Text"), new { @id = "ddlMastBlockCode" })
                                </td>
                                <td>
                                    @Html.LabelFor(model => model.IMS_YEAR)
                                </td>
                                <td>                            
                                    @Html.DropDownListFor(model => model.IMS_YEAR, new SelectList(Model.Years, "Value", "Text"), new { @id = "ddlImsYear" })
                                </td>
                                <td>
                                    @Html.LabelFor(model => model.Batch)
                                </td>
                                <td>                            
                                    @Html.DropDownListFor(model => model.Batch, new SelectList(Model.lstBatchs, "Value", "Text"), new { @id = "ddlBatchs" })
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    @Html.LabelFor(model => model.IMS_PACKAGE)
                                </td>
                                <td>
                                    @Html.DropDownListFor(model => model.PACKAGES, new SelectList(Model.PACKAGES, "Value", "Text"), new { @id = "ddlImsPackages" })
                                </td>
                                <td>
                                    @Html.LabelFor(model => model.Collaboration)
                                </td>
                                <td>
                                    @Html.DropDownListFor(model => model.Collaboration, new SelectList(Model.lstCollaborations, "Value", "Text"), new { @id = "ddlCollaborations" })
                                </td>
                                <td>
                                    @Html.LabelFor(model => model.IMS_PROPOSAL_TYPE)
                                </td>
                                <td>
                                    @Html.DropDownListFor(model => model.IMS_PROPOSAL_TYPE, new SelectList(Model.PROPOSAL_TYPES, "Value", "Text"), new { @id = "ddlImsProposalTypes" })
                                </td>
                                <td>
                                    @Html.LabelFor(model => model.UpgradeConnect)
                                </td>
                                <td>
                                    @Html.DropDownListFor(model => model.UpgradeConnect, new SelectList(Model.lstUpgradations, "Value", "Text"), new { @id = "ddlUpgradations" })
                                </td>
                                <td></td>
                                 <td>
                                    <center>
                                    <table>
                                        <tr>
                                            <input type="button" style="margin-top:5px" class="jqueryButton" value="Show Road List" id="btnNewRoadList" name ="btnNewRoadList" title="Click here to list Road Details"/>
                                        </tr>
                                    </table>
                                    </center>
                                </td>
                            </tr>
                        </table>
                        
                    </div>
</form>
</center>
</div>
</div>
    <div id="divListExecution" style="margin-top:20px; width:96%">
    <table id="tbExecutionList">

    </table>
    <div id="pagerExecution">

    </div>
    </div>

<div id="accordion" style="display:none;width:96%;margin-top:20px;margin-bottom:20px" >
    <h3 style="text-align:left"><a id="" href="#" style=" font-size:.9em;" >Execution Details</a>
                <a href="#"  style="float: right;">
                    <img src="" class="ui-icon ui-icon-closethick" onclick="CloseExecutionDetails();" /></a>
     </h3>
    <div id="divAddExecution" style="display:none">

    </div>
</div>
        
        
</center>


    }
    else
    {
    <table class="rowstyle">
        <tr>
            <td style="width: 10%;"></td>
            <td style="width: 5%;"></td>
            <td style="width: 25%;">
                <strong>
                    <label>Execution Progress Details not available for the User : "@PMGSY.Extensions.PMGSYSession.Current.UserName"</label>
                </strong>
            </td>
            <td style="width: 5%;"></td>
            <td style="width: 25%;"></td>
            <td style="width: 10%;"></td>
        </tr>
    </table>
    }
}