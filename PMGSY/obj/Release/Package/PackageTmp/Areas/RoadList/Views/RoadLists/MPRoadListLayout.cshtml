@model PMGSY.Areas.RoadList.Models.RoadListsModel

<script src="~/Areas/RoadList/Scripts/RoadListsScript.js"></script>

<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<div id="dvSearchParameter" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmQMPahse">
        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.StateName)
        @Html.HiddenFor(model => model.MPConstName)

        @Html.Hidden("hdnRoleCode",PMGSY.Extensions.PMGSYSession.Current.RoleCode.ToString())
       <table style="width: 100%;">
            <tr>
                <td style="width: 2%;"></td>
                
                <td style="text-align: left; width: 5%;">
                 @Html.LabelFor(model => model.StateCode)
                </td>
                <td style="width: 15%;">
                    
                        @Html.DropDownListFor(model => model.StateCode, new SelectList(Model.lstStates, "Value", "Text"), new { @title ="State", @tabindex = 1, @id = "ddlQMPahse", @style = "width:150px" })
                        @Html.ValidationMessageFor(m => m.StateCode)                    
                </td>


                <td style="text-align: left; width: 10%;">
                 @Html.LabelFor(model => model.MPConstCode)
                </td>
                 <td style="width: 15%;">
                    
                        @Html.DropDownListFor(model => model.MPConstCode, new SelectList(Model.lstMPConst, "Value", "Text"), new { @title ="MP Constituency", @tabindex = 1, @id = "ddlMPConst", @style = "width:150px" })
                        @Html.ValidationMessageFor(m => m.MPConstCode)                    
                </td>
                 @if (PMGSY.Extensions.PMGSYSession.Current.RoleCode != 8)
                 { 
                    <td>
                        <input type="button" id="btnQMPahse" value="View"  Class="jqueryButton"  Title="View Details"tabindex="4" />
                    </td>
                 }
                 else
                 {
                     <td></td>
                 }
            </tr>
        </table>
    </form>
</div>

<div>
<div id="loadReport3" class="divLoadReport" style="height:auto"></div>


<div id="loadChart"></div>
</div>

