@model PMGSY.Areas.Feedback.Models.ClusterViewModel

<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
<script src="~/Areas/Feedback/Scripts/DistrictBrief.js"></script>

<div id="dvhdClusterReport" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">
    <span class="ui-jqgrid-title" style="float: left">&nbsp;District Brief Details</span>
    <span id="spCollapseIconClusterReport" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>
</div>
<div id="loadFiltersClusterReportLayout" class="ui-corner-bottom" style="display: block; border: 1px solid #B5B8C8;">
    <form id="frmClusterReportLayout">
        @Html.ValidationSummary(true)

        @*        @Html.HiddenFor(model => model.Mast_State_Code)*@
        @*@Html.HiddenFor(model => model.stateName)
        @Html.HiddenFor(model => model.districtName)*@

        <table style="width: 100%;" class="rowstyle">
            <tr>
                <td style="width: 5%; vertical-align: top"></td>
                <td style="text-align: left; width: 5%; vertical-align: top">
                    @Html.LabelFor(model => model.stateCode)
                </td>
                <td style="width: 15%; vertical-align: top">
                    @Html.DropDownListFor(model => model.stateCode, new SelectList(Model.stateList, "Value", "Text"), new { @title = "State", @tabindex = 1, @id = "ddlStateCluster", @style = "width:150px" })
                    <br />
                    @Html.ValidationMessageFor(m => m.stateCode)
                </td>
                 <td style="width: 5%; vertical-align: top"></td>
                <td style="text-align: left; width: 5%; vertical-align: top">
                    @Html.LabelFor(model => model.districtCode)
                </td>
                <td style="width: 15%; vertical-align: top">
                    @Html.DropDownListFor(model => model.districtCode, new SelectList(Model.districtList, "Value", "Text"), new { @title = "District", @tabindex = 2, @id = "ddlDistrictCluster", @style = "width:150px" })
                    <br />
                    @Html.ValidationMessageFor(m => m.districtCode)
                </td>
                <td style="width: 5%; vertical-align: top"></td>
                <td style="text-align: left; width: 5%; vertical-align: top">
                    <input type="button" class="button" value="View" id="btnViewCluster" title="View" />
                </td>
                <td style="width: 15%; vertical-align: top">
                    
                </td>
            </tr>
        </table>
    </form>
</div>
<div id="loadClusterReport" class="divLoadReport"></div>
