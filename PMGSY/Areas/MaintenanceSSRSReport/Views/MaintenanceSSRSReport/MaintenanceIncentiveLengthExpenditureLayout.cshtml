@model PMGSY.Areas.MaintenanceSSRSReport.Models.MaintenanceIncentiveLenExpViewModel

<script src="@Url.Content("~/Scripts/custom/Generic.js")" type="text/javascript" ></script>
@*<script src="~/Areas/AnalysisSSRSReport/Scripts/AnalysisAverageLengthReport.js"></script>*@
<script src="~/Areas/MaintenanceSSRSReport/Scripts/MaintenanceIncentiveLengthExpenditureLayout.js"></script>
<div id="dvhdCreateNewReportDetails" class="ui-corner-top ui-widget-header panelheader" style="text-align: left; height: 20px;">@*margin-left:0px;margin-right:0px;margin-top:0px; *@
    <span class="ui-jqgrid-title" style="float: left">&nbsp;Maintenance Incentive Length Expenditure Report</span>

    @*<span id="spCollapseIconCN" class="ui-icon ui-icon-circle-triangle-n" style="float: right"></span>*@
</div>

<form id="frmMaintenanceIncentiveLenExp">
    @Html.ValidationSummary(true)
    @Html.HiddenFor(m => m.LocationName)
    @Html.HiddenFor(m => m.Level)

    <table style="width: 100%;" class="rowstyle">
        <tr>
            <td style="width: 2%;"></td>
            <td style="text-align: left; width: 7%;">
                @Html.LabelFor(model => model.StateCode)
            </td>

            <td style="width: 20%;">
                @Html.DropDownListFor(model => model.StateCode, new SelectList(Model.StateList, "Value", "Text"), new { @title = "State", @tabindex = 1, @id = "ddlState", @style = "width:150px" })
                <br />
                @Html.ValidationMessageFor(m => m.StateCode)
            </td>
            <td style="width: 4%;">
                <input type="button" id="btnView" value="View" class="jqueryButton" title="View" tabindex="2" />
            </td>
        </tr>
    </table>

    <div id="loadReport" class="divLoadReport"></div>
</form>
